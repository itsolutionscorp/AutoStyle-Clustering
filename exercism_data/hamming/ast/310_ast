s(:defn, :compute, s(:args, :a, :b), s(:lasgn, :a_chars, s(:call, s(:lvar, :a), :chars)), s(:lasgn, :b_chars, s(:call, s(:lvar, :b), :chars)), s(:iter, s(:call, s(:call, s(:lvar, :a_chars), :each_with_index), :inject, s(:lit, 0)), s(:args, :distance, s(:masgn, :char, :index)), s(:block, s(:if, s(:call, s(:lvar, :char), :!=, s(:call, s(:lvar, :b_chars), :[], s(:lvar, :index))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil), s(:lvar, :distance))))