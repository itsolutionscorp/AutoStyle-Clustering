s(:defn, :compute, s(:args, :strand1, :strand2), s(:lasgn, :strand1_array, s(:call, s(:call, s(:lvar, :strand1), :downcase), :split, s(:str, ""))), s(:lasgn, :strand2_array, s(:call, s(:call, s(:lvar, :strand2), :downcase), :split, s(:str, ""))), s(:if, s(:call, s(:call, s(:lvar, :strand1_array), :length), :!=, s(:call, s(:lvar, :strand2_array), :length)), s(:return, s(:lit, 0)), nil), s(:lasgn, :hamming_difference, s(:lit, 0)), s(:iter, s(:call, s(:lvar, :strand1_array), :each_with_index), s(:args, :letter, :index), s(:if, s(:call, s(:lvar, :letter), :!=, s(:call, s(:lvar, :strand2_array), :[], s(:lvar, :index))), s(:lasgn, :hamming_difference, s(:call, s(:lvar, :hamming_difference), :+, s(:lit, 1))), nil)), s(:return, s(:lvar, :hamming_difference)))