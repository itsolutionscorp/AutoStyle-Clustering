s(:defn, :compute, s(:args, :string1, :string2), s(:lasgn, :count, s(:lit, 0)), s(:lasgn, :array1, s(:call, s(:lvar, :string1), :chars)), s(:lasgn, :array2, s(:call, s(:lvar, :string2), :chars)), s(:lasgn, :n, s(:call, s(:lvar, :array1), :length)), s(:iter, s(:call, s(:lvar, :n), :times), s(:args, :t), s(:if, s(:call, s(:call, s(:lvar, :array2), :[], s(:lvar, :t)), :==, s(:call, s(:lvar, :array1), :[], s(:lvar, :t))), nil, s(:lasgn, :count, s(:call, s(:lvar, :count), :+, s(:lit, 1))))), s(:return, s(:lvar, :count)))