s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :strand_1_s, :strand_2_s), s(:op_asgn_or, s(:lvar, :strand_1_s), s(:lasgn, :strand_1_s, s(:str, ""))), s(:op_asgn_or, s(:lvar, :strand_2_s), s(:lasgn, :strand_2_s, s(:str, ""))), s(:masgn, s(:array, s(:lasgn, :short_strand), s(:lasgn, :long_strand)), s(:to_ary, s(:call, s(:array, s(:lvar, :strand_1_s), s(:lvar, :strand_2_s)), :sort_by!, s(:block_pass, s(:lit, :length))))), s(:lasgn, :paired_strands, s(:call, s(:call, s(:lvar, :short_strand), :chars), :zip, s(:call, s(:lvar, :long_strand), :chars))), s(:iter, s(:call, s(:lvar, :paired_strands), :count), s(:args, :a, :b), s(:call, s(:lvar, :a), :!=, s(:lvar, :b)))))