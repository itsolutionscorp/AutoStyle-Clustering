s(:defn, :compute, s(:args, :seq1, :seq2), s(:lasgn, :diff, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :seq1), :length), :times), s(:args, :n), s(:if, s(:call, s(:call, s(:lvar, :seq1), :[], s(:lvar, :n)), :==, s(:call, s(:lvar, :seq2), :[], s(:lvar, :n))), nil, s(:lasgn, :diff, s(:call, s(:lvar, :diff), :+, s(:lit, 1))))), s(:lvar, :diff))