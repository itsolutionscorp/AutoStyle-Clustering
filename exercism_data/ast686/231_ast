s(:defn, :compute, s(:args, :strand_a, :strand_b), s(:if, s(:call, s(:call, s(:lvar, :strand_a), :length), :==, s(:call, s(:lvar, :strand_b), :length)), nil, s(:return)), s(:lasgn, :mutations, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :strand_a), :each_char), :with_index), s(:args, :nucleotide, :index), s(:if, s(:call, s(:lvar, :nucleotide), :!=, s(:call, s(:lvar, :strand_b), :[], s(:lvar, :index))), s(:lasgn, :mutations, s(:call, s(:lvar, :mutations), :+, s(:lit, 1))), nil)), s(:lvar, :mutations))