s(:defn, :compute, s(:args, :strand1, :strand2), s(:lasgn, :array, s(:call, s(:call, s(:lvar, :strand1), :split, s(:str, "")), :zip, s(:call, s(:lvar, :strand2), :split, s(:str, "")))), s(:lasgn, :distance, s(:lit, 0)), s(:iter, s(:call, s(:lvar, :array), :each), s(:args, :compare), s(:if, s(:or, s(:call, s(:call, s(:lvar, :compare), :[], s(:lit, 0)), :==, s(:call, s(:lvar, :compare), :[], s(:lit, 1))), s(:call, s(:call, s(:lvar, :compare), :[], s(:lit, 1)), :==, s(:nil))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 0))), s(:if, s(:call, s(:call, s(:lvar, :compare), :[], s(:lit, 0)), :!=, s(:call, s(:lvar, :compare), :[], s(:lit, 1))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil))), s(:return, s(:lvar, :distance)))