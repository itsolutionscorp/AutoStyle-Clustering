s(:defn, :compute, s(:args, :strand1, :strand2), s(:lasgn, :strand1, s(:call, s(:lvar, :strand1), :chars)), s(:lasgn, :strand2, s(:call, s(:lvar, :strand2), :chars)), s(:lasgn, :count, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :strand1), :take, s(:call, s(:lvar, :strand2), :length)), :each_with_index), s(:args, :base, :i), s(:if, s(:call, s(:lvar, :base), :!=, s(:call, s(:lvar, :strand2), :[], s(:lvar, :i))), s(:lasgn, :count, s(:call, s(:lvar, :count), :+, s(:lit, 1))), nil)), s(:lvar, :count))