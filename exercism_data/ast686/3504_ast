s(:defn, :compute, s(:args, :left, :right), s(:lasgn, :count, s(:lit, 0)), s(:lasgn, :index, s(:lit, 0)), s(:lasgn, :shortest_length, s(:if, s(:call, s(:call, s(:lvar, :left), :length), :<, s(:call, s(:lvar, :right), :length)), s(:call, s(:lvar, :left), :length), s(:call, s(:lvar, :right), :length))), s(:while, s(:call, s(:lvar, :index), :<, s(:lvar, :shortest_length)), s(:block, s(:lasgn, :different_letters, s(:call, s(:call, s(:lvar, :left), :[], s(:lvar, :index)), :!=, s(:call, s(:lvar, :right), :[], s(:lvar, :index)))), s(:lasgn, :index, s(:call, s(:lvar, :index), :+, s(:lit, 1))), s(:if, s(:lvar, :different_letters), s(:lasgn, :count, s(:call, s(:lvar, :count), :+, s(:lit, 1))), nil)), true), s(:return, s(:lvar, :count)))