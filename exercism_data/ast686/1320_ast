s(:defn, :compute, s(:args, :strand_1, :strand_2), s(:if, s(:call, s(:call, s(:lvar, :strand_1), :size), :!=, s(:call, s(:lvar, :strand_2), :size)), s(:call, s(:lvar, :strand_1), :[], s(:lit, 0), s(:call, s(:lvar, :strand_2), :size)), nil), s(:lasgn, :result, s(:array)), s(:iter, s(:call, s(:call, s(:lvar, :strand_1), :chars), :each_with_index), s(:args, :char_1, :index_1), s(:iter, s(:call, s(:call, s(:lvar, :strand_2), :chars), :each_with_index), s(:args, :char_2, :index_2), s(:if, s(:call, s(:lvar, :index_1), :==, s(:lvar, :index_2)), s(:if, s(:call, s(:lvar, :char_1), :==, s(:lvar, :char_2)), s(:call, s(:lvar, :result), :<<, s(:str, " ")), s(:call, s(:lvar, :result), :<<, s(:str, "^"))), nil))), s(:call, s(:call, s(:call, s(:lvar, :result), :join), :scan, s(:lit, /\^/)), :count))