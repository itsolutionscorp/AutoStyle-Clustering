s(:defn, :compute, s(:args, :arg1, :arg2), s(:lasgn, :distance, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:call, s(:lvar, :arg1), :chars), :take, s(:call, s(:lvar, :arg2), :length)), :zip, s(:call, s(:lvar, :arg2), :chars)), s(:args, :arr), s(:if, s(:call, s(:call, s(:lvar, :arr), :[], s(:lit, 0)), :!=, s(:call, s(:lvar, :arr), :[], s(:lit, 1))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil)), s(:lvar, :distance))