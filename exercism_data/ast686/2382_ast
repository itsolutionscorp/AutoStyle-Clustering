s(:defn, :compute, s(:args, :first_strand, :second_strand), s(:lasgn, :hamming_distance, s(:lit, 0)), s(:lasgn, :first_strand, s(:call, s(:lvar, :first_strand), :chars)), s(:lasgn, :second_strand, s(:call, s(:lvar, :second_strand), :chars)), s(:while, s(:and, s(:call, s(:call, s(:lvar, :first_strand), :empty?), :!), s(:call, s(:call, s(:lvar, :second_strand), :empty?), :!)), s(:block, s(:lasgn, :first_strand_letter, s(:call, s(:lvar, :first_strand), :shift)), s(:lasgn, :second_strand_letter, s(:call, s(:lvar, :second_strand), :shift)), s(:if, s(:call, s(:lvar, :first_strand_letter), :!=, s(:lvar, :second_strand_letter)), s(:lasgn, :hamming_distance, s(:call, s(:lvar, :hamming_distance), :+, s(:lit, 1))), nil)), true), s(:return, s(:lvar, :hamming_distance)))