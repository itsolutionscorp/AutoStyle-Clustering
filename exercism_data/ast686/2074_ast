s(:defn, :compute, s(:args, :strand_one, :strand_two), s(:lasgn, :min_distance, s(:nil)), s(:masgn, s(:array, s(:lasgn, :one), s(:lasgn, :two)), s(:to_ary, s(:iter, s(:call, s(:array, s(:lvar, :strand_one), s(:lvar, :strand_two)), :sort), s(:args, :a, :b), s(:call, s(:call, s(:lvar, :a), :length), :<=>, s(:call, s(:lvar, :b), :length))))), s(:iter, s(:call, s(:lit, 0), :upto, s(:call, s(:call, s(:lvar, :two), :length), :-, s(:call, s(:lvar, :one), :length))), s(:args, :offset), s(:block, s(:lasgn, :distance, s(:lit, 0)), s(:iter, s(:call, s(:lit, 0), :upto, s(:call, s(:call, s(:lvar, :one), :length), :-, s(:lit, 1))), s(:args, :index), s(:if, s(:call, s(:call, s(:lvar, :one), :[], s(:lvar, :index)), :!=, s(:call, s(:lvar, :two), :[], s(:call, s(:lvar, :index), :+, s(:lvar, :offset)))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil)), s(:lasgn, :min_distance, s(:if, s(:call, s(:lvar, :min_distance), :nil?), s(:lvar, :distance), s(:call, s(:array, s(:lvar, :distance), s(:lvar, :min_distance)), :min))))), s(:return, s(:lvar, :min_distance)))