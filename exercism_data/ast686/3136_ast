s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :strand_1, :strand_2), s(:lasgn, :first_strand, s(:call, s(:lvar, :strand_1), :split, s(:lit, //))), s(:lasgn, :second_strand, s(:call, s(:lvar, :strand_2), :split, s(:lit, //))), s(:lasgn, :hamming_distance, s(:lit, 0)), s(:lasgn, :i, s(:lit, 0)), s(:while, s(:call, s(:lvar, :i), :<, s(:call, s(:lvar, :first_strand), :length)), s(:if, s(:call, s(:call, s(:lvar, :first_strand), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :second_strand), :[], s(:lvar, :i))), s(:block, s(:lasgn, :hamming_distance, s(:call, s(:lvar, :hamming_distance), :+, s(:lit, 1))), s(:lasgn, :i, s(:call, s(:lvar, :i), :+, s(:lit, 1)))), s(:lasgn, :i, s(:call, s(:lvar, :i), :+, s(:lit, 1)))), true), s(:return, s(:lvar, :hamming_distance))))