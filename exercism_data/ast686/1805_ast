s(:defn, :compute, s(:args, :first_strand, :second_strand), s(:if, s(:call, s(:call, s(:lvar, :first_strand), :length), :!=, s(:call, s(:lvar, :second_strand), :length)), s(:call, nil, :raise, s(:str, "Uneven strands!")), nil), s(:lasgn, :distance, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :first_strand), :length), :times), s(:args, :i), s(:if, s(:call, s(:call, s(:lvar, :first_strand), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :second_strand), :[], s(:lvar, :i))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil)), s(:lvar, :distance))