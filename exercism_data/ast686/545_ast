s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :strand1, :strand2), s(:lasgn, :nucleotide_to_compare, s(:if, s(:call, s(:call, s(:lvar, :strand1), :length), :<, s(:call, s(:lvar, :strand2), :length)), s(:call, s(:call, s(:lvar, :strand1), :chars), :zip, s(:call, s(:lvar, :strand2), :chars)), s(:call, s(:call, s(:lvar, :strand2), :chars), :zip, s(:call, s(:lvar, :strand1), :chars)))), s(:lasgn, :distance, s(:lit, 0)), s(:iter, s(:call, s(:lvar, :nucleotide_to_compare), :each), s(:args, :n), s(:if, s(:call, s(:call, s(:lvar, :n), :[], s(:lit, 0)), :!=, s(:call, s(:lvar, :n), :[], s(:lit, 1))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil)), s(:lvar, :distance)))