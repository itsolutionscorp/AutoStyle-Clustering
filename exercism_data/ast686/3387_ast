s(:defn, :compute, s(:args, :strand_1, :strand_2), s(:lasgn, :nucleotide_number, s(:call, s(:lvar, :strand_1), :length)), s(:lasgn, :hamming_number, s(:lit, 0)), s(:for, s(:dot2, s(:lit, 0), s(:call, s(:lvar, :nucleotide_number), :+, s(:lit, 1))), s(:lasgn, :y), s(:if, s(:call, s(:call, s(:lvar, :strand_1), :[], s(:lvar, :y)), :!=, s(:call, s(:lvar, :strand_2), :[], s(:lvar, :y))), s(:lasgn, :hamming_number, s(:call, s(:lvar, :hamming_number), :+, s(:lit, 1))), nil)), s(:return, s(:lvar, :hamming_number)))