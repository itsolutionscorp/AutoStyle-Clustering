s(:defn, :compute, s(:args, :s1, :s2), s(:lasgn, :distance, s(:lit, 0)), s(:lasgn, :strand_1, s(:call, s(:call, s(:call, s(:lvar, :s1), :to_s), :downcase), :chars)), s(:lasgn, :strand_2, s(:call, s(:call, s(:call, s(:lvar, :s2), :to_s), :downcase), :chars)), s(:iter, s(:call, s(:lvar, :strand_1), :each_with_index), s(:args, :c, :idx), s(:if, s(:call, s(:lvar, :c), :==, s(:call, s(:lvar, :strand_2), :[], s(:lvar, :idx))), nil, s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))))), s(:lvar, :distance))