s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :a, :b), s(:if, s(:call, s(:call, s(:lvar, :a), :length), :==, s(:call, s(:lvar, :b), :length)), nil, s(:call, nil, :raise, s(:str, "The arguments do not have the same length."))), s(:lasgn, :b_chars, s(:call, s(:lvar, :b), :chars)), s(:lasgn, :distance, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :a), :chars), :each_with_index), s(:args, :c, :i), s(:if, s(:call, s(:lvar, :c), :==, s(:call, s(:lvar, :b_chars), :[], s(:lvar, :i))), nil, s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))))), s(:return, s(:lvar, :distance))))