s(:defn, :compute, s(:args, :string1, :string2), s(:lasgn, :result, s(:lit, 0)), s(:if, s(:call, s(:call, s(:lvar, :string1), :size), :<=, s(:call, s(:lvar, :string2), :size)), s(:block, s(:iasgn, :@short, s(:call, s(:lvar, :string1), :split, s(:lit, //))), s(:iasgn, :@long, s(:call, s(:lvar, :string2), :split, s(:lit, //)))), s(:block, s(:iasgn, :@short, s(:call, s(:lvar, :string2), :split, s(:lit, //))), s(:iasgn, :@long, s(:call, s(:lvar, :string1), :split, s(:lit, //))))), s(:iter, s(:call, s(:ivar, :@short), :each_index), s(:args, :i), s(:if, s(:call, s(:call, s(:ivar, :@short), :[], s(:lvar, :i)), :!=, s(:call, s(:ivar, :@long), :[], s(:lvar, :i))), s(:lasgn, :result, s(:call, s(:lvar, :result), :+, s(:lit, 1))), nil)), s(:lvar, :result))