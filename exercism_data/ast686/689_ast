s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :first, :second), s(:lasgn, :h, s(:lit, 0)), s(:lasgn, :first_array, s(:call, s(:lvar, :first), :split, s(:str, ""))), s(:lasgn, :second_array, s(:call, s(:lvar, :second), :split, s(:str, ""))), s(:iter, s(:call, s(:lvar, :first_array), :each_with_index), s(:args, :l, :i), s(:block, s(:if, s(:call, s(:lvar, :i), :>=, s(:call, s(:lvar, :second_array), :length)), s(:break), nil), s(:if, s(:call, s(:call, s(:lvar, :first_array), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :second_array), :[], s(:lvar, :i))), s(:lasgn, :h, s(:call, s(:lvar, :h), :+, s(:lit, 1))), nil))), s(:return, s(:lvar, :h))))