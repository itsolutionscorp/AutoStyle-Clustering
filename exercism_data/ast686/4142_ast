s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :strand_one, :strand_two), s(:lasgn, :min_length, s(:call, s(:array, s(:call, s(:lvar, :strand_one), :length), s(:call, s(:lvar, :strand_two), :length)), :min)), s(:lasgn, :hamming_distance, s(:lit, 0)), s(:for, s(:dot3, s(:lit, 0), s(:lvar, :min_length)), s(:lasgn, :idx), s(:if, s(:call, s(:call, s(:lvar, :strand_one), :[], s(:lvar, :idx)), :!=, s(:call, s(:lvar, :strand_two), :[], s(:lvar, :idx))), s(:lasgn, :hamming_distance, s(:call, s(:lvar, :hamming_distance), :+, s(:lit, 1))), nil)), s(:return, s(:lvar, :hamming_distance))))