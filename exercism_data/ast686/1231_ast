s(:defn, :compute, s(:args, :seq1, :seq2), s(:lasgn, :count, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :seq1), :chars), :each_with_index), s(:args, :letter, :idx), s(:block, s(:if, s(:call, s(:call, s(:lvar, :seq2), :[], s(:lvar, :idx)), :nil?), s(:break), nil), s(:if, s(:call, s(:lvar, :letter), :eql?, s(:call, s(:lvar, :seq2), :[], s(:lvar, :idx))), nil, s(:lasgn, :count, s(:call, s(:lvar, :count), :+, s(:lit, 1)))))), s(:return, s(:lvar, :count)))