s(:defn, :compute, s(:args, :strand_a, :strand_b), s(:lasgn, :i, s(:lit, 0)), s(:lasgn, :distance, s(:lit, 0)), s(:while, s(:call, s(:lvar, :i), :<, s(:call, s(:lvar, :strand_a), :length)), s(:block, s(:if, s(:call, s(:call, s(:lvar, :strand_a), :[], s(:lvar, :i)), :==, s(:call, s(:lvar, :strand_b), :[], s(:lvar, :i))), nil, s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1)))), s(:lasgn, :i, s(:call, s(:lvar, :i), :+, s(:lit, 1)))), true), s(:lvar, :distance))