s(:defn, :compute, s(:args, :a, :b), s(:lasgn, :hamming_count, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :a), :split, s(:str, "")), :each_with_index), s(:args, :char, :index), s(:block, s(:if, s(:or, s(:call, s(:lvar, :index), :==, s(:call, s(:lvar, :a), :length)), s(:call, s(:lvar, :index), :==, s(:call, s(:lvar, :b), :length))), s(:break), nil), s(:if, s(:call, s(:call, s(:lvar, :b), :[], s(:lvar, :index)), :==, s(:lvar, :char)), nil, s(:lasgn, :hamming_count, s(:call, s(:lvar, :hamming_count), :+, s(:lit, 1)))))), s(:lvar, :hamming_count))