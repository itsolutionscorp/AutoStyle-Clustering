s(:defn, :compute, s(:args, :a, :b), s(:lasgn, :sum, s(:lit, 0)), s(:iter, s(:call, s(:lit, 0), :upto, s(:call, s(:lvar, :a), :length)), s(:args, :x), s(:block, s(:if, s(:and, s(:call, s(:lvar, :a), :[], s(:lvar, :x)), s(:call, s(:lvar, :b), :[], s(:lvar, :x))), nil, s(:break)), s(:if, s(:call, s(:call, s(:lvar, :a), :[], s(:lvar, :x)), :!=, s(:call, s(:lvar, :b), :[], s(:lvar, :x))), s(:lasgn, :sum, s(:call, s(:lvar, :sum), :+, s(:lit, 1))), nil))), s(:lvar, :sum))