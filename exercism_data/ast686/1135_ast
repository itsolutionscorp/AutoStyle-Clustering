s(:class, :Hamming, nil, s(:sclass, s(:self), s(:defn, :compute, s(:args, :a, :b), s(:if, s(:call, s(:call, s(:lvar, :a), :<=>, s(:lvar, :b)), :==, s(:lit, 0)), s(:return, s(:call, s(:lvar, :a), :<=>, s(:lvar, :b))), nil), s(:lasgn, :h, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:call, s(:lvar, :a), :chars), :each), :with_index), s(:args, :char, :i), s(:if, s(:call, s(:lvar, :char), :!=, s(:call, s(:call, s(:lvar, :b), :split, s(:str, "")), :[], s(:lvar, :i))), s(:lasgn, :h, s(:call, s(:lvar, :h), :+, s(:lit, 1))), nil)), s(:lvar, :h))))