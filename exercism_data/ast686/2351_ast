s(:defn, :compute, s(:args, :strand_a, :strand_b), s(:lasgn, :hamming_distance, s(:lit, 0)), s(:lasgn, :shorter_length, s(:call, s(:array, s(:call, s(:lvar, :strand_a), :length), s(:call, s(:lvar, :strand_b), :length)), :min)), s(:iter, s(:call, s(:lvar, :shorter_length), :times), s(:args, :index), s(:lasgn, :hamming_distance, s(:call, s(:lvar, :hamming_distance), :+, s(:if, s(:call, s(:call, s(:lvar, :strand_a), :[], s(:lvar, :index)), :==, s(:call, s(:lvar, :strand_b), :[], s(:lvar, :index))), s(:lit, 0), s(:lit, 1))))), s(:return, s(:lvar, :hamming_distance)))