s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :input1, :input2), s(:lasgn, :minlength, s(:call, s(:array, s(:call, s(:lvar, :input1), :length), s(:call, s(:lvar, :input2), :length)), :min)), s(:lasgn, :i, s(:lit, 0)), s(:lasgn, :result, s(:lit, 0)), s(:while, s(:call, s(:lvar, :i), :<, s(:lvar, :minlength)), s(:block, s(:if, s(:call, s(:call, s(:lvar, :input1), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :input2), :[], s(:lvar, :i))), s(:lasgn, :result, s(:call, s(:lvar, :result), :+, s(:lit, 1))), nil), s(:lasgn, :i, s(:call, s(:lvar, :i), :+, s(:lit, 1)))), true), s(:return, s(:lvar, :result))))