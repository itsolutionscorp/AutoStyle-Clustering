s(:defn, :compute, s(:args, :str1, :str2), s(:lasgn, :distance, s(:lit, 0)), s(:lasgn, :sequence1, s(:call, s(:lvar, :str1), :chars)), s(:lasgn, :sequence2, s(:call, s(:lvar, :str2), :chars)), s(:iter, s(:call, s(:call, s(:lvar, :sequence1), :zip, s(:lvar, :sequence2)), :each), s(:args, :c1, :c2), s(:if, s(:or, s(:call, s(:lvar, :c1), :==, s(:lvar, :c2)), s(:call, s(:lvar, :c2), :nil?)), nil, s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))))), s(:return, s(:lvar, :distance)))