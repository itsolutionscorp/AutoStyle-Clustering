s(:defn, :compute, s(:args, :a, :b), s(:lasgn, :ham_dist, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:array, s(:call, s(:lvar, :a), :length), s(:call, s(:lvar, :b), :length)), :min), :times), s(:args, :i), s(:if, s(:call, s(:call, s(:lvar, :a), :[], s(:lvar, :i)), :==, s(:call, s(:lvar, :b), :[], s(:lvar, :i))), nil, s(:lasgn, :ham_dist, s(:call, s(:lvar, :ham_dist), :+, s(:lit, 1))))), s(:lvar, :ham_dist))