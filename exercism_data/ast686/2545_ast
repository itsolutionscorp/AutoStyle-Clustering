s(:defn, :compute, s(:args, :first, :second), s(:lasgn, :distance, s(:lit, 0)), s(:masgn, s(:array, s(:lasgn, :shorter), s(:lasgn, :longer)), s(:to_ary, s(:iter, s(:call, s(:array, s(:call, s(:lvar, :first), :chars), s(:call, s(:lvar, :second), :chars)), :sort), s(:args, :x, :y), s(:call, s(:call, s(:lvar, :x), :length), :<=>, s(:call, s(:lvar, :y), :length))))), s(:iter, s(:call, s(:lvar, :shorter), :each_with_index), s(:args, :ch, :i), s(:if, s(:call, s(:call, s(:lvar, :longer), :[], s(:lvar, :i)), :!=, s(:lvar, :ch)), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil)), s(:lvar, :distance))