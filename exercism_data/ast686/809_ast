s(:defn, :compute, s(:args, :a, :b), s(:if, s(:call, s(:call, s(:lvar, :a), :length), :>, s(:call, s(:lvar, :b), :length)), s(:masgn, s(:array, s(:lasgn, :a), s(:lasgn, :b)), s(:array, s(:lvar, :b), s(:lvar, :a))), nil), s(:lasgn, :distance, s(:lit, 0)), s(:iter, s(:call, s(:lit, 0), :upto, s(:call, s(:call, s(:lvar, :a), :length), :-, s(:lit, 1))), s(:args, :i), s(:if, s(:call, s(:call, s(:lvar, :a), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :b), :[], s(:lvar, :i))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil)), s(:return, s(:lvar, :distance)))