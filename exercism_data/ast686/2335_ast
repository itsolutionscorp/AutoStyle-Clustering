s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :first_strand, :second_strand), s(:lasgn, :min_length, s(:call, s(:array, s(:call, s(:lvar, :first_strand), :length), s(:call, s(:lvar, :second_strand), :length)), :min)), s(:lasgn, :sum, s(:lit, 0)), s(:iter, s(:call, s(:lvar, :min_length), :times), s(:args, :i), s(:if, s(:call, s(:call, s(:lvar, :first_strand), :[], s(:lvar, :i)), :==, s(:call, s(:lvar, :second_strand), :[], s(:lvar, :i))), nil, s(:lasgn, :sum, s(:call, s(:lvar, :sum), :+, s(:lit, 1))))), s(:return, s(:lvar, :sum))))