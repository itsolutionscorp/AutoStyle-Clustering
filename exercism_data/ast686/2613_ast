s(:defn, :compute, s(:args, :sequence_1, :sequence_2), s(:lasgn, :sequence_difference, s(:call, s(:call, s(:call, s(:lvar, :sequence_1), :length), :-, s(:call, s(:lvar, :sequence_2), :length)), :abs)), s(:lasgn, :distance, s(:call, s(:lit, 0), :-, s(:lvar, :sequence_difference))), s(:iter, s(:call, s(:lit, 0), :upto, s(:call, s(:lvar, :sequence_1), :length)), s(:args, :base), s(:or, s(:call, s(:call, s(:lvar, :sequence_1), :[], s(:lvar, :base)), :==, s(:call, s(:lvar, :sequence_2), :[], s(:lvar, :base))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))))), s(:lvar, :distance))