s(:defn, :compute, s(:args, :a, :b), s(:lasgn, :distance, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :a), :chars), :each_with_index), s(:args, :symbol, :index), s(:if, s(:call, s(:call, s(:lvar, :b), :[], s(:lvar, :index)), :!=, s(:lvar, :symbol)), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil)), s(:return, s(:lvar, :distance)))