s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :strand1, :strand2), s(:if, s(:call, s(:call, s(:lvar, :strand1), :length), :!=, s(:call, s(:lvar, :strand2), :length)), s(:call, nil, :raise, s(:str, "Error: strand length must be equal")), nil), s(:lasgn, :distance, s(:lit, 0)), s(:for, s(:dot2, s(:lit, 0), s(:call, s(:lvar, :strand1), :length)), s(:lasgn, :i), s(:if, s(:call, s(:call, s(:lvar, :strand1), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :strand2), :[], s(:lvar, :i))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil)), s(:return, s(:lvar, :distance))))