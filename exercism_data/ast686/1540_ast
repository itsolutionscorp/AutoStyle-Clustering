s(:defn, :compute, s(:args, :strand1, :strand2), s(:if, s(:call, s(:call, s(:lvar, :strand1), :length), :==, s(:call, s(:lvar, :strand2), :length)), nil, s(:call, nil, :raise, s(:const, :ArgumentError), s(:str, "Lengths must be equal"))), s(:iter, s(:call, s(:call, s(:call, s(:lvar, :strand1), :chars), :zip, s(:call, s(:lvar, :strand2), :chars)), :count), s(:args, :nucleotide1, :nucleotide2), s(:call, s(:lvar, :nucleotide1), :!=, s(:lvar, :nucleotide2))))