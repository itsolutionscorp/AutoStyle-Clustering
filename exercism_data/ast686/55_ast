s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :s1, :s2), s(:lasgn, :counter, s(:lit, 0)), s(:call, s(:lvar, :s1), :upcase), s(:call, s(:lvar, :s2), :upcase), s(:lasgn, :arr1, s(:call, s(:lvar, :s1), :scan, s(:lit, /\w/))), s(:lasgn, :arr2, s(:call, s(:lvar, :s2), :scan, s(:lit, /\w/))), s(:lasgn, :iterate, s(:if, s(:call, s(:call, s(:lvar, :s1), :length), :>, s(:call, s(:lvar, :s2), :length)), s(:call, s(:lvar, :s2), :length), s(:call, s(:lvar, :s1), :length))), s(:iter, s(:call, s(:lvar, :iterate), :times), s(:args, :i), s(:lasgn, :counter, s(:call, s(:lvar, :counter), :+, s(:if, s(:call, s(:call, s(:lvar, :arr1), :[], s(:lvar, :i)), :==, s(:call, s(:lvar, :arr2), :[], s(:lvar, :i))), s(:lit, 0), s(:lit, 1))))), s(:return, s(:lvar, :counter))))