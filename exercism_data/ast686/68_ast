s(:defn, :compute, s(:args, :"*strands"), s(:lasgn, :relevant_chars, s(:call, s(:call, s(:lvar, :strands), :map, s(:block_pass, s(:lit, :length))), :min)), s(:lasgn, :result, s(:lit, 0)), s(:for, s(:dot3, s(:lit, 0), s(:lvar, :relevant_chars)), s(:lasgn, :i), s(:if, s(:call, s(:call, s(:call, s(:lvar, :strands), :[], s(:lit, 0)), :[], s(:lvar, :i)), :==, s(:call, s(:call, s(:lvar, :strands), :[], s(:lit, 1)), :[], s(:lvar, :i))), s(:nil), s(:lasgn, :result, s(:call, s(:lvar, :result), :+, s(:lit, 1))))), s(:lvar, :result))