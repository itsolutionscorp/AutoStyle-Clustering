s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :a, :b), s(:iasgn, :@hamming_distance, s(:lit, 0)), s(:if, s(:call, s(:call, s(:lvar, :a), :length), :==, s(:call, s(:lvar, :b), :length)), s(:iter, s(:call, s(:call, s(:lvar, :a), :length), :times), s(:args, :i), s(:if, s(:call, s(:call, s(:lvar, :a), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :b), :[], s(:lvar, :i))), s(:iasgn, :@hamming_distance, s(:call, s(:ivar, :@hamming_distance), :+, s(:lit, 1))), nil)), s(:str, "cannot determine hamming distance of difference length strands")), s(:ivar, :@hamming_distance)))