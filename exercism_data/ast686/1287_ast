s(:defn, :compute, s(:args, :string1, :string2), s(:if, s(:call, s(:call, s(:lvar, :string1), :length), :!=, s(:call, s(:lvar, :string2), :length)), s(:call, nil, :raise, s(:str, "Input strings of unequal length.")), nil), s(:lasgn, :result, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:call, s(:lvar, :string1), :chars), :zip, s(:call, s(:lvar, :string2), :chars)), :each), s(:args, :char1, :char2), s(:if, s(:call, s(:lvar, :char1), :!=, s(:lvar, :char2)), s(:lasgn, :result, s(:call, s(:lvar, :result), :+, s(:lit, 1))), nil)), s(:lvar, :result))