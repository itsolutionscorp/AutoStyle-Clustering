s(:defn, :compute, s(:args, :a, :b), s(:ensure, s(:block, s(:lasgn, :count, s(:lit, 0)), s(:lasgn, :a_enum, s(:call, s(:lvar, :a), :each_char)), s(:lasgn, :b_enum, s(:call, s(:lvar, :b), :each_char)), s(:while, s(:true), s(:if, s(:call, s(:call, s(:lvar, :a_enum), :next), :==, s(:call, s(:lvar, :b_enum), :next)), nil, s(:lasgn, :count, s(:call, s(:lvar, :count), :+, s(:lit, 1)))), true)), s(:return, s(:lvar, :count))))