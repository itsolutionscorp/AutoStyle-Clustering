s(:defn, :compute, s(:args, :dna1, :dna2), s(:if, s(:or, s(:call, s(:lvar, :dna1), :nil?), s(:call, s(:lvar, :dna2), :nil?)), s(:return, s(:lit, 0)), nil), s(:lasgn, :diffs, s(:lit, 0)), s(:lasgn, :smaller_size, s(:if, s(:call, s(:call, s(:lvar, :dna1), :size), :<, s(:call, s(:lvar, :dna2), :size)), s(:call, s(:lvar, :dna1), :size), s(:call, s(:lvar, :dna2), :size))), s(:iter, s(:call, s(:dot2, s(:lit, 0), s(:call, s(:lvar, :smaller_size), :-, s(:lit, 1))), :each), s(:args, :i), s(:if, s(:call, s(:call, s(:lvar, :dna1), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :dna2), :[], s(:lvar, :i))), s(:lasgn, :diffs, s(:call, s(:lvar, :diffs), :+, s(:lit, 1))), nil)), s(:return, s(:lvar, :diffs)))