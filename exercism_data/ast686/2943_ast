s(:defn, :compute, s(:args, :strand1, :strand2), s(:if, s(:call, s(:call, s(:lvar, :strand1), :size), :<, s(:call, s(:lvar, :strand2), :size)), s(:masgn, s(:array, s(:lasgn, :shorter_strand), s(:lasgn, :longer_strand)), s(:array, s(:call, s(:lvar, :strand1), :split, s(:str, "")), s(:call, s(:lvar, :strand2), :split, s(:str, "")))), s(:masgn, s(:array, s(:lasgn, :longer_strand), s(:lasgn, :shorter_strand)), s(:array, s(:call, s(:lvar, :strand1), :split, s(:str, "")), s(:call, s(:lvar, :strand2), :split, s(:str, ""))))), s(:lasgn, :hamming_distance, s(:lit, 0)), s(:iter, s(:call, s(:lvar, :shorter_strand), :each_index), s(:args, :i), s(:if, s(:call, s(:call, s(:lvar, :shorter_strand), :[], s(:lvar, :i)), :==, s(:call, s(:lvar, :longer_strand), :[], s(:lvar, :i))), nil, s(:lasgn, :hamming_distance, s(:call, s(:lvar, :hamming_distance), :+, s(:lit, 1))))), s(:lvar, :hamming_distance))