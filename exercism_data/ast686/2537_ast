s(:defn, :compute, s(:args, :first, :second), s(:lasgn, :count, s(:lit, 0)), s(:lasgn, :s_limit, s(:call, s(:lvar, :second), :size)), s(:iter, s(:call, s(:call, s(:lvar, :first), :split, s(:str, "")), :each_with_index), s(:args, :a, :i), s(:block, s(:if, s(:call, s(:lvar, :s_limit), :==, s(:lvar, :i)), s(:break), nil), s(:if, s(:call, s(:lvar, :a), :!=, s(:call, s(:lvar, :second), :[], s(:lvar, :i))), s(:lasgn, :count, s(:call, s(:lvar, :count), :+, s(:lit, 1))), nil))), s(:lvar, :count))