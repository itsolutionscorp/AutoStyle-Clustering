s(:defn, :compute, s(:args, :dna1, :dna2), s(:lasgn, :hamming_difference, s(:lit, 0)), s(:if, s(:call, s(:call, s(:lvar, :dna1), :size), :<=, s(:call, s(:lvar, :dna2), :size)), s(:iter, s(:call, s(:call, s(:lvar, :dna1), :split, s(:str, "")), :each_index), s(:args, :index), s(:if, s(:call, s(:call, s(:call, s(:lvar, :dna1), :[], s(:lvar, :index)), :ord), :!=, s(:call, s(:call, s(:lvar, :dna2), :[], s(:lvar, :index)), :ord)), s(:lasgn, :hamming_difference, s(:call, s(:lvar, :hamming_difference), :+, s(:lit, 1))), nil)), s(:iter, s(:call, s(:call, s(:lvar, :dna2), :split, s(:str, "")), :each_index), s(:args, :index), s(:if, s(:call, s(:call, s(:call, s(:lvar, :dna1), :[], s(:lvar, :index)), :ord), :!=, s(:call, s(:call, s(:lvar, :dna2), :[], s(:lvar, :index)), :ord)), s(:lasgn, :hamming_difference, s(:call, s(:lvar, :hamming_difference), :+, s(:lit, 1))), nil))), s(:return, s(:lvar, :hamming_difference)))