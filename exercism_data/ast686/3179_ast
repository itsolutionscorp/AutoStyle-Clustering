s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :str1, :str2), s(:lasgn, :tmp, s(:if, s(:call, s(:call, s(:lvar, :str1), :length), :>, s(:call, s(:lvar, :str2), :length)), s(:lvar, :str2), s(:lvar, :str1))), s(:call, s(:iter, s(:call, s(:dot2, s(:lit, 1), s(:call, s(:lvar, :tmp), :length)), :map), s(:args, :i), s(:if, s(:call, s(:call, s(:lvar, :str1), :[], s(:call, s(:lvar, :i), :-, s(:lit, 1))), :==, s(:call, s(:lvar, :str2), :[], s(:call, s(:lvar, :i), :-, s(:lit, 1)))), s(:lit, 0), s(:lit, 1))), :inject, s(:lit, 0), s(:lit, :+))))