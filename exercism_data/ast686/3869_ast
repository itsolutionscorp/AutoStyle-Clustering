s(:defn, :compute, s(:args, :strand1, :strand2), s(:lasgn, :num, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:call, s(:lvar, :strand1), :chars), :to_a), :each_with_index), s(:args, :letter, :idx), s(:block, s(:if, s(:call, s(:call, s(:call, s(:call, s(:lvar, :strand2), :chars), :to_a), :[], s(:lvar, :idx)), :nil?), s(:break), nil), s(:if, s(:call, s(:lvar, :letter), :!=, s(:call, s(:call, s(:call, s(:lvar, :strand2), :chars), :to_a), :[], s(:lvar, :idx))), s(:lasgn, :num, s(:call, s(:lvar, :num), :+, s(:lit, 1))), nil))), s(:return, s(:lvar, :num)))