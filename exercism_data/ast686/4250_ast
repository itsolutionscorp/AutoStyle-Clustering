s(:defn, :compute, s(:args, :strand1, :strand2), s(:lasgn, :count, s(:lit, 0)), s(:lasgn, :array1, s(:call, s(:call, s(:lvar, :strand1), :to_s), :split, s(:str, ""))), s(:lasgn, :array2, s(:call, s(:call, s(:lvar, :strand2), :to_s), :split, s(:str, ""))), s(:if, s(:call, s(:call, s(:lvar, :array1), :length), :>, s(:call, s(:lvar, :array2), :length)), s(:lasgn, :shorter_array, s(:lvar, :array2)), s(:lasgn, :shorter_array, s(:lvar, :array1))), s(:for, s(:dot2, s(:lit, 0), s(:call, s(:call, s(:lvar, :shorter_array), :length), :-, s(:lit, 1))), s(:lasgn, :i), s(:if, s(:call, s(:call, s(:lvar, :array1), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :array2), :[], s(:lvar, :i))), s(:lasgn, :count, s(:call, s(:lvar, :count), :+, s(:lit, 1))), nil)), s(:return, s(:lvar, :count)))