s(:class, :Hamming, nil, s(:defs, s(:const, :Hamming), :compute, s(:args, :s1, :s2), s(:if, s(:call, s(:call, s(:lvar, :s1), :length), :==, s(:call, s(:lvar, :s2), :length)), nil, s(:call, nil, :raise, s(:str, "strings must have same length"))), s(:lasgn, :diff, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :s1), :length), :times), s(:args, :i), s(:if, s(:call, s(:call, s(:lvar, :s1), :[], s(:lvar, :i)), :==, s(:call, s(:lvar, :s2), :[], s(:lvar, :i))), nil, s(:lasgn, :diff, s(:call, s(:lvar, :diff), :+, s(:lit, 1))))), s(:lvar, :diff)))