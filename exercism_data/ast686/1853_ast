s(:defn, :compute, s(:args, :string1, :string2), s(:lasgn, :distance, s(:lit, 0)), s(:lasgn, :short_length, s(:call, s(:array, s(:call, s(:lvar, :string1), :length), s(:call, s(:lvar, :string2), :length)), :min)), s(:iter, s(:call, s(:lvar, :short_length), :times), s(:args, :i), s(:if, s(:call, s(:call, s(:lvar, :string1), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :string2), :[], s(:lvar, :i))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil)), s(:return, s(:lvar, :distance)))