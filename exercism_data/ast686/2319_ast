s(:defn, :compute, s(:args, :a, :b), s(:lasgn, :n, s(:call, s(:array, s(:call, s(:lvar, :a), :length), s(:call, s(:lvar, :b), :length)), :min)), s(:lasgn, :diff, s(:lit, 0)), s(:iter, s(:call, s(:array, s(:dot3, s(:lit, 0), s(:lvar, :n))), :each), s(:args, :i), s(:if, s(:call, s(:call, s(:lvar, :a), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :b), :[], s(:lvar, :i))), s(:lasgn, :diff, s(:call, s(:lvar, :diff), :+, s(:lit, 1))), nil)), s(:return, s(:lvar, :diff)))