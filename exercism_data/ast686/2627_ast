s(:module, :Hamming, s(:call, nil, :extend, s(:self)), s(:defn, :compute, s(:args, :strand_1, :strand_2), s(:masgn, s(:array, s(:lasgn, :shorter_strand), s(:lasgn, :longer_strand)), s(:to_ary, s(:call, s(:array, s(:call, s(:lvar, :strand_1), :chars), s(:call, s(:lvar, :strand_2), :chars)), :sort))), s(:lasgn, :distance, s(:lit, 0)), s(:iter, s(:call, s(:lvar, :shorter_strand), :each_with_index), s(:args, :n, :i), s(:if, s(:call, s(:call, s(:lvar, :longer_strand), :[], s(:lvar, :i)), :!=, s(:lvar, :n)), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil)), s(:return, s(:lvar, :distance))))