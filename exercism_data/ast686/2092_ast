s(:defn, :compute, s(:args, :a, :b), s(:lasgn, :distance, s(:lit, 0)), s(:lasgn, :length, s(:call, s(:lvar, :a), :length)), s(:while, s(:call, s(:lvar, :length), :>, s(:lit, 0)), s(:block, s(:lasgn, :length, s(:call, s(:lvar, :length), :-, s(:lit, 1))), s(:if, s(:call, s(:call, s(:lvar, :a), :[], s(:lvar, :length)), :!=, s(:call, s(:lvar, :b), :[], s(:lvar, :length))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil)), true), s(:lvar, :distance))