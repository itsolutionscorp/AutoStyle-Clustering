s(:defn, :compute, s(:args, :strand1, :strand2), s(:lasgn, :hamming_distance, s(:lit, 0)), s(:lasgn, :smallest_strand_length, s(:call, s(:array, s(:call, s(:lvar, :strand1), :length), s(:call, s(:lvar, :strand2), :length)), :min)), s(:iter, s(:call, s(:lvar, :smallest_strand_length), :times), s(:args, :i), s(:if, s(:call, s(:call, s(:lvar, :strand1), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :strand2), :[], s(:lvar, :i))), s(:lasgn, :hamming_distance, s(:call, s(:lvar, :hamming_distance), :+, s(:lit, 1))), nil)), s(:lvar, :hamming_distance))