s(:defn, :compute, s(:args, :seq1, :seq2), s(:lasgn, :hamming_distance, s(:lit, 0)), s(:lasgn, :max_length_comparable, s(:call, s(:array, s(:call, s(:lvar, :seq1), :length), s(:call, s(:lvar, :seq2), :length)), :min)), s(:for, s(:dot3, s(:lit, 0), s(:lvar, :max_length_comparable)), s(:lasgn, :i), s(:block, s(:lasgn, :nucleotide1, s(:call, s(:lvar, :seq1), :[], s(:lvar, :i))), s(:lasgn, :nucleotide2, s(:call, s(:lvar, :seq2), :[], s(:lvar, :i))), s(:if, s(:call, s(:lvar, :nucleotide1), :!=, s(:lvar, :nucleotide2)), s(:lasgn, :hamming_distance, s(:call, s(:lvar, :hamming_distance), :+, s(:lit, 1))), nil))), s(:return, s(:lvar, :hamming_distance)))