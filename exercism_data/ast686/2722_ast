s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :strand1, :strand2), s(:lasgn, :hamming_count, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :strand1), :split, s(:str, "")), :each_with_index), s(:args, :strand, :i), s(:block, s(:if, s(:call, s(:lvar, :strand), :!=, s(:call, s(:call, s(:lvar, :strand2), :split, s(:str, "")), :[], s(:lvar, :i))), s(:lasgn, :hamming_count, s(:call, s(:lvar, :hamming_count), :+, s(:lit, 1))), nil), s(:if, s(:or, s(:call, s(:lvar, :i), :==, s(:call, s(:call, s(:lvar, :strand2), :length), :-, s(:lit, 1))), s(:call, s(:lvar, :i), :==, s(:call, s(:call, s(:lvar, :strand1), :length), :-, s(:lit, 1)))), s(:break), nil))), s(:lvar, :hamming_count)))