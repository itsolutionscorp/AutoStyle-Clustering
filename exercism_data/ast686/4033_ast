s(:defn, :compute, s(:args, :strand_1, :strand_2), s(:lasgn, :hamming_distance, s(:lit, 0)), s(:iter, s(:call, s(:dot2, s(:lit, 0), s(:call, s(:lvar, :strand_1), :length)), :each), s(:args, :n), s(:if, s(:call, s(:call, s(:lvar, :strand_1), :[], s(:lvar, :n)), :!=, s(:call, s(:lvar, :strand_2), :[], s(:lvar, :n))), s(:lasgn, :hamming_distance, s(:call, s(:lvar, :hamming_distance), :+, s(:lit, 1))), nil)), s(:return, s(:lvar, :hamming_distance)))