s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :strand_one, :strand_two), s(:lasgn, :hamming_difference, s(:lit, 0)), s(:lasgn, :i, s(:lit, 0)), s(:while, s(:and, s(:call, s(:call, s(:lvar, :strand_one), :[], s(:lvar, :i)), :!=, s(:nil)), s(:call, s(:call, s(:lvar, :strand_two), :[], s(:lvar, :i)), :!=, s(:nil))), s(:block, s(:if, s(:call, s(:call, s(:lvar, :strand_one), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :strand_two), :[], s(:lvar, :i))), s(:lasgn, :hamming_difference, s(:call, s(:lvar, :hamming_difference), :+, s(:lit, 1))), nil), s(:lasgn, :i, s(:call, s(:lvar, :i), :+, s(:lit, 1)))), true), s(:return, s(:lvar, :hamming_difference))))