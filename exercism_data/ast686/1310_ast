s(:defn, :compute, s(:args, :dna_1, :dna_2), s(:lasgn, :shortest, s(:if, s(:call, s(:call, s(:lvar, :dna_1), :length), :<=, s(:call, s(:lvar, :dna_2), :length)), s(:call, s(:lvar, :dna_1), :length), s(:call, s(:lvar, :dna_2), :length))), s(:lasgn, :distance, s(:lit, 0)), s(:iter, s(:call, s(:lvar, :shortest), :times), s(:args, :i), s(:if, s(:call, s(:call, s(:lvar, :dna_1), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :dna_2), :[], s(:lvar, :i))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil)), s(:return, s(:lvar, :distance)))