s(:defn, :compute, s(:args, :x, :y), s(:lasgn, :hamdist, s(:lit, 0)), s(:if, s(:call, s(:call, s(:lvar, :x), :length), :<=, s(:call, s(:lvar, :y), :length)), s(:lasgn, :nuc_length, s(:call, s(:lvar, :x), :length)), s(:lasgn, :nuc_length, s(:call, s(:lvar, :y), :length))), s(:lasgn, :i, s(:lit, 0)), s(:while, s(:call, s(:lvar, :i), :<, s(:lvar, :nuc_length)), s(:block, s(:if, s(:call, s(:call, s(:lvar, :x), :[], s(:lvar, :i)), :==, s(:call, s(:lvar, :y), :[], s(:lvar, :i))), s(:lasgn, :hamdist, s(:call, s(:lvar, :hamdist), :+, s(:lit, 0))), s(:lasgn, :hamdist, s(:call, s(:lvar, :hamdist), :+, s(:lit, 1)))), s(:lasgn, :i, s(:call, s(:lvar, :i), :+, s(:lit, 1)))), true), s(:lvar, :hamdist))