s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :s1, :s2), s(:lasgn, :distance, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :s1), :chars), :each_with_index), s(:args, :char, :idx), s(:block, s(:if, s(:or, s(:call, s(:lvar, :char), :nil?), s(:call, s(:call, s(:lvar, :s2), :[], s(:lvar, :idx)), :nil?)), s(:break), nil), s(:if, s(:call, s(:lvar, :char), :!=, s(:call, s(:lvar, :s2), :[], s(:lvar, :idx))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil))), s(:return, s(:lvar, :distance))))