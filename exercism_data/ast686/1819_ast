s(:defn, :compute, s(:args, :let1, :let2), s(:lasgn, :sum, s(:lit, 0)), s(:lasgn, :all_letters, s(:call, s(:call, s(:lvar, :let1), :chars), :zip, s(:call, s(:lvar, :let2), :chars))), s(:iter, s(:call, s(:lvar, :all_letters), :each), s(:args, :pair), s(:if, s(:call, s(:call, s(:lvar, :pair), :[], s(:lit, 0)), :!=, s(:call, s(:lvar, :pair), :[], s(:lit, 1))), s(:lasgn, :sum, s(:call, s(:lvar, :sum), :+, s(:lit, 1))), nil)), s(:lvar, :sum))