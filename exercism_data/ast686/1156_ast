s(:defn, :compute, s(:args, :a, :b), s(:if, s(:call, s(:call, s(:lvar, :a), :length), :!=, s(:call, s(:lvar, :b), :length)), s(:call, nil, :raise, s(:call, nil, :ArgumentError, s(:str, "Strands must be of equal length"))), nil), s(:iter, s(:call, s(:call, s(:call, s(:lvar, :a), :length), :times), :count), s(:args, :index), s(:call, s(:call, s(:lvar, :a), :[], s(:lvar, :index)), :!=, s(:call, s(:lvar, :b), :[], s(:lvar, :index)))))