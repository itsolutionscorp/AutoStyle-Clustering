s(:defn, :compute, s(:args, :first, :second), s(:lasgn, :first, s(:call, s(:lvar, :first), :chars)), s(:lasgn, :second, s(:call, s(:lvar, :second), :chars)), s(:lasgn, :index, s(:lit, 0)), s(:lasgn, :hamming_count, s(:lit, 0)), s(:while, s(:and, s(:call, s(:lvar, :index), :<, s(:call, s(:lvar, :first), :count)), s(:call, s(:lvar, :index), :<, s(:call, s(:lvar, :second), :count))), s(:if, s(:call, s(:call, s(:lvar, :first), :[], s(:lvar, :index)), :==, s(:call, s(:lvar, :second), :[], s(:lvar, :index))), s(:lasgn, :index, s(:call, s(:lvar, :index), :+, s(:lit, 1))), s(:block, s(:lasgn, :hamming_count, s(:call, s(:lvar, :hamming_count), :+, s(:lit, 1))), s(:lasgn, :index, s(:call, s(:lvar, :index), :+, s(:lit, 1))))), true), s(:lvar, :hamming_count))