s(:defn, :compute, s(:args, :dna1, :dna2), s(:lasgn, :test_length, s(:call, s(:array, s(:call, s(:lvar, :dna1), :length), s(:call, s(:lvar, :dna2), :length)), :min)), s(:lasgn, :differences, s(:lit, 0)), s(:for, s(:dot2, s(:lit, 0), s(:call, s(:lvar, :test_length), :-, s(:lit, 1))), s(:lasgn, :i), s(:if, s(:call, s(:call, s(:lvar, :dna1), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :dna2), :[], s(:lvar, :i))), s(:lasgn, :differences, s(:call, s(:lvar, :differences), :+, s(:lit, 1))), nil)), s(:return, s(:lvar, :differences)))