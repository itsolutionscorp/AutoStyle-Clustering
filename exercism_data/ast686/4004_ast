s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :first_dna, :second_dna), s(:lasgn, :shorter_dna_length, s(:if, s(:call, s(:call, s(:lvar, :first_dna), :length), :<, s(:call, s(:lvar, :second_dna), :length)), s(:call, s(:lvar, :first_dna), :length), s(:call, s(:lvar, :second_dna), :length))), s(:lasgn, :nucleotides_range, s(:dot2, s(:lit, 0), s(:call, s(:lvar, :shorter_dna_length), :-, s(:lit, 1)))), s(:lasgn, :hamming_distance, s(:lit, 0)), s(:for, s(:lvar, :nucleotides_range), s(:lasgn, :single_nucleotide), s(:if, s(:call, s(:call, s(:lvar, :first_dna), :[], s(:lvar, :single_nucleotide)), :!=, s(:call, s(:lvar, :second_dna), :[], s(:lvar, :single_nucleotide))), s(:lasgn, :hamming_distance, s(:call, s(:lvar, :hamming_distance), :+, s(:lit, 1))), nil)), s(:lvar, :hamming_distance)))