s(:defn, :compute, s(:args, :input1, :input2), s(:call, nil, :attr_accessor, s(:lit, :hamming_distance), s(:lit, :max_length)), s(:lasgn, :hamming_distance, s(:lit, 0)), s(:lasgn, :dna1, s(:call, s(:lvar, :input1), :split, s(:str, ""))), s(:lasgn, :dna1_length, s(:call, s(:lvar, :dna1), :length)), s(:lasgn, :dna2, s(:call, s(:lvar, :input2), :split, s(:str, ""))), s(:lasgn, :dna2_length, s(:call, s(:lvar, :dna2), :length)), s(:if, s(:call, s(:lvar, :dna1_length), :==, s(:lvar, :dna2_length)), s(:lasgn, :max_length, s(:lvar, :dna1_length)), s(:if, s(:call, s(:lvar, :dna1_length), :<, s(:lvar, :dna2_length)), s(:lasgn, :max_length, s(:lvar, :dna1_length)), s(:lasgn, :max_length, s(:lvar, :dna2_length)))), s(:iter, s(:call, s(:dot3, s(:lit, 0), s(:lvar, :max_length)), :each), s(:args, :i), s(:if, s(:call, s(:call, s(:lvar, :dna1), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :dna2), :[], s(:lvar, :i))), s(:lasgn, :hamming_distance, s(:call, s(:lvar, :hamming_distance), :+, s(:lit, 1))), nil)), s(:lvar, :hamming_distance))