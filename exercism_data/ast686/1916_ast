s(:defn, :compute, s(:args, :gene1, :gene2), s(:lasgn, :distance, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :gene1), :each_char), :with_index), s(:args, :elem, :index), s(:if, s(:call, s(:lvar, :elem), :!=, s(:call, s(:lvar, :gene2), :[], s(:lvar, :index))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil)), s(:return, s(:lvar, :distance)))