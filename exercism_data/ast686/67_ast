s(:block, s(:call, nil, :require, s(:str, "pry-debugger")), s(:class, :Phrase, nil, s(:defn, :initialize, s(:args, :phrase), s(:iasgn, :@phrase, s(:lvar, :phrase))), s(:defn, :word_count, s(:args), s(:hash, s(:ivar, :@phrase), s(:lit, 1)), s(:lasgn, :tokens, s(:call, s(:call, s(:call, s(:ivar, :@phrase), :gsub, s(:lit, /[^A-Za-z0-9\w]+/), s(:str, " ")), :split), :map, s(:block_pass, s(:lit, :downcase)))), s(:iter, s(:call, s(:lvar, :tokens), :inject, s(:hash)), s(:args, :count, :token), s(:block, s(:if, s(:call, s(:lvar, :count), :has_key?, s(:lvar, :token)), s(:attrasgn, s(:lvar, :count), :[]=, s(:lvar, :token), s(:call, s(:call, s(:lvar, :count), :[], s(:lvar, :token)), :+, s(:lit, 1))), s(:attrasgn, s(:lvar, :count), :[]=, s(:lvar, :token), s(:lit, 1))), s(:lvar, :count))))))