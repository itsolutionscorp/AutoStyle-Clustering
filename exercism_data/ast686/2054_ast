s(:defn, :compute, s(:args, :s, :t), s(:if, s(:call, s(:call, s(:lvar, :s), :length), :<, s(:call, s(:lvar, :t), :length)), s(:block, s(:lasgn, :a_str, s(:lvar, :s)), s(:lasgn, :b_str, s(:lvar, :t))), s(:block, s(:lasgn, :a_str, s(:lvar, :t)), s(:lasgn, :b_str, s(:lvar, :s)))), s(:lasgn, :hamming, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :a_str), :each_char), :with_index), s(:args, :a_char, :i), s(:block, s(:lasgn, :b_char, s(:call, s(:lvar, :b_str), :[], s(:lvar, :i))), s(:if, s(:call, s(:lvar, :a_char), :!=, s(:lvar, :b_char)), s(:lasgn, :hamming, s(:call, s(:lvar, :hamming), :+, s(:lit, 1))), nil))), s(:lvar, :hamming))