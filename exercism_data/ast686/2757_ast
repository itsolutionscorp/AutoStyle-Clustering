s(:defn, :compute, s(:args, :a, :b), s(:lasgn, :a, s(:if, s(:call, s(:call, s(:lvar, :a), :size), :>, s(:call, s(:lvar, :b), :size)), s(:call, s(:lvar, :a), :[], s(:lit, 0...-1)), s(:lvar, :a))), s(:lasgn, :new_a, s(:call, s(:call, s(:lvar, :a), :scan, s(:lit, /\w/)), :zip, s(:call, s(:lvar, :b), :scan, s(:lit, /\w/)))), s(:call, s(:iter, s(:call, s(:call, s(:lvar, :new_a), :map, s(:block_pass, s(:lit, :uniq))), :delete_if), s(:args, :a), s(:call, s(:call, s(:lvar, :a), :count), :==, s(:lit, 1))), :count))