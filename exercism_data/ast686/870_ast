s(:defn, :compute, s(:args, :a, :b), s(:iter, s(:call, s(:call, s(:call, s(:lvar, :a), :chars), :zip, s(:call, s(:lvar, :b), :chars)), :inject, s(:lit, 0)), s(:args, :sum, :i), s(:if, s(:call, s(:call, s(:lvar, :i), :[], s(:lit, 0)), :!=, s(:call, s(:lvar, :i), :[], s(:lit, 1))), s(:lasgn, :sum, s(:call, s(:lvar, :sum), :+, s(:lit, 1))), s(:lvar, :sum))))