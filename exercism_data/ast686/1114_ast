s(:defn, :compute, s(:args, :first, :second), s(:lasgn, :distance, s(:lit, 0)), s(:lasgn, :length, s(:call, s(:array, s(:call, s(:lvar, :first), :length), s(:call, s(:lvar, :second), :length)), :min)), s(:lasgn, :length, s(:call, s(:lvar, :length), :-, s(:lit, 1))), s(:iter, s(:call, s(:dot2, s(:lit, 0), s(:lvar, :length)), :each), s(:args, :i), s(:if, s(:call, s(:call, s(:lvar, :first), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :second), :[], s(:lvar, :i))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil)), s(:return, s(:lvar, :distance)))