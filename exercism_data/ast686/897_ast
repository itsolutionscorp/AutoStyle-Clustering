s(:defn, :compute, s(:args, :str1, :str2), s(:lasgn, :i, s(:lit, 0)), s(:lasgn, :char1, s(:call, s(:lvar, :str1), :chars)), s(:lasgn, :char2, s(:call, s(:lvar, :str2), :chars)), s(:iter, s(:call, s(:call, s(:lvar, :char1), :zip, s(:lvar, :char2)), :each), s(:args, :c1, :c2), s(:if, s(:and, s(:call, s(:lvar, :c1), :!=, s(:lvar, :c2)), s(:call, s(:call, s(:lvar, :c2), :nil?), :!)), s(:lasgn, :i, s(:call, s(:lvar, :i), :+, s(:lit, 1))), nil)), s(:return, s(:lvar, :i)))