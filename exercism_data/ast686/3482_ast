s(:defn, :compute, s(:args, :array1, :array2), s(:lasgn, :difference, s(:lit, 0)), s(:lasgn, :shortest_length, s(:if, s(:call, s(:call, s(:lvar, :array1), :length), :<, s(:call, s(:lvar, :array2), :length)), s(:call, s(:lvar, :array1), :length), s(:call, s(:lvar, :array2), :length))), s(:iter, s(:call, s(:lvar, :shortest_length), :times), s(:args, :i), s(:if, s(:call, s(:call, s(:lvar, :array1), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :array2), :[], s(:lvar, :i))), s(:lasgn, :difference, s(:call, s(:lvar, :difference), :+, s(:lit, 1))), nil)), s(:return, s(:lvar, :difference)))