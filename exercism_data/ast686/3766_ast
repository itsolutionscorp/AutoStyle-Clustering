s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :strand_1, :strand_2), s(:if, s(:call, s(:call, s(:lvar, :strand_1), :length), :==, s(:call, s(:lvar, :strand_2), :length)), nil, s(:call, nil, :raise, s(:str, "both strands must be of equal length"))), s(:lasgn, :strand_1_chars, s(:call, s(:lvar, :strand_1), :split, s(:str, ""))), s(:lasgn, :strand_2_chars, s(:call, s(:lvar, :strand_2), :split, s(:str, ""))), s(:lasgn, :diff, s(:iter, s(:call, s(:call, s(:lvar, :strand_1_chars), :reject), :with_index), s(:args, :char, :index), s(:call, s(:lvar, :char), :==, s(:call, s(:lvar, :strand_2_chars), :[], s(:lvar, :index))))), s(:call, s(:lvar, :diff), :length)))