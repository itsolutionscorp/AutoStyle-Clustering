s(:defn, :compute, s(:args, :string_a, :string_b), s(:lasgn, :min_length, s(:call, s(:array, s(:call, s(:call, s(:lvar, :string_a), :to_s), :length), s(:call, s(:call, s(:lvar, :string_b), :to_s), :length)), :min)), s(:lasgn, :hamming_distance, s(:lit, 0)), s(:iter, s(:call, s(:dot2, s(:lit, 0), s(:call, s(:lvar, :min_length), :-, s(:lit, 1))), :each), s(:args, :index), s(:block, s(:lasgn, :character_a, s(:call, s(:call, s(:lvar, :string_a), :[], s(:lvar, :index)), :upcase)), s(:lasgn, :character_b, s(:call, s(:call, s(:lvar, :string_b), :[], s(:lvar, :index)), :upcase)), s(:if, s(:call, s(:lvar, :character_a), :!=, s(:lvar, :character_b)), s(:lasgn, :hamming_distance, s(:call, s(:lvar, :hamming_distance), :+, s(:lit, 1))), nil))), s(:return, s(:lvar, :hamming_distance)))