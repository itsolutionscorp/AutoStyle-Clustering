s(:defn, :compute, s(:args, :strand1, :strand2), s(:lasgn, :i, s(:lit, 0)), s(:lasgn, :difference_count, s(:lit, 0)), s(:masgn, s(:array, s(:lasgn, :strand1), s(:lasgn, :strand2)), s(:array, s(:call, s(:lvar, :strand1), :split, s(:lit, //)), s(:call, s(:lvar, :strand2), :split, s(:lit, //)))), s(:iter, s(:call, s(:lvar, :strand1), :each), s(:args, :point), s(:block, s(:if, s(:call, s(:lvar, :point), :!=, s(:call, s(:lvar, :strand2), :[], s(:lvar, :i))), s(:lasgn, :difference_count, s(:call, s(:lvar, :difference_count), :+, s(:lit, 1))), nil), s(:lasgn, :i, s(:call, s(:lvar, :i), :+, s(:lit, 1))))), s(:return, s(:lvar, :difference_count)))