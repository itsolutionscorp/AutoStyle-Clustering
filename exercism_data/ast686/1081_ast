s(:defn, :compute, s(:args, :strand1, :strand2), s(:if, s(:call, s(:call, s(:lvar, :strand1), :length), :!=, s(:call, s(:lvar, :strand2), :length)), s(:call, nil, :abort), nil), s(:lasgn, :hamming_count, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:call, s(:lvar, :strand1), :chars), :zip, s(:call, s(:lvar, :strand2), :chars)), :each), s(:args, :val), s(:if, s(:call, s(:call, s(:lvar, :val), :[], s(:lit, 0)), :==, s(:call, s(:lvar, :val), :[], s(:lit, 1))), nil, s(:lasgn, :hamming_count, s(:call, s(:lvar, :hamming_count), :+, s(:lit, 1))))), s(:lvar, :hamming_count))