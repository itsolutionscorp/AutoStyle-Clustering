s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :strand1, :strand2), s(:lasgn, :mismatch, s(:lit, 0)), s(:lasgn, :strand1_chars, s(:call, s(:lvar, :strand1), :split, s(:str, ""))), s(:lasgn, :strand2_chars, s(:call, s(:lvar, :strand2), :split, s(:str, ""))), s(:while, s(:lasgn, :one, s(:call, s(:lvar, :strand1_chars), :shift)), s(:block, s(:if, s(:call, s(:lvar, :strand2_chars), :empty?), s(:break), nil), s(:if, s(:call, s(:lvar, :one), :!=, s(:call, s(:lvar, :strand2_chars), :shift)), s(:lasgn, :mismatch, s(:call, s(:lvar, :mismatch), :+, s(:lit, 1))), nil)), true), s(:lvar, :mismatch)))