s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :strand_1, :strand_2), s(:lasgn, :place_holder, s(:lit, 0)), s(:lasgn, :differences, s(:lit, 0)), s(:iter, s(:call, s(:lvar, :strand_1), :each_char), s(:args, :c), s(:block, s(:if, s(:call, s(:lvar, :c), :!=, s(:call, s(:lvar, :strand_2), :[], s(:lvar, :place_holder))), s(:lasgn, :differences, s(:call, s(:lvar, :differences), :+, s(:lit, 1))), nil), s(:lasgn, :place_holder, s(:call, s(:lvar, :place_holder), :+, s(:lit, 1))), s(:if, s(:or, s(:call, s(:lvar, :place_holder), :>=, s(:call, s(:lvar, :strand_1), :length)), s(:call, s(:lvar, :place_holder), :>=, s(:call, s(:lvar, :strand_2), :length))), s(:break), nil))), s(:return, s(:lvar, :differences))))