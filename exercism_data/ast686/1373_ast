s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :a, :b), s(:if, s(:or, s(:call, s(:lvar, :a), :nil?), s(:call, s(:lvar, :b), :nil?)), s(:call, nil, :raise, s(:const, :ArgumentError), s(:str, "Arguments must be present")), nil), s(:if, s(:call, s(:call, s(:lvar, :a), :length), :!=, s(:call, s(:lvar, :b), :length)), s(:call, nil, :raise, s(:const, :ArgumentError), s(:str, "Arguments should be the same length")), nil), s(:lasgn, :hamming_difference, s(:lasgn, :character_index, s(:lit, 0))), s(:iter, s(:call, s(:lvar, :a), :each_char), s(:args, :a_char), s(:block, s(:if, s(:call, s(:lvar, :a_char), :!=, s(:call, s(:lvar, :b), :[], s(:lvar, :character_index))), s(:lasgn, :hamming_difference, s(:call, s(:lvar, :hamming_difference), :+, s(:lit, 1))), nil), s(:lasgn, :character_index, s(:call, s(:lvar, :character_index), :+, s(:lit, 1))))), s(:lvar, :hamming_difference)))