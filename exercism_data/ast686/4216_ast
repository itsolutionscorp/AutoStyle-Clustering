s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :strand1, :strand2), s(:iter, s(:call, s(:call, s(:call, s(:lvar, :strand1), :chars), :zip, s(:call, s(:lvar, :strand2), :chars)), :reduce, s(:lit, 0)), s(:args, :distance, :bases), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:call, s(:call, s(:call, s(:lvar, :bases), :[], s(:lit, 0)), :<=>, s(:call, s(:lvar, :bases), :[], s(:lit, 1))), :abs))))))