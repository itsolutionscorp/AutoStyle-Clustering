s(:defn, :compute, s(:args, :first, :second), s(:lasgn, :min_string_length, s(:call, s(:array, s(:call, s(:lvar, :first), :length), s(:call, s(:lvar, :second), :length)), :min)), s(:lasgn, :distance, s(:lit, 0)), s(:iter, s(:call, s(:lit, 0), :upto, s(:call, s(:lvar, :min_string_length), :-, s(:lit, 1))), s(:args, :index), s(:if, s(:call, s(:call, s(:lvar, :first), :[], s(:lvar, :index)), :!=, s(:call, s(:lvar, :second), :[], s(:lvar, :index))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil)), s(:return, s(:lvar, :distance)))