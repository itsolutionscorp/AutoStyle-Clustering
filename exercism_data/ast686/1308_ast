s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :strand1, :strand2), s(:if, s(:call, s(:call, s(:lvar, :strand1), :length), :==, s(:call, s(:lvar, :strand2), :length)), s(:str, "Wrong length. The strands have to be the same length"), nil), s(:lasgn, :splitted_strand_1, s(:call, s(:lvar, :strand1), :chars)), s(:lasgn, :splitted_strand_2, s(:call, s(:lvar, :strand2), :chars)), s(:lasgn, :zipped_strands, s(:call, s(:lvar, :splitted_strand_1), :zip, s(:lvar, :splitted_strand_2))), s(:iter, s(:call, s(:lvar, :zipped_strands), :count), s(:args, :s1, :s2), s(:call, s(:lvar, :s1), :!=, s(:lvar, :s2)))))