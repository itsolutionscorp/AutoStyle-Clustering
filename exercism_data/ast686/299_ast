s(:defn, :compute, s(:args, :seq_a, :seq_b), s(:if, s(:call, s(:call, s(:lvar, :seq_a), :length), :==, s(:call, s(:lvar, :seq_b), :length)), nil, s(:call, nil, :raise, s(:call, s(:const, :ArgumentError), :new, s(:str, "Sequences must be of the same length")))), s(:lasgn, :distance, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:call, s(:lvar, :seq_a), :chars), :zip, s(:call, s(:lvar, :seq_b), :chars)), :each), s(:args, :char_a, :char_b), s(:if, s(:call, s(:lvar, :char_a), :!=, s(:lvar, :char_b)), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil)), s(:return, s(:lvar, :distance)))