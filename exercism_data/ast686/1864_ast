s(:defn, :compute, s(:args, :a1, :a2), s(:lasgn, :r, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:call, s(:lvar, :a1), :split, s(:str, "")), :zip, s(:call, s(:lvar, :a2), :split, s(:str, ""))), :each), s(:args, :i, :j), s(:if, s(:or, s(:call, s(:lvar, :i), :==, s(:lvar, :j)), s(:or, s(:call, s(:lvar, :i), :nil?), s(:call, s(:lvar, :j), :nil?))), nil, s(:lasgn, :r, s(:call, s(:lvar, :r), :+, s(:lit, 1))))), s(:return, s(:lvar, :r)))