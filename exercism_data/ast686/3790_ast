s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :a, :b), s(:call, s(:call, s(:iter, s(:call, s(:call, s(:call, s(:call, s(:array, s(:lvar, :a), s(:lvar, :b)), :min_by, s(:block_pass, s(:lit, :length))), :length), :times), :map), s(:args, :idx), s(:call, s(:call, s(:lvar, :a), :[], s(:lvar, :idx)), :==, s(:call, s(:lvar, :b), :[], s(:lvar, :idx)))), :select, s(:block_pass, s(:lit, :!))), :length)))