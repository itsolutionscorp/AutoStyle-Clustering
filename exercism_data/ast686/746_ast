s(:defn, :compute, s(:args, :input1, :input2), s(:if, s(:call, s(:call, s(:lvar, :input1), :length), :==, s(:call, s(:lvar, :input2), :length)), nil, s(:call, nil, :raise, s(:const, :ArgumentError), s(:str, "Inputs are not of equal length"))), s(:lasgn, :hamming_distance, s(:lit, 0)), s(:for, s(:dot2, s(:lit, 0), s(:call, s(:lvar, :input1), :length)), s(:lasgn, :i), s(:if, s(:call, s(:call, s(:lvar, :input1), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :input2), :[], s(:lvar, :i))), s(:lasgn, :hamming_distance, s(:call, s(:lvar, :hamming_distance), :+, s(:lit, 1))), nil)), s(:return, s(:lvar, :hamming_distance)))