s(:defn, :compute, s(:args, :strand_a, :strand_b), s(:lasgn, :strand_a, s(:call, s(:lvar, :strand_a), :split, s(:str, ""))), s(:lasgn, :strand_b, s(:call, s(:lvar, :strand_b), :split, s(:str, ""))), s(:if, s(:call, s(:lvar, :strand_a), :==, s(:lvar, :strand_b)), s(:lit, 0), nil), s(:if, s(:call, s(:call, s(:lvar, :strand_a), :length), :==, s(:call, s(:lvar, :strand_b), :length)), s(:block, s(:lasgn, :distance, s(:lit, 0)), s(:iter, s(:call, s(:lvar, :strand_a), :each_with_index), s(:args, :nucleotide, :index), s(:if, s(:call, s(:lvar, :nucleotide), :==, s(:call, s(:lvar, :strand_b), :[], s(:lvar, :index))), nil, s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))))), s(:lvar, :distance)), nil))