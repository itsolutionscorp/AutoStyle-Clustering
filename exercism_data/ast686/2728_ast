s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :strand_one, :strand_two), s(:if, s(:call, s(:lvar, :strand_one), :==, s(:lvar, :strand_two)), s(:return, s(:lit, 0)), nil), s(:lasgn, :shortest_length, s(:call, s(:array, s(:call, s(:lvar, :strand_one), :size), s(:call, s(:lvar, :strand_two), :size)), :min)), s(:lasgn, :hamming_dist, s(:lit, 0)), s(:iter, s(:call, s(:lvar, :shortest_length), :times), s(:args, :i), s(:if, s(:call, s(:call, s(:lvar, :strand_one), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :strand_two), :[], s(:lvar, :i))), s(:lasgn, :hamming_dist, s(:call, s(:lvar, :hamming_dist), :+, s(:lit, 1))), nil)), s(:lvar, :hamming_dist)))