s(:defn, :compute, s(:args, :strand_a, :strand_b), s(:lasgn, :difference, s(:lit, 0)), s(:if, s(:call, s(:call, s(:lvar, :strand_a), :length), :>, s(:call, s(:lvar, :strand_b), :length)), s(:lasgn, :strand_a, s(:call, s(:lvar, :strand_a), :[], s(:lit, 0), s(:call, s(:lvar, :strand_b), :length))), nil), s(:iter, s(:call, s(:call, s(:call, s(:call, s(:lvar, :strand_a), :split, s(:str, "")), :zip, s(:call, s(:lvar, :strand_b), :split, s(:str, ""))), :compact), :each), s(:args, :l, :r), s(:if, s(:call, s(:lvar, :l), :==, s(:lvar, :r)), nil, s(:lasgn, :difference, s(:call, s(:lvar, :difference), :+, s(:lit, 1))))), s(:lvar, :difference))