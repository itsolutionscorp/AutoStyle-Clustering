s(:defn, :compute, s(:args, :strand1, :strand2), s(:lasgn, :hamming_difference, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :strand1), :split, s(:str, "")), :each_with_index), s(:args, :character, :index), s(:if, s(:call, s(:call, s(:lvar, :strand2), :length), :>, s(:lvar, :index)), s(:lasgn, :hamming_difference, s(:call, s(:lvar, :hamming_difference), :+, s(:call, s(:call, s(:lvar, :character), :<=>, s(:call, s(:lvar, :strand2), :[], s(:lvar, :index))), :abs))), nil)), s(:return, s(:lvar, :hamming_difference)))