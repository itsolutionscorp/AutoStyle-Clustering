s(:defn, :compute, s(:args, :x, :y), s(:iter, s(:call, s(:call, s(:call, s(:lvar, :x), :chars), :zip, s(:call, s(:lvar, :y), :chars)), :inject, s(:lit, 0)), s(:args, :distance, :pair), s(:if, s(:call, s(:call, s(:lvar, :pair), :[], s(:lit, 0)), :==, s(:call, s(:lvar, :pair), :[], s(:lit, 1))), s(:lvar, :distance), s(:call, s(:lvar, :distance), :+, s(:lit, 1)))))