s(:defn, :compute, s(:args, :strand_a, :strand_b), s(:masgn, s(:array, s(:lasgn, :length_a), s(:lasgn, :length_b)), s(:array, s(:call, s(:lvar, :strand_a), :size), s(:call, s(:lvar, :strand_b), :size))), s(:lasgn, :nucleotides_to_check, s(:case, s(:call, s(:lvar, :length_a), :<=>, s(:lvar, :length_b)), s(:when, s(:array, s(:lit, 1)), s(:lvar, :length_b)), s(:when, s(:array, s(:lit, 0), s(:lit, -1)), s(:lvar, :length_a)), nil)), s(:lasgn, :distance, s(:lit, 0)), s(:iter, s(:call, s(:lit, 0), :upto, s(:call, s(:lvar, :nucleotides_to_check), :-, s(:lit, 1))), s(:args, :i), s(:if, s(:call, s(:call, s(:lvar, :strand_a), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :strand_b), :[], s(:lvar, :i))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil)), s(:lvar, :distance))