s(:defn, :compute, s(:args, :strand1, :strand2), s(:lasgn, :distance, s(:lit, 0)), s(:lasgn, :len, s(:call, s(:array, s(:call, s(:lvar, :strand1), :length), s(:call, s(:lvar, :strand2), :length)), :min)), s(:iter, s(:call, s(:dot3, s(:lit, 0), s(:lvar, :len)), :each), s(:args, :i), s(:block, s(:call, nil, :puts, s(:lvar, :i)), s(:if, s(:call, s(:call, s(:lvar, :strand1), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :strand2), :[], s(:lvar, :i))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil))), s(:return, s(:lvar, :distance)))