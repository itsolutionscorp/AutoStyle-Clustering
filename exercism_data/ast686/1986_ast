s(:class, :Hamming, nil, s(:defs, s(:const, :Hamming), :compute, s(:args, :s1, :s2), s(:if, s(:call, s(:call, s(:lvar, :s1), :length), :==, s(:call, s(:lvar, :s2), :length)), nil, s(:call, nil, :raise, s(:str, "strings must have same length"))), s(:iter, s(:call, s(:call, s(:call, s(:lvar, :s1), :chars), :zip, s(:call, s(:lvar, :s2), :chars)), :count), s(:args, :c), s(:call, s(:call, s(:lvar, :c), :[], s(:lit, 0)), :!=, s(:call, s(:lvar, :c), :[], s(:lit, 1))))))