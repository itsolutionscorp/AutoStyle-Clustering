s(:module, :Hamming, s(:defn, :compute, s(:args, :string_a, :string_b), s(:if, s(:call, s(:call, s(:lvar, :string_a), :length), :==, s(:call, s(:lvar, :string_b), :length)), nil, s(:call, nil, :raise, s(:call, s(:const, :ArgumentError), :new, s(:str, "strings must be of equal length")))), s(:lasgn, :paired_chars, s(:call, s(:call, s(:call, s(:lvar, :string_a), :each_char), :lazy), :zip, s(:call, s(:lvar, :string_b), :each_char))), s(:return, s(:iter, s(:call, s(:lvar, :paired_chars), :count), s(:args, :char_a, :char_b), s(:call, s(:lvar, :char_a), :!=, s(:lvar, :char_b))))))