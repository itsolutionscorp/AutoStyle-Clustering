s(:defn, :compute, s(:args, :strand1, :strand2), s(:lasgn, :distance, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :strand1), :each_char), :with_index), s(:args, :c, :i), s(:if, s(:and, s(:call, s(:lvar, :strand2), :[], s(:lvar, :i)), s(:call, s(:call, s(:lvar, :strand2), :[], s(:lvar, :i)), :!=, s(:lvar, :c))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil)), s(:return, s(:lvar, :distance)))