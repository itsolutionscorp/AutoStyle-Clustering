s(:defn, :compute, s(:args, :dna1, :dna2), s(:iter, s(:call, s(:call, s(:call, s(:lvar, :dna1), :chars), :zip, s(:call, s(:lvar, :dna2), :chars)), :inject, s(:lit, 0)), s(:args, :count, :pair), s(:if, s(:call, s(:call, s(:lvar, :pair), :[], s(:lit, 0)), :!=, s(:call, s(:lvar, :pair), :[], s(:lit, 1))), s(:call, s(:lvar, :count), :+, s(:lit, 1)), s(:lvar, :count))))