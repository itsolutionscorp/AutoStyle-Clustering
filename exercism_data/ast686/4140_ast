s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :strand1, :strand2), s(:lasgn, :strand1, s(:call, s(:call, s(:lvar, :strand1), :chars), :to_a)), s(:lasgn, :strand2, s(:call, s(:call, s(:lvar, :strand2), :chars), :to_a)), s(:lasgn, :distance, s(:lit, 0)), s(:while, s(:call, s(:call, s(:lvar, :strand1), :length), :>, s(:lit, 0)), s(:block, s(:lasgn, :char1, s(:call, s(:lvar, :strand1), :shift)), s(:lasgn, :char2, s(:call, s(:lvar, :strand2), :shift)), s(:if, s(:call, s(:lvar, :char1), :!=, s(:lvar, :char2)), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil)), true), s(:return, s(:lvar, :distance))))