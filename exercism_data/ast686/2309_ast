s(:module, :Hamming, s(:defn, :compute, s(:args, :original, :"*descendant"), s(:if, s(:call, s(:call, s(:lvar, :original), :length), :!=, s(:call, s(:lvar, :descendant), :length)), s(:call, nil, :raise, s(:call, nil, :ArgumentError, s(:str, "Sequences must be equal in length."))), nil), s(:iter, s(:call, s(:call, s(:array, s(:splat, s(:lvar, :original))), :zip, s(:lvar, :descendant)), :reduce, s(:lit, 0)), s(:args, :a, s(:masgn, :orig_base, :desc_base)), s(:block, s(:if, s(:call, s(:lvar, :orig_base), :!=, s(:lvar, :desc_base)), s(:lasgn, :a, s(:call, s(:lvar, :a), :+, s(:lit, 1))), nil), s(:lvar, :a)))))