s(:defn, :compute, s(:args, :strand_a, :strand_b), s(:lasgn, :distance, s(:lit, 0)), s(:lasgn, :nucleotides_a, s(:call, s(:lvar, :strand_a), :split, s(:str, ""))), s(:lasgn, :nucleotides_b, s(:call, s(:lvar, :strand_b), :split, s(:str, ""))), s(:iter, s(:call, s(:lvar, :nucleotides_a), :each_with_index), s(:args, :nucleotide_a, :i), s(:if, s(:call, s(:lvar, :nucleotide_a), :==, s(:call, s(:lvar, :nucleotides_b), :[], s(:lvar, :i))), nil, s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))))), s(:lvar, :distance))