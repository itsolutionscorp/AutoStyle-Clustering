s(:defn, :compute, s(:args, :lhs, :rhs), s(:if, s(:call, s(:call, s(:lvar, :lhs), :length), :!=, s(:call, s(:lvar, :rhs), :length)), s(:call, nil, :throw, s(:call, s(:const, :ArgumentError), :new, s(:str, "Arguments of inequal length."))), nil), s(:iter, s(:call, s(:call, s(:call, s(:lvar, :lhs), :length), :times), :count), s(:args, :i), s(:call, s(:call, s(:lvar, :lhs), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :rhs), :[], s(:lvar, :i)))))