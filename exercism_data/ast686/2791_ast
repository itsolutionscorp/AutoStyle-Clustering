s(:defn, :compute, s(:args, :strand_a, :strand_b), s(:if, s(:call, s(:call, s(:lvar, :strand_a), :length), :!=, s(:call, s(:lvar, :strand_b), :length)), s(:call, nil, :fail), nil), s(:lasgn, :difference, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :strand_a), :chars), :each_with_index), s(:args, :letter, :i), s(:if, s(:call, s(:lvar, :letter), :==, s(:call, s(:lvar, :strand_b), :[], s(:lvar, :i))), s(:true), s(:lasgn, :difference, s(:call, s(:lvar, :difference), :+, s(:lit, 1))))), s(:lvar, :difference))