s(:defn, :compute, s(:args, :str_a, :str_b), s(:if, s(:or, s(:call, s(:lvar, :str_a), :!), s(:or, s(:call, s(:lvar, :str_b), :!), s(:call, s(:call, s(:lvar, :str_a), :size), :!=, s(:call, s(:lvar, :str_b), :size)))), s(:call, nil, :raise, s(:call, nil, :ArgumentError, s(:str, "Can compare only two strings of same length"))), nil), s(:lasgn, :diffs, s(:lit, 0)), s(:lasgn, :arr_a, s(:call, s(:lvar, :str_a), :split, s(:str, ""))), s(:iter, s(:call, s(:call, s(:lvar, :arr_a), :each), :with_index), s(:args, :ele, :index), s(:if, s(:call, s(:lvar, :ele), :!=, s(:call, s(:lvar, :str_b), :[], s(:lvar, :index))), s(:lasgn, :diffs, s(:call, s(:lvar, :diffs), :+, s(:lit, 1))), nil)), s(:lvar, :diffs))