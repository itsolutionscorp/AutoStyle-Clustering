s(:defn, :compute, s(:args, :strand1, :strand2), s(:lasgn, :arr1, s(:call, s(:call, s(:lvar, :strand1), :chars), :to_a)), s(:lasgn, :arr2, s(:call, s(:call, s(:lvar, :strand2), :chars), :to_a)), s(:lasgn, :i, s(:lit, 0)), s(:while, s(:call, s(:call, s(:lvar, :arr1), :length), :>, s(:lit, 0)), s(:block, s(:lasgn, :char1, s(:call, s(:lvar, :arr1), :shift)), s(:lasgn, :char2, s(:call, s(:lvar, :arr2), :shift)), s(:if, s(:call, s(:lvar, :char1), :!=, s(:lvar, :char2)), s(:lasgn, :i, s(:call, s(:lvar, :i), :+, s(:lit, 1))), nil)), true), s(:return, s(:lvar, :i)))