s(:defn, :compute, s(:args, :reference_dna, :comparing_dna), s(:lasgn, :difference_nucleic_acid, s(:lit, 0)), s(:lasgn, :source_dna, s(:if, s(:call, s(:call, s(:lvar, :reference_dna), :length), :>, s(:call, s(:lvar, :comparing_dna), :length)), s(:lvar, :comparing_dna), s(:lvar, :reference_dna))), s(:lasgn, :target_dna, s(:if, s(:call, s(:call, s(:lvar, :reference_dna), :length), :>, s(:call, s(:lvar, :comparing_dna), :length)), s(:lvar, :reference_dna), s(:lvar, :comparing_dna))), s(:iter, s(:call, s(:call, s(:call, s(:lvar, :source_dna), :each_char), :to_a), :each_with_index), s(:args, :nucleic_acid, :index), s(:if, s(:call, s(:call, s(:call, s(:call, s(:lvar, :source_dna), :each_char), :to_a), :[], s(:lvar, :index)), :==, s(:call, s(:call, s(:call, s(:lvar, :target_dna), :each_char), :to_a), :[], s(:lvar, :index))), nil, s(:lasgn, :difference_nucleic_acid, s(:call, s(:lvar, :difference_nucleic_acid), :+, s(:lit, 1))))), s(:lvar, :difference_nucleic_acid))