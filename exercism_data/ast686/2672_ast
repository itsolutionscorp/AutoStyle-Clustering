s(:defn, :compute, s(:args, :strand1, :strand2), s(:lasgn, :count, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:call, s(:lvar, :strand1), :downcase), :each_char), :with_index), s(:args, :char, :index), s(:block, s(:lasgn, :second_char, s(:call, s(:call, s(:lvar, :strand2), :downcase), :[], s(:lvar, :index))), s(:if, s(:call, s(:lvar, :second_char), :==, s(:nil)), nil, s(:if, s(:call, s(:lvar, :char), :!=, s(:lvar, :second_char)), s(:lasgn, :count, s(:call, s(:lvar, :count), :+, s(:lit, 1))), nil)))), s(:lvar, :count))