s(:module, :Hamming, s(:defn, :compute, s(:args, :"*strings"), s(:masgn, s(:array, s(:lasgn, :short_string), s(:lasgn, :long_string)), s(:to_ary, s(:call, s(:lvar, :strings), :sort_by, s(:block_pass, s(:lit, :length))))), s(:lasgn, :result, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :short_string), :chars), :each_with_index), s(:args, :item, :index), s(:if, s(:call, s(:call, s(:lvar, :short_string), :[], s(:lvar, :index)), :!=, s(:call, s(:lvar, :long_string), :[], s(:lvar, :index))), s(:lasgn, :result, s(:call, s(:lvar, :result), :+, s(:lit, 1))), nil)), s(:lvar, :result)))