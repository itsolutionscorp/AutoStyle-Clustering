s(:defn, :compute, s(:args, :dna_a, :dna_b), s(:lasgn, :result, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:array, s(:call, s(:lvar, :dna_a), :length), s(:call, s(:lvar, :dna_b), :length)), :min), :times), s(:args, :i), s(:lasgn, :result, s(:call, s(:lvar, :result), :+, s(:if, s(:call, s(:call, s(:lvar, :dna_a), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :dna_b), :[], s(:lvar, :i))), s(:lit, 1), s(:lit, 0))))), s(:return, s(:lvar, :result)))