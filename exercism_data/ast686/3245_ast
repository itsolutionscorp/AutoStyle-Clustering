s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :strand_a, :strand_b), s(:lasgn, :strand_a, s(:call, s(:lvar, :strand_a), :split, s(:str, ""))), s(:lasgn, :strand_b, s(:call, s(:lvar, :strand_b), :split, s(:str, ""))), s(:lasgn, :mutations, s(:lit, 0)), s(:iter, s(:call, s(:lvar, :strand_a), :each_index), s(:args, :index), s(:if, s(:call, s(:call, s(:lvar, :strand_a), :[], s(:lvar, :index)), :!=, s(:call, s(:lvar, :strand_b), :[], s(:lvar, :index))), s(:lasgn, :mutations, s(:call, s(:lvar, :mutations), :+, s(:lit, 1))), nil)), s(:lasgn, :hamming_difference, s(:lvar, :mutations))))