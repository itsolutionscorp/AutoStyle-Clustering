s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :a, :b), s(:lasgn, :hamming_distance, s(:lit, 0)), s(:lasgn, :i, s(:lit, 0)), s(:if, s(:call, s(:call, s(:lvar, :a), :length), :>, s(:call, s(:lvar, :b), :length)), s(:block, s(:lasgn, :c, s(:lvar, :a)), s(:lasgn, :a, s(:lvar, :b)), s(:lasgn, :b, s(:lvar, :c))), nil), s(:while, s(:call, s(:lvar, :i), :<, s(:call, s(:lvar, :a), :length)), s(:block, s(:if, s(:call, s(:call, s(:lvar, :a), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :b), :[], s(:lvar, :i))), s(:lasgn, :hamming_distance, s(:call, s(:lvar, :hamming_distance), :+, s(:lit, 1))), nil), s(:lasgn, :i, s(:call, s(:lvar, :i), :+, s(:lit, 1)))), true), s(:return, s(:lvar, :hamming_distance))))