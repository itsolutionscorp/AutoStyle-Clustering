s(:defn, :compute, s(:args, :strand_1, :strand_2), s(:if, s(:call, s(:call, s(:lvar, :strand_1), :size), :!=, s(:call, s(:lvar, :strand_2), :size)), s(:call, nil, :raise, s(:call, s(:const, :Exception), :new, s(:str, "Strands are not the same length"))), nil), s(:lasgn, :distance, s(:lit, 0)), s(:lasgn, :chars_array_1, s(:call, s(:lvar, :strand_1), :chars)), s(:lasgn, :chars_array_2, s(:call, s(:lvar, :strand_2), :chars)), s(:iter, s(:call, s(:lvar, :chars_array_1), :each_index), s(:args, :i), s(:if, s(:call, s(:call, s(:lvar, :chars_array_1), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :chars_array_2), :[], s(:lvar, :i))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil)), s(:lvar, :distance))