s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :str1, :str2), s(:if, s(:call, s(:lvar, :str1), :==, s(:lvar, :str2)), s(:return, s(:lit, 0)), nil), s(:lasgn, :str1_arr, s(:call, s(:lvar, :str1), :split, s(:str, ""))), s(:lasgn, :str2_arr, s(:call, s(:lvar, :str2), :split, s(:str, ""))), s(:lasgn, :count, s(:lit, 0)), s(:lasgn, :limit, s(:if, s(:call, s(:call, s(:lvar, :str2_arr), :size), :<, s(:call, s(:lvar, :str1_arr), :size)), s(:call, s(:lvar, :str2_arr), :size), s(:call, s(:lvar, :str1_arr), :size))), s(:iter, s(:call, s(:lvar, :limit), :times), s(:args, :i), s(:if, s(:call, s(:call, s(:lvar, :str2_arr), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :str1_arr), :[], s(:lvar, :i))), s(:lasgn, :count, s(:call, s(:lvar, :count), :+, s(:lit, 1))), nil)), s(:return, s(:lvar, :count))))