s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :dna_one, :dna_two, s(:lasgn, :count, s(:lit, 0))), s(:lasgn, :short, s(:if, s(:call, s(:call, s(:lvar, :dna_one), :length), :<=, s(:call, s(:lvar, :dna_two), :length)), s(:lvar, :dna_one), s(:lvar, :dna_two))), s(:lasgn, :long, s(:if, s(:call, s(:lvar, :short), :==, s(:lvar, :dna_one)), s(:lvar, :dna_two), s(:lvar, :dna_one))), s(:iter, s(:call, s(:call, s(:lvar, :short), :each_char), :with_index, s(:lit, 1)), s(:args, :char, :i), s(:if, s(:call, s(:lvar, :char), :!=, s(:call, s(:lvar, :long), :[], s(:call, s(:lvar, :i), :-, s(:lit, 1)))), s(:lasgn, :count, s(:call, s(:lvar, :count), :+, s(:lit, 1))), nil)), s(:lvar, :count)))