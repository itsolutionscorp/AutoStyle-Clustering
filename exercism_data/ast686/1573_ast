s(:defn, :compute, s(:args, :a, :b), s(:lasgn, :a, s(:call, s(:lvar, :a), :split, s(:lit, //))), s(:lasgn, :b, s(:call, s(:lvar, :b), :split, s(:lit, //))), s(:lasgn, :count, s(:lit, 0)), s(:iter, s(:call, s(:lvar, :a), :each_with_index), s(:args, :c, :i), s(:if, s(:call, s(:call, s(:lvar, :b), :[], s(:lvar, :i)), :nil?), nil, s(:if, s(:call, s(:lvar, :c), :!=, s(:call, s(:lvar, :b), :[], s(:lvar, :i))), s(:lasgn, :count, s(:call, s(:lvar, :count), :+, s(:lit, 1))), nil))), s(:lvar, :count))