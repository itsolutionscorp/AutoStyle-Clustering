s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :seq_1, :seq_2), s(:lasgn, :hamm_dist, s(:lit, 0)), s(:lasgn, :seq_1_length, s(:call, s(:lvar, :seq_1), :length)), s(:lasgn, :seq_2_length, s(:call, s(:lvar, :seq_2), :length)), s(:if, s(:call, s(:lvar, :seq_1_length), :!=, s(:lvar, :seq_2_length)), s(:return, s(:str, "Invalid input strings must be of equal length")), s(:if, s(:call, s(:lvar, :seq_2), :==, s(:lvar, :seq_1)), nil, s(:for, s(:dot2, s(:lit, 0), s(:call, s(:lvar, :seq_1_length), :-, s(:lit, 1))), s(:lasgn, :char_pos), s(:if, s(:call, s(:call, s(:lvar, :seq_1), :[], s(:lvar, :char_pos)), :!=, s(:call, s(:lvar, :seq_2), :[], s(:lvar, :char_pos))), s(:lasgn, :hamm_dist, s(:call, s(:lvar, :hamm_dist), :+, s(:lit, 1))), nil)))), s(:return, s(:lvar, :hamm_dist))))