s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :dna1, :dna2), s(:if, s(:call, s(:call, s(:lvar, :dna1), :length), :!=, s(:call, s(:lvar, :dna2), :length)), s(:call, nil, :raise, s(:dstr, "unequal length! ", s(:evstr, s(:call, s(:lvar, :dna1), :length)), s(:str, " chars to "), s(:evstr, s(:call, s(:lvar, :dna2), :length)), s(:str, " chars"))), nil), s(:lasgn, :dist, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :dna1), :chars), :each_with_index), s(:args, :char, :i), s(:if, s(:call, s(:call, s(:lvar, :dna1), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :dna2), :[], s(:lvar, :i))), s(:lasgn, :dist, s(:call, s(:lvar, :dist), :+, s(:lit, 1))), nil)), s(:return, s(:lvar, :dist))))