s(:defn, :compute, s(:args, :strand_1, :strand_2), s(:lasgn, :distance, s(:lit, 0)), s(:lasgn, :length, s(:call, s(:array, s(:call, s(:lvar, :strand_1), :size), s(:call, s(:lvar, :strand_2), :size)), :min)), s(:lasgn, :i, s(:lit, 0)), s(:while, s(:call, s(:lvar, :i), :<, s(:lvar, :length)), s(:block, s(:if, s(:call, s(:call, s(:lvar, :strand_1), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :strand_2), :[], s(:lvar, :i))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil), s(:lasgn, :i, s(:call, s(:lvar, :i), :+, s(:lit, 1)))), true), s(:return, s(:lvar, :distance)))