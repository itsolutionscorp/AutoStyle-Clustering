s(:defn, :compute, s(:args, :word1, :word2), s(:if, s(:call, s(:lvar, :word1), :==, s(:lvar, :word2)), s(:return, s(:lit, 0)), nil), s(:if, s(:call, s(:call, s(:lvar, :word1), :size), :!=, s(:call, s(:lvar, :word2), :size)), s(:call, nil, :raise, s(:const, :ArgumentError), s(:str, "Words must be the same size.")), nil), s(:lasgn, :count, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :word1), :size), :times), s(:args, :i), s(:if, s(:call, s(:call, s(:lvar, :word1), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :word2), :[], s(:lvar, :i))), s(:lasgn, :count, s(:call, s(:lvar, :count), :+, s(:lit, 1))), nil)), s(:lvar, :count))