s(:defn, :compute, s(:args, :strand_a, :strand_b), s(:if, s(:call, s(:call, s(:lvar, :strand_a), :length), :==, s(:call, s(:lvar, :strand_b), :length)), nil, s(:call, nil, :raise, s(:str, "Strands must be similar length"))), s(:lasgn, :a, s(:call, s(:lvar, :strand_a), :bytes)), s(:lasgn, :b, s(:call, s(:lvar, :strand_b), :bytes)), s(:lasgn, :diff, s(:iter, s(:call, s(:call, s(:lvar, :a), :select), :with_index), s(:args, :byte, :index), s(:call, s(:lvar, :byte), :!=, s(:call, s(:lvar, :b), :[], s(:lvar, :index))))), s(:call, s(:lvar, :diff), :count))