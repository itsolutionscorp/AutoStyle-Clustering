s(:defn, :compute, s(:args, :strand1, :strand2), s(:lasgn, :hamming_distance, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :strand1), :chars), :each_with_index), s(:args, :char, :i), s(:block, s(:if, s(:call, s(:lvar, :i), :==, s(:call, s(:lvar, :strand2), :length)), s(:break), nil), s(:if, s(:call, s(:lvar, :char), :==, s(:call, s(:call, s(:lvar, :strand2), :chars), :[], s(:lvar, :i))), nil, s(:lasgn, :hamming_distance, s(:call, s(:lvar, :hamming_distance), :+, s(:lit, 1)))))), s(:lvar, :hamming_distance))