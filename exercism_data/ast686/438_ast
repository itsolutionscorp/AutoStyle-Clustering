s(:defn, :compute, s(:args, :dna_a, :dna_b), s(:lasgn, :hamming, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:array, s(:call, s(:lvar, :dna_a), :size), s(:call, s(:lvar, :dna_b), :size)), :min), :times), s(:args, :i), s(:if, s(:call, s(:call, s(:lvar, :dna_a), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :dna_b), :[], s(:lvar, :i))), s(:lasgn, :hamming, s(:call, s(:lvar, :hamming), :+, s(:lit, 1))), nil)), s(:lvar, :hamming))