s(:defn, :compute, s(:args, :strand_a, :strand_b), s(:lasgn, :hamming, s(:lit, 0)), s(:lasgn, :strand1, s(:call, s(:lvar, :strand_a), :chars)), s(:lasgn, :strand2, s(:call, s(:lvar, :strand_b), :chars)), s(:iter, s(:call, s(:call, s(:lvar, :strand1), :zip, s(:lvar, :strand2)), :each), s(:args, :strands_array), s(:if, s(:call, s(:call, s(:lvar, :strands_array), :first), :!=, s(:call, s(:lvar, :strands_array), :last)), s(:lasgn, :hamming, s(:call, s(:lvar, :hamming), :+, s(:lit, 1))), nil)), s(:lvar, :hamming))