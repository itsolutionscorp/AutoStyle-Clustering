s(:defn, :compute, s(:args, :a, :b), s(:lasgn, :distance, s(:lit, 0)), s(:lasgn, :c, s(:call, s(:call, s(:lvar, :a), :split, s(:lit, //)), :zip, s(:call, s(:lvar, :b), :split, s(:lit, //)))), s(:iter, s(:call, s(:lvar, :c), :each), s(:args, :item1, :item2), s(:block, s(:if, s(:or, s(:call, s(:lvar, :item1), :nil?), s(:call, s(:lvar, :item2), :nil?)), s(:break), nil), s(:if, s(:call, s(:lvar, :item1), :!=, s(:lvar, :item2)), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil))), s(:lvar, :distance))