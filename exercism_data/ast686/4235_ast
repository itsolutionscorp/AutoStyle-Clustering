s(:defn, :compute, s(:args, :dna1, :dna2), s(:lasgn, :length, s(:call, s(:array, s(:call, s(:lvar, :dna1), :length), s(:call, s(:lvar, :dna2), :length)), :min)), s(:lasgn, :distance, s(:lit, 0)), s(:iter, s(:call, s(:lit, 0), :upto, s(:call, s(:lvar, :length), :-, s(:lit, 1))), s(:args, :i), s(:if, s(:call, s(:call, s(:lvar, :dna1), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :dna2), :[], s(:lvar, :i))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil)), s(:return, s(:lvar, :distance)))