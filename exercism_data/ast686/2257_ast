s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :a, :b), s(:if, s(:or, s(:call, s(:call, s(:lvar, :a), :length), :!=, s(:call, s(:lvar, :b), :length)), s(:call, s(:call, s(:lvar, :a), :length), :==, s(:lit, 0))), s(:call, nil, :raise, s(:str, "lengths do not match")), nil), s(:lasgn, :diffs, s(:lit, 0)), s(:lasgn, :zipped, s(:call, s(:call, s(:lvar, :a), :split, s(:str, "")), :zip, s(:call, s(:lvar, :b), :split, s(:str, "")))), s(:iter, s(:call, s(:lvar, :zipped), :each), s(:args, s(:masgn, :a, :b)), s(:if, s(:call, s(:lvar, :a), :!=, s(:lvar, :b)), s(:lasgn, :diffs, s(:call, s(:lvar, :diffs), :+, s(:lit, 1))), nil)), s(:return, s(:lvar, :diffs))))