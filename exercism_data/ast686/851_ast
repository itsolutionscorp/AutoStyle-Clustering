s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :a, :b), s(:if, s(:call, s(:call, s(:lvar, :a), :length), :<, s(:call, s(:lvar, :b), :length)), s(:block, s(:lasgn, :smaller, s(:lvar, :a)), s(:lasgn, :bigger, s(:lvar, :b))), s(:block, s(:lasgn, :smaller, s(:lvar, :b)), s(:lasgn, :bigger, s(:lvar, :a)))), s(:lasgn, :count, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :smaller), :split, s(:str, "")), :each_with_index), s(:args, :s, :i), s(:if, s(:call, s(:lvar, :s), :!=, s(:call, s(:call, s(:lvar, :bigger), :split, s(:str, "")), :[], s(:lvar, :i))), s(:lasgn, :count, s(:call, s(:lvar, :count), :+, s(:lit, 1))), nil)), s(:return, s(:lvar, :count))))