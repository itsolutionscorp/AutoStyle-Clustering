s(:defn, :compute, s(:args, :strand_1, :strand_2), s(:lasgn, :mutations, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :strand_1), :chars), :each_with_index), s(:args, :val, :index), s(:if, s(:call, s(:call, s(:call, s(:lvar, :strand_2), :chars), :[], s(:lvar, :index)), :!=, s(:lvar, :val)), s(:lasgn, :mutations, s(:call, s(:lvar, :mutations), :+, s(:lit, 1))), nil)), s(:lvar, :mutations))