s(:defn, :compute, s(:args, :a, :b), s(:if, s(:call, s(:call, s(:lvar, :a), :length), :!=, s(:call, s(:lvar, :b), :length)), s(:str, "Strings are not of the same length"), nil), s(:lasgn, :distance, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :a), :chars), :each_with_index), s(:args, :char, :idx), s(:if, s(:call, s(:lvar, :char), :!=, s(:call, s(:lvar, :b), :[], s(:lvar, :idx))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil)), s(:lvar, :distance))