s(:defn, :compute, s(:args, :one, :two), s(:lasgn, :distance, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :one), :chars), :each_with_index), s(:args, :item, :i), s(:if, s(:call, s(:lvar, :item), :!=, s(:call, s(:lvar, :two), :[], s(:lvar, :i))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil)), s(:return, s(:lvar, :distance)))