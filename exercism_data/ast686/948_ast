s(:defn, :compute, s(:args, :first, :second), s(:lasgn, :distance, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:call, s(:lvar, :first), :chars), :zip, s(:call, s(:lvar, :second), :chars)), :each), s(:args, :a, :b), s(:if, s(:call, s(:lvar, :a), :==, s(:lvar, :b)), nil, s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))))), s(:lvar, :distance))