s(:defn, :compute, s(:args, :strand1, :strand2), s(:if, s(:call, s(:lvar, :strand1), :==, s(:lvar, :strand2)), s(:return, s(:lit, 0)), nil), s(:lasgn, :dist, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :strand1), :split, s(:str, "")), :each_with_index), s(:args, :n, :i), s(:lasgn, :dist, s(:call, s(:lvar, :dist), :+, s(:call, s(:call, s(:lvar, :n), :<=>, s(:call, s(:call, s(:lvar, :strand2), :split, s(:str, "")), :[], s(:lvar, :i))), :abs)))), s(:lvar, :dist))