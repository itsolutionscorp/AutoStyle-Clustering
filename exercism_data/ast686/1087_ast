s(:defn, :compute, s(:args, :strand1, :strand2), s(:if, s(:call, s(:call, s(:lvar, :strand1), :size), :!=, s(:call, s(:lvar, :strand2), :size)), s(:return, s(:str, "Size mismatch")), nil), s(:lasgn, :diff, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :strand1), :size), :times), s(:args, :position), s(:if, s(:call, s(:call, s(:lvar, :strand1), :[], s(:lvar, :position)), :==, s(:call, s(:lvar, :strand2), :[], s(:lvar, :position))), nil, s(:lasgn, :diff, s(:call, s(:lvar, :diff), :+, s(:lit, 1))))), s(:return, s(:lvar, :diff)))