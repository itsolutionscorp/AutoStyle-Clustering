s(:defn, :compute, s(:args, :first, :second), s(:lasgn, :distance, s(:lit, 0)), s(:lasgn, :imax, s(:if, s(:call, s(:call, s(:lvar, :first), :length), :>, s(:call, s(:lvar, :second), :length)), s(:call, s(:lvar, :second), :length), s(:call, s(:lvar, :first), :length))), s(:for, s(:dot3, s(:lit, 0), s(:lvar, :imax)), s(:lasgn, :i), s(:if, s(:call, s(:call, s(:lvar, :first), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :second), :[], s(:lvar, :i))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil)), s(:return, s(:lvar, :distance)))