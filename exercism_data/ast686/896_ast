s(:defn, :compute, s(:args, :s1, :s2), s(:if, s(:call, s(:call, s(:lvar, :s1), :length), :<, s(:call, s(:lvar, :s2), :length)), s(:lasgn, :numchars, s(:call, s(:lvar, :s1), :length)), s(:lasgn, :numchars, s(:call, s(:lvar, :s2), :length))), s(:lasgn, :num_differences, s(:lit, 0)), s(:lasgn, :index, s(:lit, 0)), s(:iter, s(:call, s(:lvar, :numchars), :times), s(:args), s(:block, s(:lasgn, :c1, s(:call, s(:lvar, :s1), :[], s(:lvar, :index))), s(:lasgn, :c2, s(:call, s(:lvar, :s2), :[], s(:lvar, :index))), s(:if, s(:call, s(:lvar, :c1), :!=, s(:lvar, :c2)), s(:lasgn, :num_differences, s(:call, s(:lvar, :num_differences), :+, s(:lit, 1))), nil), s(:lasgn, :index, s(:call, s(:lvar, :index), :+, s(:lit, 1))))), s(:return, s(:lvar, :num_differences)))