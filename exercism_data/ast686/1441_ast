s(:defn, :compute, s(:args, :a1, :a2), s(:lasgn, :res, s(:lit, 0)), s(:if, s(:call, s(:lvar, :a1), :==, s(:lvar, :a2)), s(:lvar, :res), s(:block, s(:iter, s(:call, s(:lvar, :a1), :each), s(:args, :value, :index), s(:if, s(:call, s(:call, s(:lvar, :a1), :[], s(:lvar, :index)), :!=, s(:call, s(:lvar, :a2), :[], s(:lvar, :index))), s(:lasgn, :res, s(:call, s(:lvar, :res), :+, s(:lit, 1))), nil)), s(:lvar, :res))))