s(:defn, :compute, s(:args, :a, :b), s(:lasgn, :diff, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :a), :each_char), :with_index), s(:args, :char, :i), s(:block, s(:if, s(:call, s(:call, s(:lvar, :b), :[], s(:lvar, :i)), :nil?), s(:break), nil), s(:if, s(:call, s(:lvar, :char), :==, s(:call, s(:lvar, :b), :[], s(:lvar, :i))), nil, s(:lasgn, :diff, s(:call, s(:lvar, :diff), :+, s(:lit, 1)))))), s(:return, s(:lvar, :diff)))