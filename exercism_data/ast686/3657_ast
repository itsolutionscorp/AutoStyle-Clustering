s(:defn, :compute, s(:args, :dna1, :dna2), s(:if, s(:call, s(:call, s(:lvar, :dna1), :length), :>, s(:call, s(:lvar, :dna2), :length)), s(:return, s(:lit, 1)), s(:if, s(:call, s(:call, s(:lvar, :dna2), :length), :>, s(:call, s(:lvar, :dna1), :length)), s(:return, s(:lit, 2)), nil)), s(:lasgn, :cnt, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:call, s(:lvar, :dna1), :each_char), :to_a), :each_with_index), s(:args, :char, :index), s(:if, s(:call, s(:lvar, :char), :!=, s(:call, s(:lvar, :dna2), :[], s(:lvar, :index))), s(:lasgn, :cnt, s(:call, s(:lvar, :cnt), :+, s(:lit, 1))), nil)), s(:lvar, :cnt))