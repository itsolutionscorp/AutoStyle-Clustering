s(:module, :Hamming, s(:defn, :compute, s(:args, :this, :that), s(:lasgn, :char_pairs, s(:call, s(:call, s(:lvar, :this), :chars), :zip, s(:call, s(:lvar, :that), :chars))), s(:lasgn, :distance, s(:lit, 0)), s(:iter, s(:call, s(:lvar, :char_pairs), :each), s(:args, :pair), s(:if, s(:call, s(:call, s(:lvar, :pair), :[], s(:lit, 0)), :!=, s(:call, s(:lvar, :pair), :[], s(:lit, 1))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil)), s(:return, s(:lvar, :distance))))