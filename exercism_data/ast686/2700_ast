s(:defn, :compute, s(:args, :str1, :str2), s(:lasgn, :str1_array, s(:call, s(:lvar, :str1), :split, s(:str, ""))), s(:lasgn, :str2_array, s(:call, s(:lvar, :str2), :split, s(:str, ""))), s(:lasgn, :short_array, s(:lvar, :str1_array)), s(:if, s(:call, s(:call, s(:lvar, :str1_array), :length), :>, s(:call, s(:lvar, :str2_array), :length)), s(:lasgn, :short_array, s(:lvar, :str2_array)), nil), s(:lasgn, :score, s(:lit, 0)), s(:iter, s(:call, s(:lvar, :short_array), :each_index), s(:args, :n), s(:if, s(:call, s(:call, s(:lvar, :str1_array), :[], s(:lvar, :n)), :==, s(:call, s(:lvar, :str2_array), :[], s(:lvar, :n))), nil, s(:lasgn, :score, s(:call, s(:lvar, :score), :+, s(:lit, 1))))), s(:return, s(:lvar, :score)))