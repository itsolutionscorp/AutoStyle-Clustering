s(:class, :Phrase, nil, s(:defn, :initialize, s(:args, s(:lasgn, :input, s(:str, ""))), s(:iasgn, :@phrase, s(:lvar, :input))), s(:defn, :word_count, s(:args), s(:lasgn, :words, s(:call, s(:call, s(:ivar, :@phrase), :downcase), :split, s(:lit, /\W+/))), s(:lasgn, :distinct_words, s(:call, s(:lvar, :words), :uniq)), s(:iasgn, :@counts, s(:call, s(:const, :Hash), :new)), s(:iter, s(:call, s(:lvar, :distinct_words), :each), s(:args, :word), s(:attrasgn, s(:ivar, :@counts), :[]=, s(:lvar, :word), s(:call, s(:lvar, :words), :count, s(:lvar, :word)))), s(:return, s(:ivar, :@counts))))