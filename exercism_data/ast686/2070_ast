s(:defn, :compute, s(:args, :"*strings"), s(:lasgn, :sequences, s(:array)), s(:iter, s(:call, s(:lvar, :strings), :each), s(:args, :string), s(:call, s(:lvar, :sequences), :<<, s(:call, s(:lvar, :string), :split, s(:str, "")))), s(:lasgn, :distance, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:call, s(:lvar, :sequences), :[], s(:lit, 0)), :zip, s(:splat, s(:call, s(:lvar, :sequences), :[], s(:lit, 1..-1)))), :each), s(:args, :"*chars"), s(:if, s(:call, s(:call, s(:call, s(:call, s(:lvar, :chars), :flatten), :uniq), :length), :==, s(:lit, 1)), nil, s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))))), s(:lvar, :distance))