s(:defn, :compute, s(:args, :seg1, :seg2), s(:if, s(:call, s(:call, s(:lvar, :seg1), :length), :==, s(:call, s(:lvar, :seg2), :length)), nil, s(:return, s(:lit, 0))), s(:lasgn, :seg1_array, s(:call, s(:lvar, :seg1), :split, s(:str, ""))), s(:lasgn, :seg2_array, s(:call, s(:lvar, :seg2), :split, s(:str, ""))), s(:lasgn, :strands, s(:call, s(:lvar, :seg1_array), :zip, s(:lvar, :seg2_array))), s(:or, s(:call, s(:call, s(:iter, s(:call, s(:lvar, :strands), :collect), s(:args, :e, :f), s(:if, s(:call, s(:lvar, :e), :!=, s(:lvar, :f)), s(:lit, 1), nil)), :compact), :inject, s(:lit, :+)), s(:lit, 0)))