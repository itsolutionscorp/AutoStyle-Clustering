s(:module, :Hamming, s(:defn, :compute, s(:args, :s1, :s2), s(:lasgn, :distance, s(:lit, 0)), s(:if, s(:call, s(:call, s(:lvar, :s1), :length), :<=, s(:call, s(:lvar, :s2), :length)), s(:block, s(:lasgn, :strand_1, s(:lvar, :s1)), s(:lasgn, :strand_2, s(:lvar, :s2))), s(:block, s(:lasgn, :strand_1, s(:lvar, :s2)), s(:lasgn, :strand_2, s(:lvar, :s1)))), s(:iter, s(:call, s(:call, s(:lvar, :strand_1), :chars), :each_with_index), s(:args, :x1, :i), s(:if, s(:call, s(:lvar, :x1), :!=, s(:call, s(:lvar, :strand_2), :[], s(:lvar, :i))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil)), s(:lvar, :distance)))