s(:defn, :compute, s(:args, :"*strands"), s(:lasgn, :strands, s(:call, s(:lvar, :strands), :map, s(:block_pass, s(:lit, :chars)))), s(:call, s(:lvar, :strands), :sort_by!, s(:block_pass, s(:lit, :length))), s(:lasgn, :diff_count, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:call, s(:lvar, :strands), :[], s(:lit, 0)), :each), :with_index), s(:args, :nucleotide, :index), s(:if, s(:call, s(:lvar, :nucleotide), :!=, s(:call, s(:call, s(:lvar, :strands), :[], s(:lit, 1)), :[], s(:lvar, :index))), s(:lasgn, :diff_count, s(:call, s(:lvar, :diff_count), :+, s(:lit, 1))), nil)), s(:return, s(:lvar, :diff_count)))