s(:defn, :compute, s(:args, :first_strand, :second_strand), s(:if, s(:call, s(:call, s(:lvar, :first_strand), :length), :==, s(:call, s(:lvar, :second_strand), :length)), s(:block, s(:if, s(:call, s(:lvar, :first_strand), :==, s(:lvar, :second_strand)), s(:return, s(:lit, 0)), nil), s(:lasgn, :distance, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:call, s(:lvar, :first_strand), :chars), :zip, s(:call, s(:lvar, :second_strand), :chars)), :each), s(:args, :base_one, :base_two), s(:if, s(:call, s(:lvar, :base_one), :!=, s(:lvar, :base_two)), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil)), s(:return, s(:lvar, :distance))), s(:call, nil, :raise, s(:str, "The strand lengths are not the same."))))