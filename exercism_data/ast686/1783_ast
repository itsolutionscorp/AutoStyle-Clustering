s(:defn, :compute, s(:args, :strand1, :strand2), s(:if, s(:call, s(:lvar, :strand1), :==, s(:lvar, :strand2)), s(:return, s(:lit, 0)), nil), s(:lasgn, :distance, s(:lit, 0)), s(:lasgn, :i, s(:lit, 0)), s(:lasgn, :len, s(:call, s(:array, s(:call, s(:lvar, :strand1), :length), s(:call, s(:lvar, :strand2), :length)), :min)), s(:while, s(:call, s(:lvar, :i), :<, s(:lvar, :len)), s(:block, s(:if, s(:call, s(:call, s(:lvar, :strand1), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :strand2), :[], s(:lvar, :i))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil), s(:lasgn, :i, s(:call, s(:lvar, :i), :+, s(:lit, 1)))), true), s(:return, s(:lvar, :distance)))