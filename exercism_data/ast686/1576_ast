s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :arg1, :arg2), s(:iasgn, :@i, s(:lit, 0)), s(:iasgn, :@ham, s(:lit, 0)), s(:while, s(:or, s(:call, s(:ivar, :@i), :<, s(:call, s(:lvar, :arg1), :length)), s(:call, s(:ivar, :@i), :<, s(:call, s(:lvar, :arg2), :length))), s(:block, s(:if, s(:call, s(:call, s(:lvar, :arg1), :[], s(:ivar, :@i)), :!=, s(:call, s(:lvar, :arg2), :[], s(:ivar, :@i))), s(:iasgn, :@ham, s(:call, s(:ivar, :@ham), :+, s(:lit, 1))), nil), s(:iasgn, :@i, s(:call, s(:ivar, :@i), :+, s(:lit, 1)))), true), s(:return, s(:ivar, :@ham))))