s(:defn, :compute, s(:args, :string1, :string2), s(:lasgn, :first_array, s(:call, s(:lvar, :string1), :split, s(:lit, //))), s(:lasgn, :second_array, s(:call, s(:lvar, :string2), :split, s(:lit, //))), s(:if, s(:call, s(:call, s(:lvar, :first_array), :length), :>, s(:call, s(:lvar, :second_array), :length)), s(:lasgn, :first_array, s(:call, s(:lvar, :first_array), :slice, s(:lit, 0..-2))), s(:if, s(:call, s(:call, s(:lvar, :second_array), :length), :>, s(:call, s(:lvar, :first_array), :length)), s(:lasgn, :second_array, s(:call, s(:lvar, :second_array), :slice, s(:lit, 0..2))), nil)), s(:lvar, :first_array), s(:lvar, :second_array), s(:lasgn, :zipped_array, s(:call, s(:lvar, :first_array), :zip, s(:lvar, :second_array))), s(:lasgn, :distance, s(:lit, 0)), s(:iter, s(:call, s(:lvar, :zipped_array), :each), s(:args, :x), s(:if, s(:call, s(:call, s(:lvar, :x), :[], s(:lit, 0)), :!=, s(:call, s(:lvar, :x), :[], s(:lit, 1))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil)), s(:lvar, :distance))