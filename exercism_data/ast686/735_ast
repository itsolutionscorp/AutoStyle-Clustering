s(:class, :Hamming, nil, s(:defs, s(:const, :Hamming), :compute, s(:args, :a_str, :b_str), s(:lasgn, :count, s(:lit, 0)), s(:lasgn, :a_chars, s(:call, s(:call, s(:lvar, :a_str), :chars), :to_a)), s(:lasgn, :b_chars, s(:call, s(:call, s(:lvar, :b_str), :chars), :to_a)), s(:iter, s(:call, s(:lvar, :a_chars), :each_with_index), s(:args, :a, :i), s(:block, s(:if, s(:call, s(:call, s(:lvar, :b_chars), :[], s(:lvar, :i)), :nil?), s(:break), nil), s(:if, s(:call, s(:lvar, :a), :!=, s(:call, s(:lvar, :b_chars), :[], s(:lvar, :i))), s(:lasgn, :count, s(:call, s(:lvar, :count), :+, s(:lit, 1))), nil))), s(:return, s(:lvar, :count))))