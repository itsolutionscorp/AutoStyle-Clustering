s(:defn, :compute, s(:args, :a, :b), s(:lasgn, :a_chars, s(:call, s(:call, s(:lvar, :a), :chars), :to_a)), s(:lasgn, :b_chars, s(:call, s(:call, s(:lvar, :b), :chars), :to_a)), s(:lasgn, :distance, s(:lit, 0)), s(:iter, s(:call, s(:lvar, :a_chars), :each_with_index), s(:args, :a_char, :i), s(:if, s(:call, s(:lvar, :a_char), :!=, s(:call, s(:lvar, :b_chars), :[], s(:lvar, :i))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil)), s(:lvar, :distance))