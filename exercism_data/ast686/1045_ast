s(:defn, :compute, s(:args, :sequence1, :sequence2), s(:lasgn, :distance, s(:lit, 0)), s(:lasgn, :shortSeq, s(:if, s(:call, s(:call, s(:lvar, :sequence1), :size), :<, s(:call, s(:lvar, :sequence2), :size)), s(:lvar, :sequence1), s(:lvar, :sequence2))), s(:iter, s(:call, s(:call, s(:lvar, :shortSeq), :each_char), :with_index), s(:args, :c, :i), s(:if, s(:call, s(:call, s(:lvar, :sequence1), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :sequence2), :[], s(:lvar, :i))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil)), s(:lvar, :distance))