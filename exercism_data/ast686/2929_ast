s(:defn, :compute, s(:args, :strand_a, :strand_b), s(:lasgn, :count, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :strand_a), :split, s(:str, "")), :each_with_index), s(:args, :nuc_a, :index), s(:block, s(:lasgn, :nuc_b, s(:call, s(:lvar, :strand_b), :[], s(:lvar, :index))), s(:if, s(:call, s(:lvar, :nuc_a), :==, s(:lvar, :nuc_b)), nil, s(:lasgn, :count, s(:call, s(:lvar, :count), :+, s(:lit, 1)))))), s(:lvar, :count))