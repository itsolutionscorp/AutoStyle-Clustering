s(:module, :Hamming, s(:defn, :compute, s(:args, :seq1, :seq2), s(:lasgn, :count, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:call, s(:lvar, :seq1), :chars), :each_with_index), :count), s(:args, :letter, :idx), s(:and, s(:call, s(:lvar, :seq2), :[], s(:lvar, :idx)), s(:call, s(:call, s(:lvar, :seq2), :[], s(:lvar, :idx)), :!=, s(:lvar, :letter))))))