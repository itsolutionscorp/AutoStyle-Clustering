s(:defn, :compute, s(:args, :first, :second), s(:lasgn, :distance, s(:lit, 0)), s(:lasgn, :max, s(:call, s(:call, s(:array, s(:call, s(:lvar, :first), :size), s(:call, s(:lvar, :second), :size)), :min), :-, s(:lit, 1))), s(:iter, s(:call, s(:lit, 0), :upto, s(:lvar, :max)), s(:args, :i), s(:if, s(:call, s(:call, s(:lvar, :first), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :second), :[], s(:lvar, :i))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil)), s(:return, s(:lvar, :distance)))