s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :strand1, :strand2), s(:lasgn, :strand1, s(:call, s(:lvar, :strand1), :split, s(:str, ""))), s(:lasgn, :strand2, s(:call, s(:lvar, :strand2), :split, s(:str, ""))), s(:lasgn, :difference, s(:lit, 0)), s(:lasgn, :i, s(:lit, 0)), s(:while, s(:and, s(:call, s(:lvar, :i), :<, s(:call, s(:lvar, :strand1), :length)), s(:call, s(:lvar, :i), :<, s(:call, s(:lvar, :strand2), :length))), s(:if, s(:call, s(:call, s(:lvar, :strand1), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :strand2), :[], s(:lvar, :i))), s(:block, s(:lasgn, :difference, s(:call, s(:lvar, :difference), :+, s(:lit, 1))), s(:lasgn, :i, s(:call, s(:lvar, :i), :+, s(:lit, 1)))), s(:lasgn, :i, s(:call, s(:lvar, :i), :+, s(:lit, 1)))), true), s(:return, s(:lvar, :difference))))