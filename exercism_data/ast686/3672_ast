s(:defn, :compute, s(:args, :arg1, :arg2), s(:lasgn, :ham, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :arg1), :length), :times), s(:args, :index), s(:if, s(:call, s(:call, s(:lvar, :arg1), :[], s(:lvar, :index)), :!=, s(:call, s(:lvar, :arg2), :[], s(:lvar, :index))), s(:lasgn, :ham, s(:call, s(:lvar, :ham), :+, s(:lit, 1))), nil)), s(:return, s(:lvar, :ham)))