s(:class, :Hamming, nil, s(:sclass, s(:self), s(:defn, :compute, s(:args, :first_strand, :second_strand), s(:call, nil, :puts, s(:str, "\n")), s(:lasgn, :strand1, s(:call, s(:lvar, :first_strand), :chars)), s(:lasgn, :strand2, s(:call, s(:lvar, :second_strand), :chars)), s(:lasgn, :shortest_strand_size, s(:if, s(:call, s(:call, s(:lvar, :strand1), :size), :<=, s(:call, s(:lvar, :strand2), :size)), s(:call, s(:lvar, :strand1), :size), s(:call, s(:lvar, :strand2), :size))), s(:lasgn, :distance, s(:lit, 0)), s(:iter, s(:call, s(:dot3, s(:lit, 0), s(:lvar, :shortest_strand_size)), :each), s(:args, :i), s(:if, s(:call, s(:call, s(:lvar, :strand1), :[], s(:lvar, :i)), :==, s(:call, s(:lvar, :strand2), :[], s(:lvar, :i))), nil, s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))))), s(:lvar, :distance))))