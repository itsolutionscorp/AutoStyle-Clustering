s(:class, :Phrase, nil, s(:defn, :initialize, s(:args, :input_phrase), s(:iasgn, :@input_phrase, s(:lvar, :input_phrase))), s(:defn, :word_count, s(:args), s(:lasgn, :store, s(:call, s(:const, :Hash), :new)), s(:lasgn, :array_of_words, s(:call, s(:ivar, :@input_phrase), :split)), s(:lasgn, :parsed_array_of_words, s(:call, s(:const, :Array), :new)), s(:iter, s(:call, s(:lvar, :array_of_words), :each), s(:args, :word), s(:iter, s(:call, s(:call, s(:lvar, :word), :split, s(:str, ",")), :each), s(:args, :split_word), s(:call, s(:lvar, :parsed_array_of_words), :<<, s(:call, s(:lvar, :split_word), :downcase)))), s(:iter, s(:call, s(:lvar, :parsed_array_of_words), :each), s(:args, :word), s(:block, s(:lasgn, :parsed_word, s(:call, s(:call, s(:lvar, :word), :match, s(:lit, /\w+'?\w?/)), :to_s)), s(:if, s(:call, s(:lvar, :parsed_word), :==, s(:str, "")), s(:next), nil), s(:if, s(:call, s(:call, s(:lvar, :store), :[], s(:lvar, :parsed_word)), :==, s(:nil)), s(:attrasgn, s(:lvar, :store), :[]=, s(:lvar, :parsed_word), s(:lit, 1)), s(:op_asgn1, s(:lvar, :store), s(:arglist, s(:lvar, :parsed_word)), :+, s(:lit, 1))))), s(:lvar, :store)))