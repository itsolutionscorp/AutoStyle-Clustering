s(:defn, :compute, s(:args, :s1, :s2), s(:lasgn, :s1, s(:call, s(:lvar, :s1), :chars)), s(:lasgn, :mutations, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :s2), :chars), :each_with_index), s(:args, :c, :i), s(:block, s(:if, s(:or, s(:call, s(:call, s(:lvar, :s1), :[], s(:lvar, :i)), :==, s(:nil)), s(:call, s(:lvar, :c), :==, s(:nil))), s(:return, s(:lvar, :mutations)), nil), s(:if, s(:call, s(:call, s(:lvar, :s1), :[], s(:lvar, :i)), :!=, s(:lvar, :c)), s(:lasgn, :mutations, s(:call, s(:lvar, :mutations), :+, s(:lit, 1))), nil))), s(:lvar, :mutations))