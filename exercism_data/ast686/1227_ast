s(:defn, :compute, s(:args, :sequence_a, :sequence_b), s(:lasgn, :length, s(:call, s(:array, s(:call, s(:lvar, :sequence_a), :length), s(:call, s(:lvar, :sequence_b), :length)), :min)), s(:lasgn, :distance, s(:lit, 0)), s(:for, s(:dot2, s(:lit, 0), s(:call, s(:lvar, :length), :-, s(:lit, 1))), s(:lasgn, :i), s(:if, s(:call, s(:call, s(:lvar, :sequence_a), :[], s(:lvar, :i)), :==, s(:call, s(:lvar, :sequence_b), :[], s(:lvar, :i))), nil, s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))))), s(:lvar, :distance))