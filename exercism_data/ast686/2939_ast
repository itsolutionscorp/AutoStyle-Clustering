s(:defn, :compute, s(:args, :a, :b), s(:lasgn, :distance, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :a), :split, s(:str, "")), :each_with_index), s(:args, :val, :i), s(:if, s(:and, s(:call, s(:lvar, :b), :[], s(:lvar, :i)), s(:call, s(:lvar, :val), :!=, s(:call, s(:lvar, :b), :[], s(:lvar, :i)))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil)), s(:lvar, :distance))