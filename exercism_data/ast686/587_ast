s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :"*strands"), s(:lasgn, :min_len, s(:call, s(:iter, s(:call, s(:lvar, :strands), :map), s(:args, :s), s(:call, s(:lvar, :s), :length)), :min)), s(:iter, s(:call, s(:lvar, :strands), :map!), s(:args, :s), s(:call, s(:call, s(:lvar, :s), :[], s(:dot2, s(:lit, 0), s(:call, s(:lvar, :min_len), :-, s(:lit, 1)))), :split, s(:str, ""))), s(:lasgn, :num_mismatches, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :strands), :[], s(:lit, 0)), :each_index), s(:args, :i), s(:lasgn, :num_mismatches, s(:call, s(:lvar, :num_mismatches), :+, s(:if, s(:call, s(:call, s(:call, s(:lvar, :strands), :[], s(:lit, 0)), :[], s(:lvar, :i)), :==, s(:call, s(:call, s(:lvar, :strands), :[], s(:lit, 1)), :[], s(:lvar, :i))), s(:lit, 0), s(:lit, 1))))), s(:lvar, :num_mismatches)))