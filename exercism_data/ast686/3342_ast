s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :name1, :name2), s(:call, nil, :puts, s(:dstr, "name1: ", s(:evstr, s(:lvar, :name1)))), s(:call, nil, :puts, s(:dstr, "name2: ", s(:evstr, s(:lvar, :name2)))), s(:if, s(:call, s(:call, s(:lvar, :name1), :length), :==, s(:call, s(:lvar, :name2), :length)), s(:if, s(:call, s(:lvar, :name1), :==, s(:lvar, :name2)), s(:return, s(:lit, 0)), s(:block, s(:lasgn, :c, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :name1), :length), :times), s(:args, :i), s(:if, s(:call, s(:call, s(:lvar, :name1), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :name2), :[], s(:lvar, :i))), s(:lasgn, :c, s(:call, s(:lvar, :c), :+, s(:lit, 1))), nil)), s(:return, s(:lvar, :c)))), s(:call, nil, :puts, s(:str, "The length of the DNA strands is not equivalent")))))