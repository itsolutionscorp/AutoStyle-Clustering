s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :a, :b), s(:lasgn, :length, s(:call, s(:array, s(:call, s(:lvar, :a), :length), s(:call, s(:lvar, :b), :length)), :min)), s(:lasgn, :differences, s(:lit, 0)), s(:for, s(:dot2, s(:lit, 0), s(:lvar, :length)), s(:lasgn, :i), s(:if, s(:call, s(:call, s(:lvar, :a), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :b), :[], s(:lvar, :i))), s(:lasgn, :differences, s(:call, s(:lvar, :differences), :+, s(:lit, 1))), nil)), s(:return, s(:lvar, :differences))))