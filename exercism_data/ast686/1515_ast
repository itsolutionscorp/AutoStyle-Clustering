s(:defn, :compute, s(:args, :first_strand, :second_strand), s(:lasgn, :hamming_distance, s(:lit, 0)), s(:for, s(:dot3, s(:lit, 0), s(:call, s(:lvar, :first_strand), :length)), s(:lasgn, :i), s(:if, s(:call, s(:call, s(:lvar, :first_strand), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :second_strand), :[], s(:lvar, :i))), s(:lasgn, :hamming_distance, s(:call, s(:lvar, :hamming_distance), :+, s(:lit, 1))), nil)), s(:return, s(:lvar, :hamming_distance)))