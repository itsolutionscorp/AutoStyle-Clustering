s(:defn, :compute, s(:args, :first_strand, :second_strand), s(:masgn, s(:array, s(:lasgn, :short_strand), s(:lasgn, :long_strand)), s(:to_ary, s(:iter, s(:call, s(:array, s(:lvar, :first_strand), s(:lvar, :second_strand)), :sort), s(:args, :a, :b), s(:call, s(:call, s(:lvar, :a), :length), :<=>, s(:call, s(:lvar, :b), :length))))), s(:lasgn, :count, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :short_strand), :length), :times), s(:args, :i), s(:if, s(:call, s(:call, s(:lvar, :long_strand), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :short_strand), :[], s(:lvar, :i))), s(:lasgn, :count, s(:call, s(:lvar, :count), :+, s(:lit, 1))), nil)), s(:lvar, :count))