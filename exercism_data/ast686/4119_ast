s(:defn, :compute, s(:args, :dna1, :dna2), s(:lasgn, :distance, s(:lit, 0)), s(:lasgn, :maximum, s(:call, s(:lvar, :dna1), :length)), s(:if, s(:call, s(:call, s(:lvar, :dna1), :length), :>, s(:call, s(:lvar, :dna2), :length)), s(:lasgn, :maximum, s(:call, s(:lvar, :dna2), :length)), nil), s(:iter, s(:call, s(:lvar, :maximum), :times), s(:args, :i), s(:if, s(:call, s(:call, s(:lvar, :dna1), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :dna2), :[], s(:lvar, :i))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil)), s(:return, s(:lvar, :distance)))