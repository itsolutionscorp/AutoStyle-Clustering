s(:block, s(:call, nil, :require, s(:str, "pry")), s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :a, :b), s(:lasgn, :a_array, s(:call, s(:lvar, :a), :split, s(:str, ""))), s(:lasgn, :b_array, s(:call, s(:lvar, :b), :split, s(:str, ""))), s(:lasgn, :shortest_len, s(:if, s(:call, s(:call, s(:lvar, :a_array), :length), :>, s(:call, s(:lvar, :b_array), :length)), s(:call, s(:lvar, :b_array), :length), s(:call, s(:lvar, :a_array), :length))), s(:lasgn, :results, s(:array)), s(:iter, s(:call, s(:lit, 0), :upto, s(:call, s(:lvar, :shortest_len), :-, s(:lit, 1))), s(:args, :i), s(:call, s(:lvar, :results), :<<, s(:if, s(:call, s(:call, s(:lvar, :a_array), :[], s(:lvar, :i)), :==, s(:call, s(:lvar, :b_array), :[], s(:lvar, :i))), s(:true), s(:false)))), s(:call, s(:lvar, :results), :count, s(:false)))))