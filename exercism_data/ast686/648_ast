s(:defn, :compute, s(:args, :strand1, :strand2), s(:lasgn, :difference, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :strand1), :split, s(:str, "")), :each_with_index), s(:args, :nucleotide, :index), s(:if, s(:and, s(:call, s(:lvar, :strand2), :[], s(:lvar, :index)), s(:call, s(:lvar, :nucleotide), :!=, s(:call, s(:lvar, :strand2), :[], s(:lvar, :index)))), s(:lasgn, :difference, s(:call, s(:lvar, :difference), :+, s(:lit, 1))), nil)), s(:return, s(:lvar, :difference)))