s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :x, :y), s(:if, s(:call, s(:call, s(:lvar, :x), :length), :!=, s(:call, s(:lvar, :y), :length)), s(:call, nil, :raise, s(:str, "Arguments must be of equal length.")), nil), s(:iter, s(:call, s(:call, s(:call, s(:lvar, :x), :length), :times), :count), s(:args, :n), s(:call, s(:call, s(:lvar, :x), :[], s(:lvar, :n)), :!=, s(:call, s(:lvar, :y), :[], s(:lvar, :n))))))