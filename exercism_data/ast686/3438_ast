s(:defn, :compute, s(:args, :first_strand, :second_strand), s(:lasgn, :hamming_pt, s(:lit, 0)), s(:lasgn, :i, s(:lit, 0)), s(:lasgn, :min_length, s(:call, s(:array, s(:call, s(:lvar, :first_strand), :length), s(:call, s(:lvar, :second_strand), :length)), :min)), s(:while, s(:call, s(:lvar, :i), :<, s(:lvar, :min_length)), s(:block, s(:if, s(:call, s(:call, s(:lvar, :first_strand), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :second_strand), :[], s(:lvar, :i))), s(:lasgn, :hamming_pt, s(:call, s(:lvar, :hamming_pt), :+, s(:lit, 1))), nil), s(:lasgn, :i, s(:call, s(:lvar, :i), :+, s(:lit, 1)))), true), s(:return, s(:lvar, :hamming_pt)))