s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :s1, :s2), s(:if, s(:call, s(:call, s(:lvar, :s1), :length), :==, s(:call, s(:lvar, :s2), :length)), nil, s(:call, nil, :raise, s(:const, :ArgumentError), s(:str, "Strings must be the same length"))), s(:iter, s(:call, s(:dot2, s(:lit, 0), s(:call, s(:call, s(:lvar, :s1), :length), :-, s(:lit, 1))), :count), s(:args, :x), s(:call, s(:call, s(:lvar, :s1), :[], s(:lvar, :x)), :!=, s(:call, s(:lvar, :s2), :[], s(:lvar, :x))))))