s(:defn, :compute, s(:args, :first_strand, :other_strand), s(:lasgn, :strands, s(:call, s(:array, s(:lvar, :first_strand), s(:lvar, :other_strand)), :sort_by, s(:block_pass, s(:lit, :length)))), s(:masgn, s(:array, s(:lasgn, :short_strand), s(:lasgn, :long_strand)), s(:array, s(:call, s(:lvar, :strands), :first), s(:call, s(:lvar, :strands), :last))), s(:masgn, s(:array, s(:lasgn, :diff), s(:lasgn, :index)), s(:array, s(:lit, 0), s(:lit, 0))), s(:iter, s(:call, s(:lvar, :short_strand), :chars), s(:args, :char), s(:block, s(:if, s(:call, s(:lvar, :char), :!=, s(:call, s(:lvar, :long_strand), :[], s(:lvar, :index))), s(:lasgn, :diff, s(:call, s(:lvar, :diff), :+, s(:lit, 1))), nil), s(:lasgn, :index, s(:call, s(:lvar, :index), :+, s(:lit, 1))))), s(:lvar, :diff))