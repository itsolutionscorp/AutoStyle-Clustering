s(:defn, :compute, s(:args, :a, :b), s(:lasgn, :difference, s(:lit, 0)), s(:lasgn, :min_length, s(:call, s(:array, s(:call, s(:lvar, :a), :length), s(:call, s(:lvar, :b), :length)), :min)), s(:iter, s(:call, s(:lvar, :min_length), :times), s(:args, :n), s(:if, s(:call, s(:call, s(:lvar, :a), :[], s(:lvar, :n)), :!=, s(:call, s(:lvar, :b), :[], s(:lvar, :n))), s(:lasgn, :difference, s(:call, s(:lvar, :difference), :+, s(:lit, 1))), nil)), s(:return, s(:lvar, :difference)))