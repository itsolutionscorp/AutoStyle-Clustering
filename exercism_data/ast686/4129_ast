s(:defn, :compute, s(:args, :arg1, :arg2), s(:lasgn, :array1, s(:call, s(:lvar, :arg1), :split, s(:lit, //))), s(:lasgn, :array2, s(:call, s(:lvar, :arg2), :split, s(:lit, //))), s(:if, s(:call, s(:call, s(:lvar, :array1), :count), :==, s(:call, s(:lvar, :array2), :count)), s(:if, s(:call, s(:lvar, :array1), :==, s(:lvar, :array2)), s(:lit, 0), s(:if, s(:lasgn, :max, s(:call, s(:lvar, :array1), :count)), s(:block, s(:lasgn, :i, s(:lit, 0)), s(:lasgn, :ungleichheiten, s(:lit, 0)), s(:while, s(:call, s(:lvar, :i), :<, s(:lvar, :max)), s(:block, s(:if, s(:call, s(:call, s(:lvar, :array1), :first), :==, s(:call, s(:lvar, :array2), :first)), nil, s(:lasgn, :ungleichheiten, s(:call, s(:lvar, :ungleichheiten), :+, s(:lit, 1)))), s(:call, s(:lvar, :array1), :delete_at, s(:lit, 0)), s(:call, s(:lvar, :array2), :delete_at, s(:lit, 0)), s(:lasgn, :i, s(:call, s(:lvar, :i), :+, s(:lit, 1)))), true), s(:lvar, :ungleichheiten)), nil)), s(:call, nil, :puts, s(:str, "Fehler: Zwei unterschiedlich lange Eingaben"))))