s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :strand1, :strand2), s(:lasgn, :hamming_distance, s(:lit, 0)), s(:lasgn, :longer_strand, s(:lvar, :strand1)), s(:lasgn, :shorter_strand, s(:lvar, :strand2)), s(:if, s(:call, s(:call, s(:lvar, :strand2), :length), :>, s(:call, s(:lvar, :strand1), :length)), s(:block, s(:lasgn, :longer_strand, s(:lvar, :strand2)), s(:lasgn, :shorter_strand, s(:lvar, :strand1))), nil), s(:for, s(:dot2, s(:lit, 0), s(:call, s(:call, s(:lvar, :shorter_strand), :length), :-, s(:lit, 1))), s(:lasgn, :pos), s(:if, s(:call, s(:call, s(:lvar, :shorter_strand), :[], s(:lvar, :pos)), :!=, s(:call, s(:lvar, :longer_strand), :[], s(:lvar, :pos))), s(:lasgn, :hamming_distance, s(:call, s(:lvar, :hamming_distance), :+, s(:lit, 1))), nil)), s(:return, s(:lvar, :hamming_distance))))