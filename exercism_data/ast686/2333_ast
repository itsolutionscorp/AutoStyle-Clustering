s(:defn, :compute, s(:args, :strand_1, :strand_2), s(:lasgn, :mutations, s(:lit, 0)), s(:lasgn, :strand_1_array, s(:call, s(:lvar, :strand_1), :split, s(:str, ""))), s(:lasgn, :strand_2_array, s(:call, s(:lvar, :strand_2), :split, s(:str, ""))), s(:iter, s(:call, s(:lvar, :strand_1_array), :each_with_index), s(:args, :val, :index), s(:if, s(:call, s(:call, s(:lvar, :strand_2_array), :[], s(:lvar, :index)), :!=, s(:lvar, :val)), s(:lasgn, :mutations, s(:call, s(:lvar, :mutations), :+, s(:lit, 1))), nil)), s(:lvar, :mutations))