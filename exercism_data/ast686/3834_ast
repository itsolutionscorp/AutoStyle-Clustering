s(:defn, :compute, s(:args, :a, :b), s(:if, s(:call, s(:call, s(:lvar, :a), :length), :!=, s(:call, s(:lvar, :b), :length)), s(:call, nil, :fail, s(:const, :ArgumentError), s(:str, "Sequences must be the same length")), nil), s(:iter, s(:call, s(:dot3, s(:lit, 0), s(:call, s(:lvar, :a), :length)), :count), s(:args, :i), s(:call, s(:call, s(:lvar, :a), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :b), :[], s(:lvar, :i)))))