s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :strand_a, :strand_b), s(:if, s(:call, s(:call, s(:lvar, :strand_a), :length), :==, s(:call, s(:lvar, :strand_b), :length)), nil, s(:call, nil, :raise, s(:const, :ArgumentError), s(:str, "Strands must be the same length"))), s(:if, s(:call, s(:lvar, :strand_a), :==, s(:lvar, :strand_b)), s(:return, s(:lit, 0)), nil), s(:lasgn, :count, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :strand_a), :each_char), :each_with_index), s(:args, :nucleotide, :index), s(:if, s(:call, s(:lvar, :nucleotide), :!=, s(:call, s(:lvar, :strand_b), :[], s(:lvar, :index))), s(:lasgn, :count, s(:call, s(:lvar, :count), :+, s(:lit, 1))), nil)), s(:lvar, :count)))