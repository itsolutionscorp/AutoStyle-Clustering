s(:defn, :compute, s(:args, :dna1, :dna2), s(:lasgn, :count, s(:lit, 0)), s(:lasgn, :x, s(:call, s(:call, s(:lvar, :dna1), :length), :-, s(:lit, 1))), s(:while, s(:call, s(:lvar, :x), :>=, s(:lit, 0)), s(:block, s(:if, s(:call, s(:call, s(:lvar, :dna1), :[], s(:lvar, :x)), :==, s(:call, s(:lvar, :dna2), :[], s(:lvar, :x))), s(:lvar, :count), s(:lasgn, :count, s(:call, s(:lvar, :count), :+, s(:lit, 1)))), s(:lasgn, :x, s(:call, s(:lvar, :x), :-, s(:lit, 1)))), true), s(:lasgn, :assert_equal, s(:lvar, :count)))