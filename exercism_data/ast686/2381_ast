s(:defn, :compute, s(:args, :string_a, :string_b), s(:lasgn, :distance, s(:lit, 0)), s(:iter, s(:call, s(:lvar, :string_a), :each_char_with_index), s(:args, :char, :i), s(:if, s(:call, s(:lvar, :char), :!=, s(:call, s(:lvar, :string_b), :[], s(:lvar, :i))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil)), s(:lvar, :distance))