s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :strand_1, :strand_2), s(:lasgn, :hamming_distance, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :strand_1), :length), :times), s(:args, :index), s(:block, s(:lasgn, :nucleotide_1, s(:call, s(:lvar, :strand_1), :[], s(:lvar, :index))), s(:lasgn, :nucleotide_2, s(:call, s(:lvar, :strand_2), :[], s(:lvar, :index))), s(:if, s(:call, s(:lvar, :nucleotide_1), :!=, s(:lvar, :nucleotide_2)), s(:lasgn, :hamming_distance, s(:call, s(:lvar, :hamming_distance), :+, s(:lit, 1))), nil))), s(:return, s(:lvar, :hamming_distance))))