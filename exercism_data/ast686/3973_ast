s(:defn, :compute, s(:args, :a, :b), s(:lasgn, :distance, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :a), :each_char), :with_index), s(:args, :char, :index), s(:block, s(:if, s(:call, s(:lvar, :index), :>=, s(:call, s(:lvar, :b), :length)), s(:break), nil), s(:if, s(:call, s(:lvar, :char), :!=, s(:call, s(:lvar, :b), :[], s(:lvar, :index))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil))), s(:lvar, :distance))