s(:defn, :compute, s(:args, :strand_1, :strand_2), s(:lasgn, :strand_1_nucleotides, s(:call, s(:lvar, :strand_1), :chars)), s(:lasgn, :strand_2_nucleotides, s(:call, s(:lvar, :strand_2), :chars)), s(:lasgn, :output, s(:lit, 0)), s(:iter, s(:call, s(:lvar, :strand_1_nucleotides), :each_with_index), s(:args, :char, :index), s(:block, s(:if, s(:call, s(:call, s(:lvar, :strand_2_nucleotides), :[], s(:lvar, :index)), :nil?), s(:break), nil), s(:if, s(:call, s(:lvar, :char), :!=, s(:call, s(:lvar, :strand_2_nucleotides), :[], s(:lvar, :index))), s(:lasgn, :output, s(:call, s(:lvar, :output), :+, s(:lit, 1))), nil))), s(:lvar, :output))