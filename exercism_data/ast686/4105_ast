s(:class, :Hamming, nil, s(:cvdecl, :@@distancia, s(:lit, 0)), s(:defs, s(:const, :Hamming), :compute, s(:args, :primerCodigo, :segundoCodigo), s(:if, s(:call, s(:call, s(:lvar, :primerCodigo), :length), :==, s(:call, s(:lvar, :segundoCodigo), :length)), s(:for, s(:dot2, s(:lit, 0), s(:call, s(:call, s(:lvar, :primerCodigo), :length), :-, s(:lit, 1))), s(:lasgn, :i), s(:if, s(:call, s(:call, s(:lvar, :primerCodigo), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :segundoCodigo), :[], s(:lvar, :i))), s(:cvasgn, :@@distancia, s(:call, s(:cvar, :@@distancia), :+, s(:lit, 1))), nil)), nil), s(:return, s(:cvar, :@@distancia))))