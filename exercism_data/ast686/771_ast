s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :strand1, :strand2), s(:if, s(:call, s(:call, s(:lvar, :strand1), :length), :==, s(:call, s(:lvar, :strand2), :length)), s(:str, "Wrong length. The strands have to be the same length"), nil), s(:lasgn, :splitted_strand_1, s(:call, s(:lvar, :strand1), :scan, s(:lit, /\w/))), s(:lasgn, :splitted_strand_2, s(:call, s(:lvar, :strand2), :scan, s(:lit, /\w/))), s(:lasgn, :count, s(:lit, 0)), s(:iter, s(:call, s(:lvar, :splitted_strand_1), :each_with_index), s(:args, :seq, :i), s(:if, s(:call, s(:call, s(:lvar, :splitted_strand_2), :[], s(:lvar, :i)), :==, s(:lvar, :seq)), nil, s(:lasgn, :count, s(:call, s(:lvar, :count), :+, s(:lit, 1))))), s(:lvar, :count)))