s(:defn, :compute, s(:args, :seq_1, :seq_2), s(:lasgn, :hamm_dist, s(:lit, 0)), s(:lasgn, :seq_1_length, s(:call, s(:lvar, :seq_1), :length)), s(:lasgn, :seq_2_length, s(:call, s(:lvar, :seq_2), :length)), s(:if, s(:call, s(:lvar, :seq_1_length), :!=, s(:lvar, :seq_2_length)), s(:call, nil, :fail, s(:str, "Invalid input sequences must be of equal length")), s(:if, s(:call, s(:lvar, :seq_2), :==, s(:lvar, :seq_1)), nil, s(:iter, s(:call, s(:call, s(:lvar, :seq_1), :each_char), :with_index), s(:args, :char, :index), s(:if, s(:call, s(:lvar, :char), :!=, s(:call, s(:lvar, :seq_2), :[], s(:lvar, :index))), s(:lasgn, :hamm_dist, s(:call, s(:lvar, :hamm_dist), :+, s(:lit, 1))), nil)))), s(:lvar, :hamm_dist))