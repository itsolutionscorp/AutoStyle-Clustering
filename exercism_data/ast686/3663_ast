s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :dna1, :dna2), s(:lasgn, :dna1_chars, s(:call, s(:lvar, :dna1), :chars)), s(:lasgn, :dna2_chars, s(:call, s(:lvar, :dna2), :chars)), s(:if, s(:call, s(:call, s(:lvar, :dna1_chars), :size), :<=, s(:call, s(:lvar, :dna2_chars), :size)), s(:block, s(:lasgn, :shorter, s(:lvar, :dna1_chars)), s(:lasgn, :longer, s(:lvar, :dna2_chars))), s(:block, s(:lasgn, :shorter, s(:lvar, :dna2_chars)), s(:lasgn, :longer, s(:lvar, :dna1_chars)))), s(:lasgn, :result, s(:iter, s(:call, s(:call, s(:lvar, :shorter), :each_with_index), :map), s(:args, :x, :i), s(:if, s(:call, s(:lvar, :x), :==, s(:call, s(:lvar, :longer), :[], s(:lvar, :i))), s(:lit, 0), s(:lit, 1)))), s(:or, s(:iter, s(:call, s(:lvar, :result), :reduce), s(:args, :x, :y), s(:call, s(:lvar, :x), :+, s(:lvar, :y))), s(:lit, 0))))