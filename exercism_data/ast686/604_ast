s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :first_input, :second_input), s(:lasgn, :distance, s(:lit, 0)), s(:if, s(:call, s(:lvar, :first_input), :!=, s(:lvar, :second_input)), s(:block, s(:lasgn, :first_input_array, s(:call, s(:lvar, :first_input), :split, s(:str, ""))), s(:lasgn, :second_input_array, s(:call, s(:lvar, :second_input), :split, s(:str, ""))), s(:iter, s(:call, s(:lvar, :first_input_array), :each_with_index), s(:args, :first_input_char, :index), s(:if, s(:call, s(:lvar, :index), :==, s(:call, s(:lvar, :second_input), :length)), s(:break), s(:if, s(:call, s(:lvar, :first_input_char), :!=, s(:call, s(:lvar, :second_input_array), :[], s(:lvar, :index))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil)))), nil), s(:lvar, :distance)))