s(:defn, :compute, s(:args, :a, :b), s(:lasgn, :shorter, s(:call, s(:call, s(:array, s(:call, s(:lvar, :a), :length), s(:call, s(:lvar, :b), :length)), :min), :-, s(:lit, 1))), s(:lasgn, :a_trimmed, s(:call, s(:lvar, :a), :[], s(:dot2, s(:lit, 0), s(:lvar, :shorter)))), s(:lasgn, :b_trimmed, s(:call, s(:lvar, :b), :[], s(:dot2, s(:lit, 0), s(:lvar, :shorter)))), s(:lasgn, :distance, s(:lit, 0)), s(:for, s(:dot2, s(:lit, 0), s(:lvar, :shorter)), s(:lasgn, :i), s(:if, s(:call, s(:call, s(:lvar, :a), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :b), :[], s(:lvar, :i))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil)), s(:return, s(:lvar, :distance)))