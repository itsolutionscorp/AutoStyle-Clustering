s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :first_dna, :second_dna), s(:lasgn, :acc, s(:lit, 0)), s(:lasgn, :first_arr, s(:if, s(:call, s(:call, s(:lvar, :first_dna), :length), :>, s(:call, s(:lvar, :second_dna), :length)), s(:call, s(:lvar, :second_dna), :split, s(:str, "")), s(:call, s(:lvar, :first_dna), :split, s(:str, "")))), s(:iter, s(:call, s(:lvar, :first_arr), :each_with_index), s(:args, :value, :index), s(:if, s(:call, s(:call, s(:call, s(:lvar, :first_dna), :[], s(:lvar, :index)), :==, s(:call, s(:lvar, :second_dna), :[], s(:lvar, :index))), :!), s(:lasgn, :acc, s(:call, s(:lvar, :acc), :+, s(:lit, 1))), nil)), s(:lvar, :acc)))