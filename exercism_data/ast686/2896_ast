s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :strand1, :strand2), s(:iasgn, :@distance, s(:lit, 0)), s(:lasgn, :strand1Letters, s(:call, s(:lvar, :strand1), :split, s(:str, ""))), s(:lasgn, :strand2Letters, s(:call, s(:lvar, :strand2), :split, s(:str, ""))), s(:iter, s(:call, s(:lvar, :strand1Letters), :each_with_index), s(:args, :letter, :index), s(:if, s(:or, s(:call, s(:lvar, :letter), :==, s(:call, s(:lvar, :strand2Letters), :[], s(:lvar, :index))), s(:call, s(:call, s(:lvar, :strand2Letters), :[], s(:lvar, :index)), :nil?)), s(:iasgn, :@distance, s(:ivar, :@distance)), s(:iasgn, :@distance, s(:call, s(:ivar, :@distance), :+, s(:lit, 1))))), s(:return, s(:ivar, :@distance))))