s(:defn, :compute, s(:args, :strand_one, :strand_two), s(:lasgn, :distance, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :strand_one), :split, s(:str, "")), :each_with_index), s(:args, :code, :index), s(:block, s(:if, s(:call, s(:call, s(:lvar, :strand_two), :[], s(:lvar, :index)), :!=, s(:nil)), nil, s(:break)), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:if, s(:call, s(:lvar, :code), :==, s(:call, s(:lvar, :strand_two), :[], s(:lvar, :index))), s(:lit, 0), s(:lit, 1)))))), s(:lvar, :distance))