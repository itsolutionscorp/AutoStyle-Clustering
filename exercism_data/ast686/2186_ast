s(:defn, :compute, s(:args, :first, :second), s(:lasgn, :distance, s(:lit, 0)), s(:iter, s(:call, s(:dot3, s(:lit, 0), s(:call, s(:lvar, :first), :length)), :each), s(:args, :i), s(:block, s(:if, s(:call, s(:lvar, :i), :==, s(:call, s(:lvar, :second), :length)), s(:break), nil), s(:if, s(:call, s(:call, s(:lvar, :first), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :second), :[], s(:lvar, :i))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil))), s(:lvar, :distance))