s(:class, :HammingTest, nil, s(:defn, :hamming, s(:args, :string1, :string2), s(:lasgn, :string1_letters, s(:call, s(:lvar, :string1), :split, s(:str, ""))), s(:lasgn, :string2_letters, s(:call, s(:lvar, :string2), :split, s(:str, ""))), s(:lasgn, :hamming_distance, s(:lit, 0)), s(:if, s(:call, s(:call, s(:lvar, :string1_letters), :length), :==, s(:call, s(:lvar, :string2_letters), :length)), s(:block, s(:lasgn, :count, s(:lit, 0)), s(:while, s(:call, s(:lvar, :count), :<, s(:call, s(:lvar, :string1_letters), :length)), s(:block, s(:if, s(:call, s(:call, s(:lvar, :string1_letters), :[], s(:lvar, :count)), :!=, s(:call, s(:lvar, :string2_letters), :[], s(:lvar, :count))), s(:lasgn, :hamming_distance, s(:call, s(:lvar, :hamming_distance), :+, s(:lit, 1))), nil), s(:lasgn, :count, s(:call, s(:lvar, :count), :+, s(:lit, 1)))), true)), nil), s(:call, nil, :puts, s(:dstr, "The hamming distance of these two sequences is: ", s(:evstr, s(:lvar, :hamming_distance))))))