s(:defn, :compute, s(:args, :word_one, :word_two), s(:if, s(:call, s(:call, s(:lvar, :word_one), :size), :==, s(:call, s(:lvar, :word_two), :size)), s(:block, s(:lasgn, :sum, s(:lit, 0)), s(:lasgn, :position, s(:lit, 0)), s(:iter, s(:call, s(:lvar, :word_one), :each_char), s(:args, :a_letter), s(:block, s(:if, s(:call, s(:lvar, :a_letter), :!=, s(:call, s(:lvar, :word_two), :[], s(:lvar, :position))), s(:lasgn, :sum, s(:call, s(:lvar, :sum), :+, s(:lit, 1))), nil), s(:lasgn, :position, s(:call, s(:lvar, :position), :+, s(:lit, 1))))), s(:lvar, :sum)), s(:call, nil, :puts, s(:str, "Error string needs to be the same length"))))