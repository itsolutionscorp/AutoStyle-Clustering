s(:defn, :compute, s(:args, :a, :b), s(:masgn, s(:array, s(:lasgn, :first), s(:lasgn, :second)), s(:to_ary, s(:iter, s(:call, s(:array, s(:lvar, :a), s(:lvar, :b)), :sort), s(:args, :x, :y), s(:call, s(:call, s(:lvar, :x), :length), :<=>, s(:call, s(:lvar, :y), :length))))), s(:return, s(:iter, s(:call, s(:call, s(:call, s(:lvar, :first), :split, s(:str, "")), :each_with_index), :inject, s(:lit, 0)), s(:args, :r, s(:masgn, :e, :index)), s(:if, s(:call, s(:lvar, :e), :==, s(:call, s(:lvar, :second), :[], s(:lvar, :index))), s(:lvar, :r), s(:call, s(:lvar, :r), :+, s(:lit, 1))))))