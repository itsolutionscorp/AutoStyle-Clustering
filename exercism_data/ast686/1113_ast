s(:defn, :compute, s(:args, :strand1, :strand2), s(:iasgn, :@diff_count, s(:lit, 0)), s(:if, s(:call, s(:call, s(:lvar, :strand1), :length), :===, s(:call, s(:lvar, :strand2), :length)), s(:iter, s(:call, s(:call, s(:array, s(:call, s(:lvar, :strand1), :chars), s(:call, s(:lvar, :strand2), :chars)), :transpose), :each), s(:args, :char1, :char2), s(:if, s(:call, s(:lvar, :char1), :!=, s(:lvar, :char2)), s(:iasgn, :@diff_count, s(:call, s(:ivar, :@diff_count), :+, s(:lit, 1))), nil)), nil), s(:ivar, :@diff_count))