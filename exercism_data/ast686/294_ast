s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :l, :r), s(:lasgn, :minlen, s(:call, s(:array, s(:call, s(:lvar, :l), :length), s(:call, s(:lvar, :r), :length)), :min)), s(:lasgn, :odds, s(:lit, 0)), s(:iter, s(:call, s(:lvar, :minlen), :times), s(:args, :i), s(:if, s(:call, s(:call, s(:lvar, :l), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :r), :[], s(:lvar, :i))), s(:lasgn, :odds, s(:call, s(:lvar, :odds), :+, s(:lit, 1))), nil)), s(:return, s(:call, nil, :count))))