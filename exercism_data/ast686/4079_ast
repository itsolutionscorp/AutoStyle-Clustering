s(:defn, :compute, s(:args, :strand_a, :strand_b), s(:lasgn, :strand_a, s(:call, s(:lvar, :strand_a), :chars)), s(:lasgn, :strand_b, s(:call, s(:lvar, :strand_b), :chars)), s(:lasgn, :mutations, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :strand_a), :zip, s(:lvar, :strand_b)), :each), s(:args, :nucleotid_a, :nucleotid_b), s(:if, s(:call, s(:lvar, :nucleotid_a), :!=, s(:lvar, :nucleotid_b)), s(:lasgn, :mutations, s(:call, s(:lvar, :mutations), :+, s(:lit, 1))), nil)), s(:lvar, :mutations))