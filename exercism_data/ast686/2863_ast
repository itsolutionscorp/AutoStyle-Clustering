s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :strand_1, :strand_2), s(:lasgn, :counter, s(:lit, 0)), s(:lasgn, :strand_1_array, s(:call, s(:lvar, :strand_1), :split, s(:str, ""))), s(:lasgn, :strand_2_array, s(:call, s(:lvar, :strand_2), :split, s(:str, ""))), s(:if, s(:call, s(:call, s(:lvar, :strand_1_array), :length), :<=, s(:call, s(:lvar, :strand_2_array), :length)), s(:block, s(:lasgn, :shorter_array, s(:lvar, :strand_1_array)), s(:lasgn, :longer_array, s(:lvar, :strand_2_array))), s(:block, s(:lasgn, :shorter_array, s(:lvar, :strand_2_array)), s(:lasgn, :longer_array, s(:lvar, :strand_1_array)))), s(:iter, s(:call, s(:lvar, :shorter_array), :each_with_index), s(:args, :letter, :index), s(:if, s(:call, s(:lvar, :letter), :!=, s(:call, s(:lvar, :longer_array), :[], s(:lvar, :index))), s(:lasgn, :counter, s(:call, s(:lvar, :counter), :+, s(:lit, 1))), nil)), s(:lvar, :counter)))