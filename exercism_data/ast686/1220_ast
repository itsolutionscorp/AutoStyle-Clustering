s(:defn, :compute, s(:args, :s1, :s2), s(:lasgn, :distance, s(:lit, 0)), s(:lasgn, :zipped, s(:call, s(:call, s(:lvar, :s1), :chars), :zip, s(:call, s(:lvar, :s2), :chars))), s(:iter, s(:call, s(:lvar, :zipped), :each), s(:args, :i), s(:block, s(:if, s(:or, s(:call, s(:call, s(:lvar, :i), :[], s(:lit, 0)), :nil?), s(:call, s(:call, s(:lvar, :i), :[], s(:lit, 1)), :nil?)), s(:break), nil), s(:if, s(:call, s(:call, s(:lvar, :i), :[], s(:lit, 0)), :!=, s(:call, s(:lvar, :i), :[], s(:lit, 1))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil))), s(:return, s(:lvar, :distance)))