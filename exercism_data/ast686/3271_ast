s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :strand_a, :strand_b), s(:lasgn, :min_l, s(:call, s(:array, s(:call, s(:lvar, :strand_a), :length), s(:call, s(:lvar, :strand_b), :length)), :min)), s(:lasgn, :a_list, s(:call, s(:call, s(:lvar, :strand_a), :slice, s(:lit, 0), s(:lvar, :min_l)), :split, s(:str, ""))), s(:lasgn, :b_list, s(:call, s(:call, s(:lvar, :strand_b), :slice, s(:lit, 0), s(:lvar, :min_l)), :split, s(:str, ""))), s(:lasgn, :hamming_distance, s(:lit, 0)), s(:iter, s(:call, s(:lvar, :min_l), :times), s(:args, :i), s(:if, s(:call, s(:call, s(:lvar, :a_list), :[], s(:lvar, :i)), :==, s(:call, s(:lvar, :b_list), :[], s(:lvar, :i))), nil, s(:lasgn, :hamming_distance, s(:call, s(:lvar, :hamming_distance), :+, s(:lit, 1))))), s(:lvar, :hamming_distance)))