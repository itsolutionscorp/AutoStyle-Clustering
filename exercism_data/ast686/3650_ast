s(:defn, :compute, s(:args, :first_strand, :second_strand), s(:lasgn, :distance, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :first_strand), :size), :times), s(:args, :position), s(:if, s(:call, s(:call, s(:lvar, :first_strand), :[], s(:lvar, :position)), :==, s(:call, s(:lvar, :second_strand), :[], s(:lvar, :position))), nil, s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))))), s(:lvar, :distance))