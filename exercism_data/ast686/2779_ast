s(:defn, :compute, s(:args, :strand1, :strand2), s(:lasgn, :hamming, s(:lit, 0)), s(:lasgn, :i, s(:lit, 0)), s(:lasgn, :array1, s(:call, s(:lvar, :strand1), :split, s(:str, ""))), s(:lasgn, :array2, s(:call, s(:lvar, :strand2), :split, s(:str, ""))), s(:while, s(:and, s(:call, s(:lvar, :i), :<, s(:call, s(:lvar, :array1), :length)), s(:call, s(:lvar, :i), :<, s(:call, s(:lvar, :array2), :length))), s(:block, s(:if, s(:call, s(:call, s(:lvar, :array1), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :array2), :[], s(:lvar, :i))), s(:lasgn, :hamming, s(:call, s(:lvar, :hamming), :+, s(:lit, 1))), nil), s(:lasgn, :i, s(:call, s(:lvar, :i), :+, s(:lit, 1)))), true), s(:return, s(:lvar, :hamming)))