s(:defn, :compute, s(:args, :arg1, :arg2), s(:lasgn, :array1, s(:call, s(:lvar, :arg1), :split, s(:lit, //))), s(:lasgn, :array2, s(:call, s(:lvar, :arg2), :split, s(:lit, //))), s(:lasgn, :outputs, s(:array)), s(:lasgn, :super_array, s(:call, s(:lvar, :array1), :zip, s(:lvar, :array2))), s(:iter, s(:call, s(:lvar, :super_array), :each), s(:args, :set), s(:if, s(:call, s(:call, s(:lvar, :set), :first), :==, s(:call, s(:lvar, :set), :last)), s(:call, s(:lvar, :outputs), :<<, s(:lit, 0)), s(:call, s(:lvar, :outputs), :<<, s(:lit, 1)))), s(:lasgn, :sum, s(:lit, 0)), s(:iter, s(:call, s(:lvar, :outputs), :each), s(:args, :i), s(:lasgn, :sum, s(:call, s(:lvar, :sum), :+, s(:lvar, :i)))), s(:lvar, :sum))