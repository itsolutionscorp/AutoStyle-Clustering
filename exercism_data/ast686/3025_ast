s(:defn, :compute, s(:args, :seg_a, :seg_b), s(:lasgn, :count, s(:lit, 0)), s(:lasgn, :iter_times, s(:if, s(:call, s(:call, s(:lvar, :seg_a), :length), :<=, s(:call, s(:lvar, :seg_b), :length)), s(:call, s(:lvar, :seg_a), :length), s(:call, s(:lvar, :seg_b), :length))), s(:iter, s(:call, s(:lvar, :iter_times), :times), s(:args, :index), s(:if, s(:call, s(:call, s(:lvar, :seg_a), :[], s(:lvar, :index)), :!=, s(:call, s(:lvar, :seg_b), :[], s(:lvar, :index))), s(:lasgn, :count, s(:call, s(:lvar, :count), :+, s(:lit, 1))), nil)), s(:lvar, :count))