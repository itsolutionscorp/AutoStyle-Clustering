s(:defn, :compute, s(:args, :a, :b), s(:lasgn, :differences, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :a), :chars), :each_with_index), s(:args, :char, :index), s(:if, s(:or, s(:call, s(:lvar, :char), :==, s(:call, s(:lvar, :b), :[], s(:lvar, :index))), s(:call, s(:call, s(:lvar, :b), :[], s(:lvar, :index)), :nil?)), nil, s(:lasgn, :differences, s(:call, s(:lvar, :differences), :+, s(:lit, 1))))), s(:lvar, :differences))