s(:class, :Hamming, nil, s(:class, :Error, s(:const, :StandardError)), s(:class, :ArgumentError, s(:const, :StandardError)), s(:defn, :compute, s(:args, :a, :b), s(:if, s(:call, s(:call, s(:lvar, :a), :length), :==, s(:call, s(:lvar, :b), :length)), nil, s(:call, nil, :raise, s(:const, :ArgumentError), s(:dstr, "Expected strands of the same length, got ", s(:evstr, s(:lvar, :a)), s(:str, " and "), s(:evstr, s(:lvar, :b))))), s(:call, s(:iter, s(:call, s(:iter, s(:call, s(:call, s(:call, s(:lvar, :a), :split, s(:str, "")), :each_with_index), :map), s(:args, :_, :i), s(:call, s(:call, s(:lvar, :a), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :b), :[], s(:lvar, :i)))), :select), s(:args, :n), s(:lvar, :n)), :length)))