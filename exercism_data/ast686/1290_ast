s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :strand1, :strand2), s(:lasgn, :length_to_compare, s(:call, s(:array, s(:call, s(:lvar, :strand1), :length), s(:call, s(:lvar, :strand2), :length)), :min)), s(:lasgn, :bases1, s(:call, s(:call, s(:lvar, :strand1), :[], s(:dot3, s(:lit, 0), s(:lvar, :length_to_compare))), :chars)), s(:lasgn, :bases2, s(:call, s(:call, s(:lvar, :strand2), :[], s(:dot3, s(:lit, 0), s(:lvar, :length_to_compare))), :chars)), s(:iter, s(:call, s(:call, s(:lvar, :bases1), :zip, s(:lvar, :bases2)), :count), s(:args, :base1, :base2), s(:call, s(:lvar, :base1), :!=, s(:lvar, :base2)))))