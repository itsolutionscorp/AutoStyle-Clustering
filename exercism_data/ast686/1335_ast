s(:defn, :compute, s(:args, :x, :y), s(:lasgn, :a, s(:call, s(:lvar, :x), :chars)), s(:lasgn, :b, s(:call, s(:lvar, :y), :chars)), s(:lasgn, :count, s(:lit, 0)), s(:iter, s(:call, s(:lvar, :a), :each_with_index), s(:args, :v, :i), s(:if, s(:call, s(:lvar, :v), :eql?, s(:call, s(:lvar, :b), :[], s(:lvar, :i))), nil, s(:lasgn, :count, s(:call, s(:lvar, :count), :+, s(:lit, 1))))), s(:lvar, :count))