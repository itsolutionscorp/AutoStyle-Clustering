s(:defn, :compute, s(:args, :strand1, :strand2), s(:lasgn, :min_strand_length, s(:call, s(:array, s(:call, s(:lvar, :strand1), :length), s(:call, s(:lvar, :strand2), :length)), :min)), s(:lasgn, :strand1_nucleotides, s(:call, s(:call, s(:lvar, :strand1), :[], s(:dot3, s(:lit, 0), s(:lvar, :min_strand_length))), :chars)), s(:iter, s(:call, s(:dot3, s(:lit, 0), s(:lvar, :min_strand_length)), :inject, s(:lit, 0)), s(:args, :distance, :index), s(:if, s(:call, s(:call, s(:lvar, :strand1), :[], s(:lvar, :index)), :!=, s(:call, s(:lvar, :strand2), :[], s(:lvar, :index))), s(:call, s(:lvar, :distance), :+, s(:lit, 1)), s(:lvar, :distance))))