s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :a, :b), s(:lasgn, :a, s(:call, s(:lvar, :a), :chars)), s(:lasgn, :b, s(:call, s(:lvar, :b), :chars)), s(:lasgn, :max, s(:if, s(:call, s(:call, s(:lvar, :a), :count), :<, s(:call, s(:lvar, :b), :count)), s(:call, s(:lvar, :a), :count), s(:call, s(:lvar, :b), :count))), s(:lasgn, :a, s(:call, s(:lvar, :a), :[], s(:lit, 0), s(:lvar, :max))), s(:lasgn, :b, s(:call, s(:lvar, :b), :[], s(:lit, 0), s(:lvar, :max))), s(:lasgn, :y, s(:call, s(:lvar, :a), :zip, s(:lvar, :b))), s(:iter, s(:call, s(:lvar, :y), :inject, s(:lit, 0)), s(:args, :ret, :h), s(:block, s(:if, s(:call, s(:call, s(:lvar, :h), :first), :!=, s(:call, s(:lvar, :h), :last)), s(:lasgn, :ret, s(:call, s(:lvar, :ret), :+, s(:lit, 1))), nil), s(:lvar, :ret)))))