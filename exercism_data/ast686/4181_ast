s(:defn, :compute, s(:args, :first, :second), s(:lasgn, :count, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:call, s(:lvar, :first), :chars), :to_a), :each_with_index), s(:args, :c, :i), s(:if, s(:call, s(:lvar, :c), :==, s(:call, s(:lvar, :second), :[], s(:lvar, :i))), nil, s(:lasgn, :count, s(:call, s(:lvar, :count), :+, s(:lit, 1))))), s(:lvar, :count))