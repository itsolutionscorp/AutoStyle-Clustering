s(:defn, :compute, s(:args, :strand1, :strand2), s(:lasgn, :difference, s(:lit, 0)), s(:lasgn, :strand1, s(:call, s(:lvar, :strand1), :split, s(:str, ""))), s(:lasgn, :strand2, s(:call, s(:lvar, :strand2), :split, s(:str, ""))), s(:while, s(:and, s(:lasgn, :base1, s(:call, s(:lvar, :strand1), :shift)), s(:lasgn, :base2, s(:call, s(:lvar, :strand2), :shift))), s(:if, s(:call, s(:lvar, :base1), :!=, s(:lvar, :base2)), s(:lasgn, :difference, s(:call, s(:lvar, :difference), :+, s(:lit, 1))), nil), true), s(:return, s(:lvar, :difference)))