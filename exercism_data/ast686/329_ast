s(:defn, :compute, s(:args, :strand_1, :strand_2), s(:lasgn, :distance, s(:lit, 0)), s(:lasgn, :strand_length, s(:call, s(:lvar, :strand_1), :length)), s(:iter, s(:call, s(:lvar, :strand_length), :times), s(:args, :i), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:if, s(:call, s(:call, s(:lvar, :strand_1), :[], s(:lvar, :i)), :==, s(:call, s(:lvar, :strand_2), :[], s(:lvar, :i))), s(:lit, 0), s(:lit, 1))))), s(:lvar, :distance))