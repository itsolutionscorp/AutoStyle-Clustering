s(:defn, :compute, s(:args, :dna_strand_1, :dna_strand_2), s(:if, s(:call, s(:call, s(:lvar, :dna_strand_1), :length), :==, s(:call, s(:lvar, :dna_strand_2), :length)), nil, s(:call, nil, :fail, s(:const, :ArgumentError), s(:str, "strands are not equal size"))), s(:lasgn, :first_strand, s(:call, s(:lvar, :dna_strand_1), :chars)), s(:lasgn, :second_strand, s(:call, s(:lvar, :dna_strand_2), :chars)), s(:iter, s(:call, s(:call, s(:lvar, :first_strand), :zip, s(:lvar, :second_strand)), :count), s(:args, :a, :b), s(:call, s(:lvar, :a), :!=, s(:lvar, :b))))