s(:defn, :compute, s(:args, :strand_a, :strand_b), s(:lasgn, :strand_b, s(:call, s(:lvar, :strand_b), :split, s(:str, ""))), s(:iter, s(:call, s(:call, s(:call, s(:lvar, :strand_a), :split, s(:str, "")), :each_with_index), :inject, s(:lit, 0)), s(:args, :sum, s(:masgn, :value, :index)), s(:block, s(:if, s(:call, s(:call, s(:lvar, :strand_b), :[], s(:lvar, :index)), :!), s(:return, s(:lvar, :sum)), nil), s(:if, s(:call, s(:lvar, :value), :!=, s(:call, s(:lvar, :strand_b), :[], s(:lvar, :index))), s(:lasgn, :sum, s(:call, s(:lvar, :sum), :+, s(:lit, 1))), nil), s(:lvar, :sum))))