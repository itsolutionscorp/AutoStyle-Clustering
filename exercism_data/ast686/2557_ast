s(:defn, :compute, s(:args, :strand_one, :strand_two), s(:lasgn, :strands, s(:call, s(:array, s(:lvar, :strand_one), s(:lvar, :strand_two)), :sort_by!, s(:block_pass, s(:lit, :length)))), s(:iter, s(:call, s(:dot2, s(:lit, 0), s(:call, s(:call, s(:call, s(:lvar, :strands), :first), :length), :-, s(:lit, 1))), :inject, s(:lit, 0)), s(:args, :dif, :index), s(:if, s(:call, s(:call, s(:call, s(:lvar, :strands), :first), :[], s(:lvar, :index)), :!=, s(:call, s(:call, s(:lvar, :strands), :last), :[], s(:lvar, :index))), s(:call, s(:lvar, :dif), :+, s(:lit, 1)), s(:lvar, :dif))))