s(:defn, :compute, s(:args, :stringA, :stringB), s(:lasgn, :hamming, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :stringA), :split, s(:str, "")), :each_with_index), s(:args, :char, :index), s(:block, s(:if, s(:call, s(:call, s(:lvar, :stringB), :[], s(:lvar, :index)), :nil?), s(:return, s(:lvar, :hamming)), nil), s(:if, s(:call, s(:lvar, :char), :!=, s(:call, s(:lvar, :stringB), :[], s(:lvar, :index))), s(:lasgn, :hamming, s(:call, s(:lvar, :hamming), :+, s(:lit, 1))), nil))), s(:return, s(:lvar, :hamming)))