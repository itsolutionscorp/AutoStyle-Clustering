s(:defn, :compute, s(:args, :a, :b), s(:lasgn, :min_length, s(:call, s(:array, s(:call, s(:lvar, :a), :length), s(:call, s(:lvar, :b), :length)), :min)), s(:lasgn, :max_length, s(:call, s(:array, s(:call, s(:lvar, :a), :length), s(:call, s(:lvar, :b), :length)), :max)), s(:lasgn, :start_differences, s(:call, s(:iter, s(:call, s(:dot2, s(:lit, 0), s(:lvar, :min_length)), :find_all), s(:args, :i), s(:call, s(:call, s(:lvar, :a), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :b), :[], s(:lvar, :i)))), :length)), s(:return, s(:call, s(:call, s(:lvar, :max_length), :-, s(:lvar, :min_length)), :+, s(:lvar, :start_differences))))