s(:defn, :compute, s(:args, :strand1, :strand2), s(:lasgn, :i, s(:lit, 0)), s(:lasgn, :hamming_distance, s(:lit, 0)), s(:lasgn, :length, s(:if, s(:call, s(:call, s(:lvar, :strand1), :length), :<, s(:call, s(:lvar, :strand2), :length)), s(:call, s(:lvar, :strand1), :length), s(:call, s(:lvar, :strand2), :length))), s(:while, s(:call, s(:lvar, :i), :<, s(:lvar, :length)), s(:block, s(:lasgn, :hamming_distance, s(:call, s(:lvar, :hamming_distance), :+, s(:if, s(:call, s(:call, s(:lvar, :strand1), :[], s(:lvar, :i)), :==, s(:call, s(:lvar, :strand2), :[], s(:lvar, :i))), s(:lit, 0), s(:lit, 1)))), s(:lasgn, :i, s(:call, s(:lvar, :i), :+, s(:lit, 1)))), true), s(:return, s(:lvar, :hamming_distance)))