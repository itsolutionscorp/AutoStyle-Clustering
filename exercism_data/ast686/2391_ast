s(:defn, :compute, s(:args, :base_strand, :descendent_strand), s(:lasgn, :base_sequence, s(:call, s(:lvar, :base_strand), :chars)), s(:lasgn, :descendent_sequence, s(:call, s(:lvar, :descendent_strand), :chars)), s(:lasgn, :delta, s(:lit, 0)), s(:iter, s(:call, s(:lvar, :base_sequence), :each_with_index), s(:args, :nucleotide, :index), s(:block, s(:if, s(:call, s(:call, s(:lvar, :descendent_sequence), :[], s(:lvar, :index)), :!), s(:next), nil), s(:if, s(:call, s(:lvar, :nucleotide), :!=, s(:call, s(:lvar, :descendent_sequence), :[], s(:lvar, :index))), s(:lasgn, :delta, s(:call, s(:lvar, :delta), :+, s(:lit, 1))), nil))), s(:return, s(:lvar, :delta)))