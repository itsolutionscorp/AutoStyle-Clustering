s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :strand1, :strand2), s(:if, s(:call, s(:call, s(:lvar, :strand1), :length), :==, s(:call, s(:lvar, :strand2), :length)), s(:lasgn, :chars_to_count, s(:call, s(:lvar, :strand1), :length)), s(:if, s(:call, s(:call, s(:lvar, :strand1), :length), :<, s(:call, s(:lvar, :strand2), :length)), s(:lasgn, :chars_to_count, s(:call, s(:lvar, :strand1), :length)), s(:lasgn, :chars_to_count, s(:call, s(:lvar, :strand2), :length)))), s(:lasgn, :hamming, s(:lit, 0)), s(:iter, s(:call, s(:lvar, :chars_to_count), :times), s(:args, :n), s(:if, s(:call, s(:call, s(:lvar, :strand1), :[], s(:lvar, :n)), :!=, s(:call, s(:lvar, :strand2), :[], s(:lvar, :n))), s(:lasgn, :hamming, s(:call, s(:lvar, :hamming), :+, s(:lit, 1))), nil)), s(:lvar, :hamming)))