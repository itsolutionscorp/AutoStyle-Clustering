s(:defn, :compute, s(:args, :dna1, :dna2), s(:if, s(:call, s(:call, s(:lvar, :dna1), :size), :==, s(:call, s(:lvar, :dna2), :size)), s(:call, s(:iter, s(:call, s(:call, s(:call, s(:lvar, :dna1), :chars), :zip, s(:call, s(:lvar, :dna2), :chars)), :map), s(:args, :q, :w), s(:if, s(:call, s(:lvar, :q), :!=, s(:lvar, :w)), s(:lit, 1), s(:lit, 0))), :reduce, s(:lit, :+)), s(:call, nil, :puts, s(:str, "Cannot run computation: Strands are of differing length."))))