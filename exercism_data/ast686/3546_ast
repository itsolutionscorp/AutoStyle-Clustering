s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :dna1, :dna2), s(:if, s(:call, s(:call, s(:lvar, :dna1), :length), :!=, s(:call, s(:lvar, :dna2), :length)), s(:call, nil, :raise, s(:str, "strands need to be the same length!")), nil), s(:lasgn, :dist, s(:lit, 0)), s(:lasgn, :i, s(:lit, 0)), s(:while, s(:call, s(:lvar, :i), :<, s(:call, s(:lvar, :dna1), :length)), s(:block, s(:if, s(:call, s(:call, s(:lvar, :dna1), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :dna2), :[], s(:lvar, :i))), s(:lasgn, :dist, s(:call, s(:lvar, :dist), :+, s(:lit, 1))), nil), s(:lasgn, :i, s(:call, s(:lvar, :i), :+, s(:lit, 1)))), true), s(:return, s(:lvar, :dist))))