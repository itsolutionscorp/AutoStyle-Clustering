s(:defn, :compute, s(:args, :strand_a, :strand_b), s(:lasgn, :differences, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :strand_a), :split, s(:str, "")), :each_with_index), s(:args, :letter, :index), s(:block, s(:if, s(:call, s(:lvar, :index), :>, s(:call, s(:call, s(:lvar, :strand_b), :length), :-, s(:lit, 1))), s(:break), nil), s(:if, s(:call, s(:call, s(:lvar, :strand_b), :[], s(:lvar, :index)), :!=, s(:lvar, :letter)), s(:lasgn, :differences, s(:call, s(:lvar, :differences), :+, s(:lit, 1))), nil))), s(:lvar, :differences))