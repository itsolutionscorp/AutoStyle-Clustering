s(:defn, :compute, s(:args, :strand_a, :strand_b), s(:lasgn, :min_strand_length, s(:call, s(:array, s(:call, s(:lvar, :strand_a), :length), s(:call, s(:lvar, :strand_b), :length)), :min)), s(:lasgn, :mutations, s(:lit, 0)), s(:iter, s(:call, s(:lvar, :min_strand_length), :times), s(:args, :i), s(:if, s(:call, s(:call, s(:lvar, :strand_a), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :strand_b), :[], s(:lvar, :i))), s(:lasgn, :mutations, s(:call, s(:lvar, :mutations), :+, s(:lit, 1))), nil)), s(:lvar, :mutations))