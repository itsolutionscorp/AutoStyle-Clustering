s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :str_a, :str_b), s(:lasgn, :str_a_array, s(:call, s(:lvar, :str_a), :split, s(:str, ""))), s(:lasgn, :str_b_array, s(:call, s(:lvar, :str_b), :split, s(:str, ""))), s(:lasgn, :str_length, s(:call, s(:lvar, :str_a_array), :length)), s(:lasgn, :index, s(:lit, 0)), s(:lasgn, :mismatches, s(:lit, 0)), s(:iter, s(:call, s(:lvar, :str_length), :times), s(:args), s(:block, s(:if, s(:call, s(:call, s(:lvar, :str_a_array), :at, s(:lvar, :index)), :==, s(:call, s(:lvar, :str_b_array), :at, s(:lvar, :index))), s(:lasgn, :mismatches, s(:call, s(:lvar, :mismatches), :+, s(:lit, 1))), nil), s(:lasgn, :index, s(:call, s(:lvar, :index), :+, s(:lit, 1))))), s(:return, s(:call, s(:lvar, :str_length), :-, s(:lvar, :mismatches)))))