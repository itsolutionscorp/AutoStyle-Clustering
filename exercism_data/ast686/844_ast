s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :x, :y), s(:if, s(:call, s(:lvar, :x), :==, s(:lvar, :y)), s(:return, s(:lit, 0)), nil), s(:lasgn, :s1, s(:call, s(:lvar, :x), :split, s(:str, ""))), s(:lasgn, :s2, s(:call, s(:lvar, :y), :split, s(:str, ""))), s(:lasgn, :hammingDistance, s(:lit, 0)), s(:iter, s(:call, s(:lvar, :s1), :each_with_index), s(:args, :val, :index), s(:block, s(:if, s(:call, s(:lvar, :index), :>=, s(:call, s(:lvar, :s2), :length)), s(:break), nil), s(:if, s(:call, s(:lvar, :val), :!=, s(:call, s(:lvar, :s2), :[], s(:lvar, :index))), s(:lasgn, :hammingDistance, s(:call, s(:lvar, :hammingDistance), :+, s(:lit, 1))), nil))), s(:return, s(:lvar, :hammingDistance))))