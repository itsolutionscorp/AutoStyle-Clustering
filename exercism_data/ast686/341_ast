s(:class, :Phrase, nil, s(:defn, :initialize, s(:args, :words), s(:iasgn, :@words, s(:lvar, :words))), s(:defn, :word_count, s(:args), s(:iasgn, :@words, s(:call, s(:ivar, :@words), :downcase)), s(:call, s(:ivar, :@words), :delete!, s(:str, ":.!&@$%^&")), s(:call, s(:ivar, :@words), :gsub!, s(:str, ","), s(:str, " ")), s(:iasgn, :@split_words, s(:call, s(:ivar, :@words), :split)), s(:lasgn, :counts, s(:hash)), s(:iter, s(:call, s(:ivar, :@split_words), :each), s(:args, :word), s(:if, s(:call, s(:call, s(:lvar, :counts), :[], s(:lvar, :word)), :!=, s(:nil)), s(:attrasgn, s(:lvar, :counts), :[]=, s(:lvar, :word), s(:call, s(:call, s(:lvar, :counts), :[], s(:lvar, :word)), :+, s(:lit, 1))), s(:attrasgn, s(:lvar, :counts), :[]=, s(:lvar, :word), s(:lit, 1)))), s(:lvar, :counts)))