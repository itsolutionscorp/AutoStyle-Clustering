s(:defn, :compute, s(:args, :strand_a, :strand_b), s(:lasgn, :hamming_difference, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :strand_a), :length), :times), s(:args, :position), s(:if, s(:call, s(:call, s(:lvar, :strand_a), :[], s(:lvar, :position)), :!=, s(:call, s(:lvar, :strand_b), :[], s(:lvar, :position))), s(:lasgn, :hamming_difference, s(:call, s(:lvar, :hamming_difference), :+, s(:lit, 1))), nil)), s(:lvar, :hamming_difference))