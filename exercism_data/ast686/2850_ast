s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :str_a, :str_b), s(:lasgn, :count, s(:call, s(:array, s(:call, s(:lvar, :str_a), :length), s(:call, s(:lvar, :str_b), :length)), :min)), s(:lasgn, :res, s(:lit, 0)), s(:iter, s(:call, s(:lvar, :count), :times), s(:args, :i), s(:if, s(:call, s(:call, s(:lvar, :str_a), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :str_b), :[], s(:lvar, :i))), s(:lasgn, :res, s(:call, s(:lvar, :res), :+, s(:lit, 1))), nil)), s(:lvar, :res)))