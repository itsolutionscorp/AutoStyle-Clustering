s(:defn, :compute, s(:args, :a, :b), s(:lasgn, :result, s(:lit, 0)), s(:lasgn, :n, s(:if, s(:call, s(:call, s(:lvar, :a), :length), :>, s(:call, s(:lvar, :b), :length)), s(:call, s(:lvar, :b), :length), s(:call, s(:lvar, :a), :length))), s(:for, s(:dot3, s(:lit, 0), s(:lvar, :n)), s(:lasgn, :i), s(:if, s(:call, s(:call, s(:lvar, :a), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :b), :[], s(:lvar, :i))), s(:lasgn, :result, s(:call, s(:lvar, :result), :+, s(:lit, 1))), nil)), s(:lvar, :result))