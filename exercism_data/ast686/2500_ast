s(:defn, :compute, s(:args, :source, :dest), s(:if, s(:and, s(:call, s(:lvar, :source), :kind_of?, s(:const, :String)), s(:call, s(:lvar, :dest), :kind_of?, s(:const, :String))), nil, s(:return, s(:nil))), s(:lasgn, :distance, s(:lit, 0)), s(:lasgn, :max_length, s(:if, s(:call, s(:call, s(:lvar, :source), :length), :>, s(:call, s(:lvar, :dest), :length)), s(:call, s(:lvar, :dest), :length), s(:call, s(:lvar, :source), :length))), s(:iter, s(:call, s(:lit, 0), :upto, s(:call, s(:lvar, :max_length), :-, s(:lit, 1))), s(:args, :idx), s(:if, s(:call, s(:call, s(:lvar, :source), :[], s(:lvar, :idx)), :!=, s(:call, s(:lvar, :dest), :[], s(:lvar, :idx))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil)), s(:lvar, :distance))