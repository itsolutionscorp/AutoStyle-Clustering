s(:defn, :compute, s(:args, :arg1, :arg2), s(:if, s(:call, s(:call, s(:lvar, :arg1), :length), :!=, s(:call, s(:lvar, :arg2), :length)), s(:return, s(:str, "Error: the two arguments must be of the same length")), s(:block, s(:lasgn, :arg1, s(:call, s(:lvar, :arg1), :split, s(:str, ""))), s(:lasgn, :arg2, s(:call, s(:lvar, :arg2), :split, s(:str, ""))), s(:lasgn, :diff_count, s(:lit, 0)), s(:iter, s(:call, s(:lvar, :arg1), :each_index), s(:args, :i), s(:if, s(:call, s(:call, s(:lvar, :arg1), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :arg2), :[], s(:lvar, :i))), s(:lasgn, :diff_count, s(:call, s(:lvar, :diff_count), :+, s(:lit, 1))), nil)), s(:return, s(:lvar, :diff_count)))))