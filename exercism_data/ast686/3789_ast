s(:defn, :compute, s(:args, :strand1, :strand2), s(:lasgn, :diff_count, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :strand1), :each_char), :with_index), s(:args, :char1, :i), s(:block, s(:lasgn, :char2, s(:call, s(:lvar, :strand2), :[], s(:lvar, :i))), s(:if, s(:call, s(:lvar, :char2), :nil?), s(:break), nil), s(:if, s(:call, s(:lvar, :char1), :!=, s(:lvar, :char2)), s(:lasgn, :diff_count, s(:call, s(:lvar, :diff_count), :+, s(:lit, 1))), nil))), s(:lvar, :diff_count))