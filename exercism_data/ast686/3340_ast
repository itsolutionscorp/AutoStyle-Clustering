s(:module, :Hamming, s(:defn, :compute, s(:args, :ham1, :ham2), s(:lasgn, :limit, s(:if, s(:call, s(:call, s(:lvar, :ham1), :size), :>, s(:call, s(:lvar, :ham2), :size)), s(:call, s(:lvar, :ham2), :size), s(:call, s(:lvar, :ham1), :size))), s(:iter, s(:call, s(:call, s(:call, s(:call, s(:lvar, :ham1), :[], s(:dot3, s(:lit, 0), s(:lvar, :limit))), :chars), :zip, s(:call, s(:call, s(:lvar, :ham2), :[], s(:dot3, s(:lit, 0), s(:lvar, :limit))), :chars)), :reduce, s(:lit, 0)), s(:args, :sum, s(:masgn, :a, :b)), s(:call, s(:lvar, :sum), :+, s(:if, s(:call, s(:lvar, :a), :eql?, s(:lvar, :b)), s(:lit, 0), s(:lit, 1))))))