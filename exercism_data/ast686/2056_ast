s(:defn, :compute, s(:args, :strand1, :strand2), s(:lasgn, :hamming_distance, s(:lit, 0)), s(:lasgn, :strand1_nucleotides, s(:call, s(:lvar, :strand1), :split, s(:lit, //))), s(:lasgn, :strand2_nucleotides, s(:call, s(:lvar, :strand2), :split, s(:lit, //))), s(:iter, s(:call, s(:call, s(:lvar, :strand1_nucleotides), :length), :times), s(:args, :nucleotide), s(:if, s(:call, s(:call, s(:lvar, :strand1_nucleotides), :[], s(:lvar, :nucleotide)), :==, s(:call, s(:lvar, :strand2_nucleotides), :[], s(:lvar, :nucleotide))), nil, s(:lasgn, :hamming_distance, s(:call, s(:lvar, :hamming_distance), :+, s(:lit, 1))))), s(:lvar, :hamming_distance))