s(:defn, :compute, s(:args, :a, :b), s(:lasgn, :distance, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :a), :split, s(:lit, //)), :each_with_index), s(:args, :e, :i), s(:if, s(:and, s(:call, s(:lvar, :a), :[], s(:lvar, :i)), s(:call, s(:lvar, :b), :[], s(:lvar, :i))), s(:if, s(:call, s(:call, s(:lvar, :a), :[], s(:lvar, :i)), :==, s(:call, s(:lvar, :b), :[], s(:lvar, :i))), nil, s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1)))), nil)), s(:return, s(:lvar, :distance)))