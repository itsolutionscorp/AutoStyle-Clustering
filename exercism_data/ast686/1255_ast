s(:defn, :compute, s(:args, :dna, :another_dna), s(:if, s(:call, s(:call, s(:lvar, :dna), :length), :!=, s(:call, s(:lvar, :another_dna), :length)), s(:call, nil, :raise, s(:const, :ArgumentError), s(:str, "different lengths")), nil), s(:lasgn, :count, s(:lit, 0)), s(:if, s(:call, s(:lvar, :dna), :==, s(:lvar, :another_dna)), s(:return, s(:lvar, :count)), nil), s(:iter, s(:call, s(:call, s(:call, s(:lvar, :dna), :split, s(:str, "")), :zip, s(:call, s(:lvar, :another_dna), :split, s(:str, ""))), :each), s(:args, :a, :b), s(:if, s(:call, s(:lvar, :a), :!=, s(:lvar, :b)), s(:lasgn, :count, s(:call, s(:lvar, :count), :+, s(:lit, 1))), nil)), s(:lvar, :count))