s(:defn, :compute, s(:args, :first_strand, :second_strand), s(:lasgn, :first_strand_array, s(:call, s(:lvar, :first_strand), :split, s(:lit, //))), s(:lasgn, :second_strand_array, s(:call, s(:lvar, :second_strand), :split, s(:lit, //))), s(:lasgn, :hamming_distance, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :first_strand_array), :each_with_index), :map), s(:args, :element, :index), s(:block, s(:if, s(:call, s(:lvar, :index), :==, s(:call, s(:lvar, :second_strand_array), :count)), s(:break), nil), s(:if, s(:call, s(:lvar, :element), :!=, s(:call, s(:lvar, :second_strand_array), :[], s(:lvar, :index))), s(:lasgn, :hamming_distance, s(:call, s(:lvar, :hamming_distance), :+, s(:lit, 1))), nil))), s(:return, s(:lvar, :hamming_distance)))