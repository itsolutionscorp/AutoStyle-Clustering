s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :a, :b), s(:call, s(:iter, s(:call, s(:call, s(:call, s(:lvar, :a), :split, s(:lit, //)), :each_with_index), :map), s(:args, :c, :i), s(:if, s(:call, s(:lvar, :c), :==, s(:or, s(:call, s(:lvar, :b), :[], s(:lvar, :i)), s(:lvar, :c))), s(:lit, 0), s(:lit, 1))), :inject, s(:block_pass, s(:lit, :+)))))