s(:defn, :compute, s(:args, :s1, :s2), s(:if, s(:or, s(:lvar, :s1), s(:lvar, :s2)), nil, s(:return)), s(:lasgn, :s2_chars, s(:call, s(:lvar, :s2), :chars)), s(:iter, s(:call, s(:call, s(:call, s(:call, s(:lvar, :s1), :chars), :each), :with_index), :inject, s(:lit, 0)), s(:args, :sum, s(:masgn, :s1_char, :index)), s(:block, s(:if, s(:lasgn, :s2_char, s(:call, s(:lvar, :s2_chars), :[], s(:lvar, :index))), nil, s(:return, s(:lvar, :sum))), s(:if, s(:call, s(:lvar, :s1_char), :!=, s(:lvar, :s2_char)), s(:lasgn, :sum, s(:call, s(:lvar, :sum), :+, s(:lit, 1))), nil), s(:lvar, :sum))))