s(:defn, :compute, s(:args, :first_strand, :second_strand), s(:lasgn, :hamming_count, s(:lit, 0)), s(:if, s(:call, s(:call, s(:lvar, :first_strand), :length), :<, s(:call, s(:lvar, :second_strand), :length)), s(:lasgn, :test_length, s(:call, s(:lvar, :first_strand), :length)), s(:lasgn, :test_length, s(:call, s(:lvar, :second_strand), :length))), s(:iter, s(:call, s(:lvar, :test_length), :times), s(:args, :i), s(:if, s(:call, s(:call, s(:lvar, :first_strand), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :second_strand), :[], s(:lvar, :i))), s(:lasgn, :hamming_count, s(:call, s(:lvar, :hamming_count), :+, s(:lit, 1))), nil)), s(:return, s(:lvar, :hamming_count)))