s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :sqc1, :sqc2), s(:if, s(:call, s(:call, s(:lvar, :sqc1), :length), :!=, s(:call, s(:lvar, :sqc2), :length)), s(:call, nil, :raise, s(:call, s(:const, :ArgumentError), :new, s(:str, "Seqences have to have the same lenght"))), nil), s(:lasgn, :counter, s(:lit, 0)), s(:lasgn, :arry1, s(:call, s(:call, s(:lvar, :sqc1), :chars), :to_a)), s(:lasgn, :arry2, s(:call, s(:call, s(:lvar, :sqc2), :chars), :to_a)), s(:iter, s(:call, s(:lvar, :arry1), :each_with_index), s(:args, :letter, :index), s(:if, s(:call, s(:lvar, :letter), :==, s(:call, s(:lvar, :arry2), :[], s(:lvar, :index))), s(:lasgn, :counter, s(:call, s(:lvar, :counter), :+, s(:lit, 0))), s(:lasgn, :counter, s(:call, s(:lvar, :counter), :+, s(:lit, 1))))), s(:lvar, :counter)))