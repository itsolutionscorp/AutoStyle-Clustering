s(:defn, :compute, s(:args, :strand1, :strand2), s(:lasgn, :total_difference, s(:lit, 0)), s(:lasgn, :a, s(:call, s(:array, s(:call, s(:lvar, :strand1), :length), s(:call, s(:lvar, :strand2), :length)), :min)), s(:iter, s(:call, s(:lit, 0), :upto, s(:call, s(:lvar, :a), :-, s(:lit, 1))), s(:args, :index_position), s(:if, s(:call, s(:call, s(:lvar, :strand1), :[], s(:lvar, :index_position)), :==, s(:call, s(:lvar, :strand2), :[], s(:lvar, :index_position))), nil, s(:lasgn, :total_difference, s(:call, s(:lvar, :total_difference), :+, s(:lit, 1))))), s(:lvar, :total_difference))