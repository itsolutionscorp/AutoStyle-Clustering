s(:defn, :compute, s(:args, :sequence1, :sequence2), s(:if, s(:call, s(:call, s(:lvar, :sequence1), :length), :!=, s(:call, s(:lvar, :sequence2), :length)), s(:call, nil, :puts, s(:str, "ValueError, cannot calculate hamming distance for sequences of different length!")), s(:block, s(:iasgn, :@distance, s(:lit, 0)), s(:lasgn, :hamming_pairs, s(:call, s(:call, s(:lvar, :sequence1), :split, s(:str, "")), :zip, s(:call, s(:lvar, :sequence2), :split, s(:str, "")))), s(:iter, s(:call, s(:lvar, :hamming_pairs), :each), s(:args, :pair), s(:if, s(:call, s(:call, s(:lvar, :pair), :[], s(:lit, 0)), :!=, s(:call, s(:lvar, :pair), :[], s(:lit, 1))), s(:iasgn, :@distance, s(:call, s(:ivar, :@distance), :+, s(:lit, 1))), nil)), s(:ivar, :@distance))))