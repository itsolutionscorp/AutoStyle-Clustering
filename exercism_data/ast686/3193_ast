s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :first_strand, :other_strand), s(:if, s(:call, s(:lvar, :first_strand), :===, s(:lvar, :other_strand)), s(:return, s(:lit, 0)), nil), s(:if, s(:call, s(:call, s(:lvar, :first_strand), :length), :>, s(:call, s(:lvar, :other_strand), :length)), s(:return, s(:lit, 1)), nil), s(:lasgn, :distance, s(:lit, 0)), s(:for, s(:dot3, s(:lit, 0), s(:call, s(:lvar, :first_strand), :size)), s(:lasgn, :i), s(:if, s(:call, s(:call, s(:lvar, :first_strand), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :other_strand), :[], s(:lvar, :i))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil)), s(:lvar, :distance)))