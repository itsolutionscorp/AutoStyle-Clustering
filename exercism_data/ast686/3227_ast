s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :s1, :s2), s(:iter, s(:call, s(:call, s(:call, s(:call, s(:lvar, :s1), :chars), :first, s(:call, s(:lvar, :s2), :length)), :zip, s(:call, s(:lvar, :s2), :chars)), :inject, s(:lit, 0)), s(:args, :distance, :chars), s(:if, s(:call, s(:call, s(:lvar, :chars), :[], s(:lit, 0)), :==, s(:call, s(:lvar, :chars), :[], s(:lit, 1))), s(:lvar, :distance), s(:call, s(:lvar, :distance), :+, s(:lit, 1))))))