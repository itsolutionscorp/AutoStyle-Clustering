s(:defn, :compute, s(:args, :input1, :input2), s(:iasgn, :@minlength, s(:call, s(:array, s(:call, s(:lvar, :input1), :length), s(:call, s(:lvar, :input2), :length)), :min)), s(:iasgn, :@maxlength, s(:call, s(:array, s(:call, s(:lvar, :input1), :length), s(:call, s(:lvar, :input2), :length)), :max)), s(:iasgn, :@to_ignore, s(:lit, 0)), s(:if, s(:call, s(:ivar, :@maxlength), :>, s(:ivar, :@minlength)), s(:iasgn, :@to_ignore, s(:call, s(:ivar, :@maxlength), :-, s(:ivar, :@minlength))), nil), s(:iasgn, :@i, s(:lit, 0)), s(:iasgn, :@j, s(:call, s(:ivar, :@maxlength), :-, s(:ivar, :@to_ignore))), s(:iasgn, :@result, s(:lit, 0)), s(:while, s(:call, s(:ivar, :@i), :<, s(:ivar, :@j)), s(:block, s(:if, s(:call, s(:call, s(:lvar, :input1), :[], s(:ivar, :@i)), :!=, s(:call, s(:lvar, :input2), :[], s(:ivar, :@i))), s(:iasgn, :@result, s(:call, s(:ivar, :@result), :+, s(:lit, 1))), nil), s(:iasgn, :@i, s(:call, s(:ivar, :@i), :+, s(:lit, 1)))), true), s(:return, s(:ivar, :@result)))