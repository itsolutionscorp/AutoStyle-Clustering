s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :strand1, :strand2), s(:if, s(:and, s(:call, s(:lvar, :strand1), :empty?), s(:call, s(:lvar, :strand2), :empty?)), s(:lit, 0), s(:iter, s(:call, s(:iter, s(:call, s(:call, s(:call, s(:lvar, :strand1), :split, s(:str, "")), :each_with_index), :map), s(:args, :letter, :i), s(:if, s(:or, s(:call, s(:call, s(:call, s(:lvar, :strand2), :split, s(:str, "")), :[], s(:lvar, :i)), :nil?), s(:call, s(:lvar, :letter), :==, s(:call, s(:lvar, :strand2), :[], s(:lvar, :i)))), s(:lit, 0), s(:lit, 1))), :inject), s(:args, :sum, :x), s(:call, s(:lvar, :sum), :+, s(:lvar, :x))))))