s(:defn, :compute, s(:args, :strand1, :strand2), s(:lasgn, :runs, s(:if, s(:call, s(:call, s(:lvar, :strand1), :length), :<=, s(:call, s(:lvar, :strand2), :length)), s(:call, s(:lvar, :strand1), :length), s(:call, s(:lvar, :strand2), :length))), s(:lasgn, :count, s(:lit, 0)), s(:iter, s(:call, s(:lvar, :runs), :times), s(:args, :position), s(:if, s(:call, s(:call, s(:lvar, :strand1), :[], s(:lvar, :position)), :!=, s(:call, s(:lvar, :strand2), :[], s(:lvar, :position))), s(:lasgn, :count, s(:call, s(:lvar, :count), :+, s(:lit, 1))), nil)), s(:lvar, :count))