s(:defn, :compute, s(:args, :gene_1, :gene_2), s(:lasgn, :strand_1, s(:call, s(:lvar, :gene_1), :split, s(:lit, //))), s(:lasgn, :strand_2, s(:call, s(:lvar, :gene_2), :split, s(:lit, //))), s(:lasgn, :short_strand, s(:if, s(:call, s(:call, s(:lvar, :strand_1), :length), :<, s(:call, s(:lvar, :strand_2), :length)), s(:lvar, :strand_1), s(:lvar, :strand_2))), s(:lasgn, :long_strand, s(:if, s(:call, s(:call, s(:lvar, :strand_2), :length), :<, s(:call, s(:lvar, :strand_1), :length)), s(:lvar, :strand_2), s(:lvar, :strand_1))), s(:iter, s(:call, s(:call, s(:lvar, :short_strand), :zip, s(:lvar, :long_strand)), :inject, s(:lit, 0)), s(:args, :memo, :tuple), s(:call, s(:lvar, :memo), :+, s(:if, s(:call, s(:call, s(:lvar, :tuple), :first), :!=, s(:call, s(:lvar, :tuple), :last)), s(:lit, 1), s(:lit, 0)))))