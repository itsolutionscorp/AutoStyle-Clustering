s(:defn, :compute, s(:args, :strand_a, :strand_b), s(:lasgn, :nucleotide_position, s(:lit, 0)), s(:lasgn, :hamming_distance, s(:lit, 0)), s(:while, s(:call, s(:lvar, :nucleotide_position), :<, s(:call, s(:lvar, :strand_a), :length)), s(:block, s(:if, s(:call, s(:call, s(:lvar, :strand_a), :[], s(:lvar, :nucleotide_position)), :!=, s(:call, s(:lvar, :strand_b), :[], s(:lvar, :nucleotide_position))), s(:lasgn, :hamming_distance, s(:call, s(:lvar, :hamming_distance), :+, s(:lit, 1))), nil), s(:lasgn, :nucleotide_position, s(:call, s(:lvar, :nucleotide_position), :+, s(:lit, 1)))), true), s(:lvar, :hamming_distance))