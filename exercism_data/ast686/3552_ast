s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :first_strand, :second_strand), s(:iasgn, :@string_difference, s(:lit, 0)), s(:if, s(:call, s(:call, s(:lvar, :first_strand), :length), :==, s(:call, s(:lvar, :second_strand), :length)), s(:block, s(:lasgn, :string_length, s(:call, s(:lvar, :first_strand), :length)), s(:for, s(:dot2, s(:lit, 0), s(:lvar, :string_length)), s(:lasgn, :i), s(:block, s(:if, s(:call, s(:call, s(:lvar, :first_strand), :slice, s(:lvar, :i)), :!=, s(:call, s(:lvar, :second_strand), :slice, s(:lvar, :i))), s(:iasgn, :@string_difference, s(:call, s(:ivar, :@string_difference), :+, s(:lit, 1))), nil), s(:lasgn, :i, s(:call, s(:lvar, :i), :+, s(:lit, 1)))))), s(:call, nil, :puts, s(:str, "Strings are not of identical length"))), s(:ivar, :@string_difference)))