s(:defn, :compute, s(:args, :firstDNA, :secondDNA), s(:lasgn, :shorterDNALength, s(:if, s(:call, s(:call, s(:lvar, :firstDNA), :length), :<, s(:call, s(:lvar, :secondDNA), :length)), s(:call, s(:lvar, :firstDNA), :length), s(:call, s(:lvar, :secondDNA), :length))), s(:lasgn, :nucleotidesRange, s(:dot2, s(:lit, 0), s(:call, s(:lvar, :shorterDNALength), :-, s(:lit, 1)))), s(:lasgn, :hammingDistance, s(:lit, 0)), s(:for, s(:lvar, :nucleotidesRange), s(:lasgn, :singleNucleotide), s(:if, s(:call, s(:call, s(:lvar, :firstDNA), :[], s(:lvar, :singleNucleotide)), :!=, s(:call, s(:lvar, :secondDNA), :[], s(:lvar, :singleNucleotide))), s(:lasgn, :hammingDistance, s(:call, s(:lvar, :hammingDistance), :+, s(:lit, 1))), nil)), s(:lvar, :hammingDistance))