s(:defn, :compute, s(:args, :seq1, :seq2), s(:lasgn, :count, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:call, s(:lvar, :seq1), :chars), :map!), :with_index), s(:args, :n, :i), s(:if, s(:call, s(:lvar, :n), :!=, s(:call, s(:lvar, :seq2), :[], s(:lvar, :i))), s(:lasgn, :count, s(:call, s(:lvar, :count), :+, s(:lit, 1))), nil)), s(:lvar, :count))