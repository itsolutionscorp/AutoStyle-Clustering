s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :strand_a, :strand_b), s(:if, s(:call, s(:call, s(:lvar, :strand_a), :length), :!=, s(:call, s(:lvar, :strand_b), :length)), s(:call, nil, :raise, s(:str, "Strands are different lengths!")), nil), s(:lasgn, :hamming_distance, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :strand_a), :length), :times), s(:args, :position), s(:if, s(:call, s(:call, s(:lvar, :strand_a), :[], s(:lvar, :position)), :==, s(:call, s(:lvar, :strand_b), :[], s(:lvar, :position))), nil, s(:lasgn, :hamming_distance, s(:call, s(:lvar, :hamming_distance), :+, s(:lit, 1))))), s(:lvar, :hamming_distance)))