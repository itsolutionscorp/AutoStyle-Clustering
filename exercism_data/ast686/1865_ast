s(:defn, :compute, s(:args, :x, :y), s(:lasgn, :_score, s(:lit, 0)), s(:if, s(:call, s(:call, s(:lvar, :x), :length), :!=, s(:call, s(:lvar, :y), :length)), s(:block, s(:call, nil, :puts, s(:str, "You are comparing differently lengthed strings, please try again.")), s(:call, nil, :exit)), nil), s(:lasgn, :_i, s(:lit, 0)), s(:lasgn, :_z, s(:call, s(:lvar, :x), :length)), s(:lasgn, :_score, s(:lit, 0)), s(:while, s(:call, s(:lvar, :_i), :<, s(:lvar, :_z)), s(:block, s(:if, s(:call, s(:call, s(:lvar, :x), :[], s(:lvar, :_i)), :!=, s(:call, s(:lvar, :y), :[], s(:lvar, :_i))), s(:lasgn, :_score, s(:call, s(:lvar, :_score), :+, s(:lit, 1))), nil), s(:lasgn, :_i, s(:call, s(:lvar, :_i), :+, s(:lit, 1)))), true), s(:lvar, :_score))