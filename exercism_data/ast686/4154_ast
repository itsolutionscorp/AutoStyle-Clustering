s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :seq_a, :seq_b), s(:if, s(:call, s(:call, s(:lvar, :seq_a), :length), :==, s(:call, s(:lvar, :seq_b), :length)), nil, s(:call, nil, :raise, s(:const, :ArgumentError))), s(:lasgn, :char_pairs, s(:call, s(:call, s(:lvar, :seq_a), :chars), :zip, s(:call, s(:lvar, :seq_b), :chars))), s(:iter, s(:call, s(:lvar, :char_pairs), :inject, s(:lit, 0)), s(:args, :hamming_distance, :char_pair), s(:call, s(:call, s(:lvar, :hamming_distance), :+, s(:call, s(:call, s(:lvar, :char_pair), :uniq), :length)), :-, s(:lit, 1)))))