s(:defn, :compute, s(:args, :a, :b), s(:lasgn, :delta, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :a), :length), :times), s(:args, :i), s(:if, s(:call, s(:call, s(:lvar, :b), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :a), :[], s(:lvar, :i))), s(:lasgn, :delta, s(:call, s(:lvar, :delta), :+, s(:lit, 1))), nil)), s(:lvar, :delta))