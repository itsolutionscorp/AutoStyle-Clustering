s(:class, :Hamming, nil, s(:defn, :compute, s(:args, :first, :second), s(:lasgn, :dist, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :first), :chars), :each_index), s(:args, :i), s(:lasgn, :dist, s(:call, s(:lvar, :dist), :+, s(:if, s(:call, s(:call, s(:lvar, :first), :[], s(:lvar, :i)), :==, s(:call, s(:lvar, :second), :[], s(:lvar, :i))), s(:lit, 0), s(:lit, 1))))), s(:return, s(:lvar, :dist))))