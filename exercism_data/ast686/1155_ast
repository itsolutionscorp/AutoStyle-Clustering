s(:defn, :compute, s(:args, :strand1, :strand2), s(:lasgn, :distance, s(:lit, 0)), s(:iter, s(:call, s(:dot2, s(:lit, 0), s(:call, s(:lvar, :strand1), :length)), :each), s(:args, :index), s(:if, s(:call, s(:call, s(:lvar, :strand1), :[], s(:lvar, :index)), :==, s(:call, s(:lvar, :strand2), :[], s(:lvar, :index))), nil, s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))))), s(:return, s(:lvar, :distance)))