s(:defn, :compute, s(:args, :first_strand, :other_strand), s(:if, s(:call, s(:lvar, :first_strand), :===, s(:lvar, :other_strand)), s(:return, s(:lit, 0)), nil), s(:if, s(:call, s(:call, s(:lvar, :first_strand), :length), :>, s(:call, s(:lvar, :other_strand), :length)), s(:return, s(:lit, 1)), nil), s(:lasgn, :distance, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :first_strand), :each_char), :each_with_index), s(:args, :ch, :index), s(:if, s(:call, s(:call, s(:lvar, :first_strand), :[], s(:lvar, :index)), :!=, s(:call, s(:lvar, :other_strand), :[], s(:lvar, :index))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil)), s(:lvar, :distance))