s(:module, :Hamming, s(:defn, :compute, s(:args, :strand_a, :strand_b), s(:if, s(:call, s(:call, s(:lvar, :strand_a), :length), :==, s(:call, s(:lvar, :strand_b), :length)), nil, s(:call, nil, :raise, s(:call, s(:const, :ArgumentError), :new, s(:str, "Strands must be of equal length")))), s(:call, s(:iter, s(:call, s(:dot3, s(:lit, 0), s(:call, s(:lvar, :strand_a), :length)), :collect), s(:args, :i), s(:if, s(:call, s(:call, s(:lvar, :strand_a), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :strand_b), :[], s(:lvar, :i))), s(:lit, 1), s(:lit, 0))), :inject, s(:lit, :+))))