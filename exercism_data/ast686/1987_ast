s(:defn, :compute, s(:args, :a, :b), s(:lasgn, :a_len, s(:call, s(:lvar, :a), :length)), s(:lasgn, :b_len, s(:call, s(:lvar, :b), :length)), s(:lasgn, :hamming, s(:lit, 0)), s(:lasgn, :common_len, s(:if, s(:call, s(:lvar, :a_len), :>, s(:lvar, :b_len)), s(:lvar, :b_len), s(:lvar, :a_len))), s(:lasgn, :common_len, s(:call, s(:lvar, :common_len), :-, s(:lit, 1))), s(:for, s(:dot2, s(:lit, 0), s(:lvar, :common_len)), s(:lasgn, :i), s(:if, s(:call, s(:call, s(:lvar, :a), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :b), :[], s(:lvar, :i))), s(:lasgn, :hamming, s(:call, s(:lvar, :hamming), :+, s(:lit, 1))), nil)), s(:return, s(:lvar, :hamming)))