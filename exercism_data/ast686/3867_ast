s(:defn, :compute, s(:args, :source, :dest), s(:lasgn, :thing, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :source), :chars), :each_with_index), s(:args, :char, :numb), s(:if, s(:or, s(:call, s(:lvar, :char), :==, s(:call, s(:call, s(:lvar, :dest), :chars), :[], s(:lvar, :numb))), s(:call, s(:call, s(:call, s(:lvar, :dest), :chars), :[], s(:lvar, :numb)), :nil?)), s(:false), s(:lasgn, :thing, s(:call, s(:lvar, :thing), :+, s(:lit, 1))))), s(:lvar, :thing))