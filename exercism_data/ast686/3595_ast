s(:defn, :compute, s(:args, :strand_one, :strand_two), s(:lasgn, :hamming_distance_count, s(:lit, 0)), s(:lasgn, :string_position, s(:lit, 0)), s(:lasgn, :shortest_string_length, s(:call, s(:array, s(:call, s(:lvar, :strand_one), :length), s(:call, s(:lvar, :strand_two), :length)), :min)), s(:while, s(:call, s(:lvar, :string_position), :<, s(:lvar, :shortest_string_length)), s(:block, s(:if, s(:call, s(:call, s(:call, s(:lvar, :strand_one), :chars), :[], s(:lvar, :string_position)), :!=, s(:call, s(:call, s(:lvar, :strand_two), :chars), :[], s(:lvar, :string_position))), s(:lasgn, :hamming_distance_count, s(:call, s(:lvar, :hamming_distance_count), :+, s(:lit, 1))), nil), s(:lasgn, :string_position, s(:call, s(:lvar, :string_position), :+, s(:lit, 1)))), true), s(:lvar, :hamming_distance_count))