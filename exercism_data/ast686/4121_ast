s(:module, :Hamming, s(:defn, :compute, s(:args, :first, :second), s(:lasgn, :diff_counter, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :first), :each_char), :with_index), s(:args, :f, :i), s(:block, s(:if, s(:call, s(:call, s(:lvar, :second), :[], s(:lvar, :i)), :nil?), s(:break), nil), s(:if, s(:call, s(:lvar, :f), :!=, s(:call, s(:lvar, :second), :[], s(:lvar, :i))), s(:lasgn, :diff_counter, s(:call, s(:lvar, :diff_counter), :+, s(:lit, 1))), nil))), s(:return, s(:lvar, :diff_counter))))