s(:class, :Phrase, nil, s(:defn, :initialize, s(:args, :phrase), s(:iasgn, :@phrase, s(:lvar, :phrase))), s(:defn, :word_count, s(:args), s(:call, s(:ivar, :@phrase), :downcase!), s(:call, s(:ivar, :@phrase), :gsub!, s(:lit, /[^0-9a-z,' ]/i), s(:str, "")), s(:call, s(:ivar, :@phrase), :gsub!, s(:str, ","), s(:str, " ")), s(:call, s(:ivar, :@phrase), :gsub!, s(:str, "  "), s(:str, " ")), s(:lasgn, :word_array, s(:call, s(:ivar, :@phrase), :split)), s(:lasgn, :out, s(:call, s(:const, :Hash), :new)), s(:iter, s(:call, s(:lvar, :word_array), :each), s(:args, :elem), s(:if, s(:call, s(:call, s(:lvar, :out), :[], s(:lvar, :elem)), :==, s(:nil)), s(:attrasgn, s(:lvar, :out), :[]=, s(:lvar, :elem), s(:lit, 1)), s(:attrasgn, s(:lvar, :out), :[]=, s(:lvar, :elem), s(:call, s(:call, s(:lvar, :out), :[], s(:lvar, :elem)), :+, s(:lit, 1))))), s(:lvar, :out)))