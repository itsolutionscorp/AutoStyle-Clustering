s(:class, :Hamming, nil, s(:sclass, s(:self), s(:defn, :compute, s(:args, :a, :b), s(:if, s(:call, s(:lvar, :a), :==, s(:lvar, :b)), s(:return, s(:lit, 0)), nil), s(:if, s(:call, s(:call, s(:lvar, :a), :length), :>, s(:call, s(:lvar, :b), :length)), s(:lasgn, :a, s(:call, s(:lvar, :a), :[], s(:dot2, s(:lit, 0), s(:call, s(:call, s(:lvar, :b), :length), :-, s(:lit, 1))))), nil), s(:lasgn, :result, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :a), :split, s(:str, "")), :each_with_index), s(:args, :e, :i), s(:if, s(:call, s(:lvar, :e), :!=, s(:call, s(:lvar, :b), :[], s(:lvar, :i))), s(:lasgn, :result, s(:call, s(:lvar, :result), :+, s(:lit, 1))), nil)), s(:lvar, :result))))