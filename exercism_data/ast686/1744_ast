s(:defn, :compute, s(:args, :strand_1, :strand_2), s(:lasgn, :zipped_strands, s(:call, s(:call, s(:lvar, :strand_1), :chars), :zip, s(:call, s(:lvar, :strand_2), :chars))), s(:iter, s(:call, s(:lvar, :zipped_strands), :inject, s(:lit, 0)), s(:args, :distance, s(:masgn, :this_char, :other_char)), s(:if, s(:call, s(:lvar, :this_char), :!=, s(:lvar, :other_char)), s(:call, s(:lvar, :distance), :+, s(:lit, 1)), s(:lvar, :distance))))