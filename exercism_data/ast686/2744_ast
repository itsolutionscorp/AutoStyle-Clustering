s(:defn, :compute, s(:args, :dna_strand_a, :dna_strand_b), s(:lasgn, :hamming_distance, s(:lit, 0)), s(:lasgn, :min_length, s(:call, s(:array, s(:call, s(:lvar, :dna_strand_a), :length), s(:call, s(:lvar, :dna_strand_b), :length)), :min)), s(:iter, s(:call, s(:lvar, :min_length), :times), s(:args, :i), s(:if, s(:call, s(:call, s(:lvar, :dna_strand_a), :[], s(:lvar, :i)), :==, s(:call, s(:lvar, :dna_strand_b), :[], s(:lvar, :i))), nil, s(:lasgn, :hamming_distance, s(:call, s(:lvar, :hamming_distance), :+, s(:lit, 1))))), s(:lvar, :hamming_distance))