s(:defn, :compute, s(:args, :strand_a, :strand_b), s(:lasgn, :distance, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :strand_a), :length), :times), s(:args, :index), s(:block, s(:if, s(:call, s(:lvar, :strand_b), :[], s(:lvar, :index)), nil, s(:break)), s(:if, s(:call, s(:call, s(:lvar, :strand_a), :[], s(:lvar, :index)), :!=, s(:call, s(:lvar, :strand_b), :[], s(:lvar, :index))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil))), s(:lvar, :distance))