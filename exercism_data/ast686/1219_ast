s(:defn, :compute, s(:args, :s1, :s2), s(:lasgn, :e1, s(:call, s(:lvar, :s1), :chars)), s(:lasgn, :e2, s(:call, s(:lvar, :s2), :chars)), s(:lasgn, :distance, s(:lit, 0)), s(:iter, s(:call, nil, :loop), s(:args), s(:if, s(:call, s(:call, s(:lvar, :e1), :next), :!=, s(:call, s(:lvar, :e2), :next)), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil)), s(:lvar, :distance))