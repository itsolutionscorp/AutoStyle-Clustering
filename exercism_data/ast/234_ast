s(:defn, :compute, s(:args, :strand1, :strand2), s(:lasgn, :nucleotides1, s(:call, s(:lvar, :strand1), :split, s(:lit, //))), s(:lasgn, :nucleotides2, s(:call, s(:lvar, :strand2), :split, s(:lit, //))), s(:lasgn, :hamming_count, s(:lit, 0)), s(:if, s(:call, s(:call, s(:lvar, :nucleotides1), :length), :!=, s(:call, s(:lvar, :nucleotides2), :length)), s(:lit, 0), s(:while, s(:call, s(:lvar, :nucleotides1), :!=, s(:array)), s(:if, s(:call, s(:call, s(:lvar, :nucleotides1), :[], s(:lit, 0)), :!=, s(:call, s(:lvar, :nucleotides2), :[], s(:lit, 0))), s(:block, s(:lasgn, :hamming_count, s(:call, s(:lvar, :hamming_count), :+, s(:lit, 1))), s(:call, s(:lvar, :nucleotides1), :shift), s(:call, s(:lvar, :nucleotides2), :shift)), s(:block, s(:call, s(:lvar, :nucleotides2), :shift), s(:call, s(:lvar, :nucleotides1), :shift))), true)), s(:return, s(:lvar, :hamming_count)))