s(:defn, :compute, s(:args, :first_dna, :second_dna), s(:lasgn, :count, s(:lit, 0)), s(:iter, s(:call, s(:lit, 0), :upto, s(:call, s(:lvar, :first_dna), :size)), s(:args, :i), s(:if, s(:call, s(:call, s(:lvar, :first_dna), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :second_dna), :[], s(:lvar, :i))), s(:lasgn, :count, s(:call, s(:lvar, :count), :+, s(:lit, 1))), nil)), s(:return, s(:lvar, :count)))