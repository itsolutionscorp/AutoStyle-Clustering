s(:defn, :compute, s(:args, :first_strand, :second_strand), s(:lasgn, :distance, s(:lit, 0)), s(:if, s(:call, s(:call, s(:lvar, :first_strand), :length), :<=, s(:call, s(:lvar, :second_strand), :length)), s(:block, s(:lasgn, :strand1, s(:lvar, :first_strand)), s(:lasgn, :strand2, s(:lvar, :second_strand))), s(:block, s(:lasgn, :strand1, s(:lvar, :second_strand)), s(:lasgn, :strand2, s(:lvar, :first_strand)))), s(:lasgn, :strand1_nucleotides, s(:call, s(:lvar, :strand1), :split, s(:str, ""))), s(:lasgn, :strand2_nucleotides, s(:call, s(:lvar, :strand2), :split, s(:str, ""))), s(:iter, s(:call, s(:lvar, :strand1_nucleotides), :each_with_index), s(:args, :nucleotide, :i), s(:if, s(:call, s(:lvar, :nucleotide), :!=, s(:call, s(:lvar, :strand2_nucleotides), :[], s(:lvar, :i))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil)), s(:lvar, :distance))