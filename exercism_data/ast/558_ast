s(:defn, :compute, s(:args, :first_strand, :second_strand), s(:iasgn, :@pom, s(:lit, 0)), s(:iasgn, :@first_length, s(:call, s(:lvar, :first_strand), :length)), s(:iasgn, :@second_length, s(:call, s(:lvar, :second_strand), :length)), s(:iasgn, :@min_length, s(:call, s(:array, s(:ivar, :@first_length), s(:ivar, :@second_length)), :min)), s(:for, s(:dot2, s(:lit, 0), s(:call, s(:ivar, :@min_length), :-, s(:lit, 1))), s(:lasgn, :i), s(:if, s(:call, s(:call, s(:call, s(:lvar, :first_strand), :chars), :[], s(:lvar, :i)), :==, s(:call, s(:call, s(:lvar, :second_strand), :chars), :[], s(:lvar, :i))), nil, s(:iasgn, :@pom, s(:call, s(:ivar, :@pom), :+, s(:lit, 1))))), s(:ivar, :@pom))