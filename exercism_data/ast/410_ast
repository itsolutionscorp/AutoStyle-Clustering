s(:defn, :compute, s(:args, :strand_a, :strand_b), s(:lasgn, :distance, s(:lit, 0)), s(:lasgn, :length, s(:call, s(:call, s(:iter, s(:call, s(:array, s(:lvar, :strand_a), s(:lvar, :strand_b)), :sort), s(:args, :a, :b), s(:call, s(:call, s(:lvar, :a), :length), :<=>, s(:call, s(:lvar, :b), :length))), :first), :length)), s(:iter, s(:call, s(:lvar, :length), :times), s(:args, :index), s(:if, s(:call, s(:call, s(:lvar, :strand_a), :[], s(:lvar, :index)), :!=, s(:call, s(:lvar, :strand_b), :[], s(:lvar, :index))), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil)), s(:lvar, :distance))