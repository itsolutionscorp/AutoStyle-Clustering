s(:defn, :compute, s(:args, :strand_a, :strand_b), s(:lasgn, :position, s(:lit, 0)), s(:lasgn, :distance, s(:lit, 0)), s(:while, s(:and, s(:lasgn, :a, s(:call, s(:lvar, :strand_a), :[], s(:lvar, :position))), s(:lasgn, :b, s(:call, s(:lvar, :strand_b), :[], s(:lvar, :position)))), s(:block, s(:if, s(:call, s(:lvar, :a), :==, s(:lvar, :b)), nil, s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1)))), s(:lasgn, :position, s(:call, s(:lvar, :position), :+, s(:lit, 1)))), true), s(:lvar, :distance))