s(:defn, :compute, s(:args, :seq1, :seq2), s(:lasgn, :res, s(:array)), s(:iter, s(:call, s(:call, s(:lvar, :seq1), :length), :times), s(:args, :n), s(:call, s(:lvar, :res), :<<, s(:if, s(:call, s(:call, s(:lvar, :seq1), :[], s(:lvar, :n)), :==, s(:call, s(:lvar, :seq2), :[], s(:lvar, :n))), s(:lit, 0), s(:lit, 1)))), s(:lasgn, :sum, s(:call, s(:lvar, :res), :reduce, s(:lit, :+))))