s(:defn, :combine_anagrams, s(:args, :words), s(:lasgn, :groups, s(:array)), s(:iter, s(:call, s(:lvar, :words), :each), s(:args, :word), s(:block, s(:iter, s(:call, s(:lvar, :groups), :each), s(:args, :group), s(:block, s(:lasgn, :a, s(:call, s(:call, s(:call, s(:call, s(:call, s(:lvar, :group), :[], s(:lit, 0)), :upcase), :split, s(:lit, //)), :sort), :join)), s(:lasgn, :b, s(:call, s(:call, s(:call, s(:call, s(:lvar, :word), :upcase), :split, s(:lit, //)), :sort), :join)), s(:if, s(:call, s(:lvar, :a), :==, s(:lvar, :b)), s(:block, s(:call, s(:lvar, :group), :<<, s(:lvar, :word)), s(:lasgn, :word, s(:str, "")), s(:break)), nil))), s(:if, s(:call, s(:lvar, :word), :!=, s(:str, "")), s(:call, s(:lvar, :groups), :<<, s(:array, s(:lvar, :word))), nil))), s(:return, s(:lvar, :groups)))