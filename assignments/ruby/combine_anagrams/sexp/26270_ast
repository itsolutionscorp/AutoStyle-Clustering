s(:defn, :combine_anagrams, s(:args, :words), s(:lasgn, :all_groups, s(:array)), s(:if, s(:call, s(:call, s(:lvar, :words), :length), :==, s(:lit, 0)), s(:return, s(:lvar, :all_groups)), nil), s(:while, s(:call, s(:call, s(:lvar, :words), :length), :>, s(:lit, 0)), s(:block, s(:lasgn, :ana_group, s(:array)), s(:call, s(:lvar, :ana_group), :<<, s(:call, s(:lvar, :words), :[], s(:lit, 0))), s(:call, s(:lvar, :words), :delete_at, s(:lit, 0)), s(:if, s(:call, s(:call, s(:lvar, :words), :length), :>, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :words), :reverse), :each), s(:args, :wrd), s(:if, s(:call, s(:call, s(:call, s(:call, s(:call, s(:lvar, :wrd), :downcase), :split, s(:lit, //)), :sort), :join), :==, s(:call, s(:call, s(:call, s(:call, s(:call, s(:lvar, :ana_group), :[], s(:lit, 0)), :downcase), :split, s(:lit, //)), :sort), :join)), s(:block, s(:call, s(:lvar, :ana_group), :<<, s(:lvar, :wrd)), s(:call, s(:lvar, :words), :delete_at, s(:call, s(:lvar, :words), :rindex, s(:lvar, :wrd)))), nil)), nil), s(:call, s(:lvar, :all_groups), :<<, s(:lvar, :ana_group))), false), s(:return, s(:lvar, :all_groups)))