s(:defn, :combine_anagrams, s(:args, :words), s(:lasgn, :result, s(:array)), s(:while, s(:call, s(:call, s(:lvar, :words), :empty?), :!), s(:block, s(:lasgn, :word1, s(:call, s(:lvar, :words), :[], s(:lit, 0))), s(:lasgn, :group, s(:array)), s(:lasgn, :changed_input, s(:lvar, :words)), s(:iter, s(:call, s(:lvar, :words), :each), s(:args, :word2), s(:if, s(:call, s(:call, s(:call, s(:call, s(:lvar, :word1), :upcase), :chars), :sort), :==, s(:call, s(:call, s(:call, s(:lvar, :word2), :upcase), :chars), :sort)), s(:block, s(:call, s(:lvar, :group), :<<, s(:lvar, :word2)), s(:lasgn, :changed_input, s(:call, s(:lvar, :changed_input), :-, s(:array, s(:lvar, :word2))))), nil)), s(:lasgn, :words, s(:lvar, :changed_input)), s(:call, s(:lvar, :result), :<<, s(:lvar, :group))), true), s(:return, s(:lvar, :result)))