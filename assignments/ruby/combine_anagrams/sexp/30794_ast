s(:defn, :combine_anagrams, s(:args, :words), s(:if, s(:call, s(:lvar, :words), :==, s(:array)), s(:return, s(:lvar, :words)), nil), s(:if, s(:call, s(:call, s(:lvar, :words), :[], s(:lit, 0)), :kind_of?, s(:const, :String)), s(:lasgn, :anagrams, s(:call, s(:iter, s(:call, s(:lvar, :words), :group_by), s(:args, :word), s(:call, s(:call, s(:call, s(:lvar, :word), :downcase), :chars), :sort)), :values)), s(:if, s(:call, s(:call, s(:lvar, :words), :[], s(:lit, 0)), :kind_of?, s(:const, :Array)), s(:lasgn, :anagrams, s(:call, s(:iter, s(:call, s(:lvar, :words), :group_by), s(:args, :word), s(:call, s(:lvar, :word), :sort)), :values)), s(:call, nil, :raise, s(:const, :IncorrectInputError)))), s(:return, s(:lvar, :anagrams)))