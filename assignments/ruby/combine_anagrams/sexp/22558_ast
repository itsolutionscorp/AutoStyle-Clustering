s(:defn, :combine_anagrams, s(:args, :words), s(:lasgn, :sorted_words, s(:array)), s(:lasgn, :i, s(:lit, 0)), s(:iter, s(:call, s(:lvar, :words), :each), s(:args, :word), s(:block, s(:call, s(:lvar, :sorted_words), :<<, s(:lvar, :word)), s(:attrasgn, s(:lvar, :sorted_words), :[]=, s(:lvar, :i), s(:call, s(:call, s(:lvar, :sorted_words), :[], s(:lvar, :i)), :downcase)), s(:attrasgn, s(:lvar, :sorted_words), :[]=, s(:lvar, :i), s(:call, s(:call, s(:call, s(:call, s(:lvar, :sorted_words), :[], s(:lvar, :i)), :chars), :sort_by, s(:block_pass, s(:lit, :downcase))), :join)), s(:lasgn, :i, s(:call, s(:lvar, :i), :+, s(:lit, 1))))), s(:lasgn, :anagrams_list, s(:array)), s(:iter, s(:call, s(:lvar, :sorted_words), :each), s(:args, :word), s(:if, s(:call, s(:lvar, :anagrams_list), :include?, s(:lvar, :word)), nil, s(:call, s(:lvar, :anagrams_list), :<<, s(:lvar, :word)))), s(:lasgn, :i, s(:lit, 0)), s(:lasgn, :final_list, s(:array)), s(:for, s(:lvar, :anagrams_list), s(:lasgn, :anagram), s(:block, s(:lasgn, :j, s(:lit, 0)), s(:attrasgn, s(:lvar, :final_list), :[]=, s(:lvar, :i), s(:array)), s(:for, s(:lvar, :sorted_words), s(:lasgn, :word), s(:block, s(:if, s(:call, s(:lvar, :anagram), :==, s(:lvar, :word)), s(:call, s(:call, s(:lvar, :final_list), :[], s(:lvar, :i)), :<<, s(:call, s(:lvar, :words), :[], s(:lvar, :j))), nil), s(:lasgn, :j, s(:call, s(:lvar, :j), :+, s(:lit, 1))))), s(:lasgn, :i, s(:call, s(:lvar, :i), :+, s(:lit, 1))))), s(:return, s(:lvar, :final_list)))