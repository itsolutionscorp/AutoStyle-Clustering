s(:defn, :combine_anagrams, s(:args, :words), s(:lasgn, :match, s(:call, s(:const, :Array), :new, s(:lit, 0))), s(:lasgn, :anagram, s(:call, s(:const, :Array), :new, s(:lit, 0))), s(:lasgn, :ordered_array, s(:call, s(:const, :Array), :new, s(:lit, 0))), s(:lasgn, :copy_ordered_array, s(:call, s(:const, :Array), :new, s(:lit, 0))), s(:for, s(:lvar, :words), s(:lasgn, :i), s(:block, s(:lasgn, :word_array, s(:call, s(:const, :Array), :new, s(:lit, 0))), s(:lasgn, :word_array, s(:call, s(:lvar, :i), :split, s(:lit, /\s*/))), s(:lasgn, :count, s(:lit, 0)), s(:for, s(:lvar, :word_array), s(:lasgn, :r), s(:block, s(:attrasgn, s(:lvar, :word_array), :[]=, s(:lvar, :count), s(:call, s(:lvar, :r), :downcase)), s(:lasgn, :count, s(:call, s(:lvar, :count), :+, s(:lit, 1))))), s(:attrasgn, s(:lvar, :ordered_array), :[]=, s(:call, s(:lvar, :ordered_array), :length), s(:call, s(:const, :Array), :new, s(:lit, 0))), s(:attrasgn, s(:call, s(:lvar, :ordered_array), :[], s(:call, s(:call, s(:lvar, :ordered_array), :length), :-, s(:lit, 1))), :[]=, s(:lit, 0), s(:call, s(:call, s(:lvar, :word_array), :sort), :join)), s(:attrasgn, s(:call, s(:lvar, :ordered_array), :[], s(:call, s(:call, s(:lvar, :ordered_array), :length), :-, s(:lit, 1))), :[]=, s(:lit, 1), s(:lvar, :i)))), s(:for, s(:lvar, :ordered_array), s(:lasgn, :i), s(:attrasgn, s(:lvar, :copy_ordered_array), :[]=, s(:call, s(:lvar, :copy_ordered_array), :length), s(:lvar, :i))), s(:for, s(:lvar, :ordered_array), s(:lasgn, :i), s(:block, s(:for, s(:lvar, :copy_ordered_array), s(:lasgn, :j), s(:if, s(:call, s(:call, s(:lvar, :i), :[], s(:lit, 0)), :==, s(:call, s(:lvar, :j), :[], s(:lit, 0))), s(:attrasgn, s(:lvar, :match), :[]=, s(:call, s(:lvar, :match), :length), s(:call, s(:lvar, :j), :[], s(:lit, 1))), nil)), s(:if, s(:call, s(:call, s(:lvar, :match), :length), :>, s(:lit, 0)), s(:block, s(:attrasgn, s(:lvar, :anagram), :[]=, s(:call, s(:lvar, :anagram), :length), s(:lvar, :match)), s(:for, s(:lvar, :match), s(:lasgn, :h), s(:for, s(:lvar, :copy_ordered_array), s(:lasgn, :q), s(:if, s(:call, s(:lvar, :h), :==, s(:call, s(:lvar, :q), :[], s(:lit, 1))), s(:call, s(:lvar, :copy_ordered_array), :delete, s(:lvar, :q)), nil))), s(:lasgn, :match, s(:call, s(:const, :Array), :new, s(:lit, 0)))), nil))), s(:return, s(:call, s(:lvar, :anagram), :sort)))