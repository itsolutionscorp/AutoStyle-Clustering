s(:defn, :combine_anagrams, s(:args, :words), s(:lasgn, :groups, s(:array)), s(:iter, s(:call, s(:lvar, :words), :each), s(:args, :x), s(:block, s(:lasgn, :inc_bool, s(:false)), s(:iter, s(:call, s(:lvar, :groups), :each), s(:args, :y), s(:if, s(:call, s(:lvar, :y), :include?, s(:lvar, :x)), s(:lasgn, :inc_bool, s(:true)), nil)), s(:if, s(:call, s(:lvar, :inc_bool), :==, s(:false)), s(:block, s(:lasgn, :group, s(:array)), s(:call, s(:lvar, :groups), :<<, s(:lvar, :group)), s(:iter, s(:call, s(:lvar, :words), :each), s(:args, :y), s(:if, s(:call, s(:call, s(:call, s(:call, s(:call, s(:lvar, :x), :upcase), :chars), :sort), :join), :==, s(:call, s(:call, s(:call, s(:call, s(:lvar, :y), :upcase), :chars), :sort), :join)), s(:call, s(:call, s(:lvar, :groups), :last), :<<, s(:lvar, :y)), nil))), nil))), s(:return, s(:lvar, :groups)))