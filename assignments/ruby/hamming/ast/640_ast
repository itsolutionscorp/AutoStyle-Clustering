s(:defn, :compute, s(:args, :original, :mutated), s(:lasgn, :difference, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :original), :chars), :each_with_index), s(:args, :orig_chr, :index), s(:block, s(:if, s(:call, s(:lvar, :mutated), :[], s(:lvar, :index)), nil, s(:break)), s(:if, s(:call, s(:lvar, :orig_chr), :!=, s(:call, s(:lvar, :mutated), :[], s(:lvar, :index))), s(:lasgn, :difference, s(:call, s(:lvar, :difference), :+, s(:lit, 1))), nil))), s(:lvar, :difference))