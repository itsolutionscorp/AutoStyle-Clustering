s(:defn, :compute, s(:args, :a, :b), s(:if, s(:call, s(:call, s(:lvar, :a), :length), :>, s(:call, s(:lvar, :b), :length)), s(:masgn, s(:array, s(:lasgn, :a), s(:lasgn, :b)), s(:array, s(:lvar, :b), s(:lvar, :a))), nil), s(:if, s(:call, s(:lvar, :a), :empty?), s(:lit, 0), s(:call, s(:if, s(:call, s(:call, s(:lvar, :a), :[], s(:lit, 0)), :==, s(:call, s(:lvar, :b), :[], s(:lit, 0))), s(:lit, 0), s(:lit, 1)), :+, s(:call, nil, :compute, s(:call, s(:lvar, :a), :[], s(:lit, 1..-1)), s(:call, s(:lvar, :b), :[], s(:lit, 1..-1))))))