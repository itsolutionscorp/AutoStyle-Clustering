s(:defn, :compute, s(:args, :a, :b), s(:lasgn, :errors, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:array, s(:call, s(:lvar, :a), :length), s(:call, s(:lvar, :b), :length)), :min), :times), s(:args, :n), s(:if, s(:call, s(:call, s(:lvar, :a), :[], s(:lvar, :n)), :==, s(:call, s(:lvar, :b), :[], s(:lvar, :n))), nil, s(:lasgn, :errors, s(:call, s(:lvar, :errors), :+, s(:lit, 1))))), s(:lvar, :errors))