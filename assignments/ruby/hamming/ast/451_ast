s(:defn, :compute, s(:args, :arg1, :arg2), s(:if, s(:call, s(:call, s(:lvar, :arg1), :length), :!=, s(:call, s(:lvar, :arg2), :length)), s(:call, nil, :abort, s(:lasgn, :message, s(:str, "Sequences must be of equal length"))), s(:block, s(:lasgn, :arg1, s(:call, s(:lvar, :arg1), :chars)), s(:lasgn, :arg2, s(:call, s(:lvar, :arg2), :chars)), s(:lasgn, :distance, s(:lit, 0)), s(:iter, s(:call, s(:lvar, :arg1), :each_index), s(:args, :index), s(:if, s(:call, s(:call, s(:call, s(:lvar, :arg1), :[], s(:lvar, :index)), :==, s(:call, s(:lvar, :arg2), :[], s(:lvar, :index))), :!), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil)))), s(:return, s(:lvar, :distance)))