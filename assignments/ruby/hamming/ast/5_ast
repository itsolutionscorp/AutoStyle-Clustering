s(:defn, :compute, s(:args, :a, :b), s(:lasgn, :dist, s(:lit, 0)), s(:lasgn, :len, s(:call, s(:array, s(:call, s(:lvar, :a), :length), s(:call, s(:lvar, :b), :length)), :min)), s(:iter, s(:call, s(:lvar, :len), :times), s(:args, :i), s(:lasgn, :dist, s(:call, s(:lvar, :dist), :+, s(:if, s(:call, s(:call, s(:lvar, :a), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :b), :[], s(:lvar, :i))), s(:lit, 1), s(:lit, 0))))), s(:lvar, :dist))