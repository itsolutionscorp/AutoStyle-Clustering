s(:defn, :compute, s(:args, :strand_a, :strand_b), s(:lasgn, :hamming_distance, s(:lit, 0)), s(:lasgn, :strand_1, s(:call, s(:lvar, :strand_a), :chars)), s(:lasgn, :strand_2, s(:call, s(:lvar, :strand_b), :chars)), s(:iter, s(:call, s(:lvar, :strand_1), :zip, s(:lvar, :strand_2)), s(:args, :a, :b), s(:if, s(:and, s(:call, s(:lvar, :a), :!=, s(:lvar, :b)), s(:call, s(:lvar, :b), :!=, s(:nil))), s(:lasgn, :hamming_distance, s(:call, s(:lvar, :hamming_distance), :+, s(:lit, 1))), nil)), s(:lvar, :hamming_distance))