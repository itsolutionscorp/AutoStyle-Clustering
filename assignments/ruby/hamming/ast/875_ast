s(:defn, :compute, s(:args, :strand_one, :strand_two), s(:lasgn, :hamming_distance, s(:lit, 0)), s(:for, s(:dot2, s(:lit, 0), s(:call, s(:lvar, :strand_one), :length)), s(:lasgn, :i), s(:if, s(:call, s(:call, s(:lvar, :strand_one), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :strand_two), :[], s(:lvar, :i))), s(:lasgn, :hamming_distance, s(:call, s(:lvar, :hamming_distance), :+, s(:lit, 1))), nil)), s(:lvar, :hamming_distance))