s(:defn, :compute, s(:args, :original_dna, :mutated_dna), s(:lasgn, :hamming_distance, s(:lit, 0)), s(:lasgn, :step, s(:lit, 1)), s(:iter, s(:call, s(:lvar, :original_dna), :each_char), s(:args, :c), s(:block, s(:if, s(:call, s(:lvar, :c), :==, s(:call, s(:lvar, :mutated_dna), :[], s(:call, s(:lvar, :step), :-, s(:lit, 1)))), nil, s(:lasgn, :hamming_distance, s(:call, s(:lvar, :hamming_distance), :+, s(:lit, 1)))), s(:lasgn, :step, s(:call, s(:lvar, :step), :+, s(:lit, 1))), s(:if, s(:or, s(:call, s(:lvar, :step), :>, s(:call, s(:lvar, :original_dna), :length)), s(:call, s(:lvar, :step), :>, s(:call, s(:lvar, :mutated_dna), :length))), s(:return, s(:lvar, :hamming_distance)), nil))), s(:return, s(:lvar, :hamming_distance)))