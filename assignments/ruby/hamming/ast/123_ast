s(:defn, :compute, s(:args, :a, :b), s(:lasgn, :distance, s(:lit, 0)), s(:masgn, s(:array, s(:lasgn, :as), s(:lasgn, :bs)), s(:array, s(:call, s(:lvar, :a), :to_s), s(:call, s(:lvar, :b), :to_s))), s(:lasgn, :alen, s(:call, s(:lvar, :as), :length)), s(:lasgn, :blen, s(:call, s(:lvar, :bs), :length)), s(:lasgn, :min, s(:call, s(:array, s(:lvar, :alen), s(:lvar, :blen)), :min)), s(:lasgn, :as, s(:call, s(:lvar, :as), :[], s(:dot2, s(:lit, 0), s(:call, s(:lvar, :min), :-, s(:lit, 1))))), s(:lasgn, :bs, s(:call, s(:lvar, :bs), :[], s(:dot2, s(:lit, 0), s(:call, s(:lvar, :min), :-, s(:lit, 1))))), s(:masgn, s(:array, s(:lasgn, :short), s(:lasgn, :long)), s(:to_ary, s(:call, s(:array, s(:lvar, :as), s(:lvar, :bs)), :sort))), s(:iter, s(:call, s(:call, s(:call, s(:lvar, :long), :chars), :zip, s(:call, s(:lvar, :short), :chars)), :each), s(:args, :ac, :bc), s(:if, s(:call, s(:lvar, :ac), :!=, s(:lvar, :bc)), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil)), s(:lvar, :distance))