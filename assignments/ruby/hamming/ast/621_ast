s(:defn, :compute, s(:args, :seq_a, :seq_b), s(:lasgn, :distance, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:call, s(:lvar, :seq_a), :scan, s(:lit, /./)), :zip, s(:call, s(:lvar, :seq_b), :scan, s(:lit, /./))), :each), s(:args, :a, :b), s(:if, s(:call, s(:lvar, :a), :!=, s(:lvar, :b)), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:lit, 1))), nil)), s(:lvar, :distance))