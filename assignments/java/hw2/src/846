public int multiply(int x,int y){
  if ((x == 0) || (y == 0)) {
    return 0;
  }
 else   if (x == 1) {
    return y;
  }
 else   if (y == 1) {
    return x;
  }
 else   if (x == add(1,~1)) {
    return add(~y,1);
  }
 else   if (y == add(1,~1)) {
    return add(~x,1);
  }
 else   if ((y & 1) == 0) {
    return multiply(x << 1,y >> 1);
  }
 else {
    return add(x,multiply(x,add(y,add(1,~1))));
  }
}

