public int multiply(int x,int y){
  int multiplied=0;
  int sum=0;
  for (int i=0; i < 32; i++) {
    int multiplier=getBit(x,i);
    for (int j=0; j < 32; j++) {
      int multiplier2=getBit(y,j);
      if ((multiplier2 & multiplier) == 1) {
        multiplied=setBit(multiplied,j);
      }
    }
    multiplied=multiplied << i;
    sum=add(sum,multiplied);
    multiplied=0;
  }
  return sum;
}

