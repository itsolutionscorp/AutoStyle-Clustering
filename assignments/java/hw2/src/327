public int multiply(int x,int y){
  if (y == 0 || x == 0) {
    return 0;
  }
 else   if (y == 1) {
    return x;
  }
 else   if (x == 1) {
    return y;
  }
 else   if (y == add(~1,1)) {
    return add(~x,1);
  }
  if ((y & 1) == 1) {
    return add(x,multiply(x << 1,y >> 1));
  }
  return multiply(x << 1,y >> 1);
}

