public int multiply(int x,int y){
  int result=0;
  int yHolder=y;
  int xHolder=x;
  if (yHolder < 0) {
    yHolder=~yHolder;
    yHolder=add(yHolder,1);
  }
  if (xHolder < 0) {
    xHolder=~xHolder;
    xHolder=add(xHolder,1);
  }
  for (int i=0; i < yHolder; i+=1) {
    result=add(result,xHolder);
  }
  if ((x < 0) & (y > 0)) {
    result=~result;
    result=add(result,1);
  }
 else   if ((x > 0) & (y < 0)) {
    result=~result;
    result=add(result,1);
  }
  return result;
}

