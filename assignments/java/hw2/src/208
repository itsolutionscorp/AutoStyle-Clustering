public int multiply(int x,int y){
  if (x == 0 || y == 0) {
    return 0;
  }
  int tot=x;
  int start=x;
  boolean yneg=false;
  if (y < 0) {
    y=add(~y,1);
    System.out.println(y);
    yneg=true;
  }
  if (y == 1) {
    return tot;
  }
  while (y > 1) {
    tot=tot << 1;
    y=y - 2;
  }
  if (y == 1) {
    tot=add(tot,x);
  }
  if (yneg) {
    return add(~tot,1);
  }
  return tot;
}

