public int multiply(int x,int y){
  int a=x, b=y, result=0;
  int is_neg;
  if (a < 0 && b < 0) {
    a=~a;
    b=~b;
    a=add(a,1);
    b=add(b,1);
  }
 else   if (a > 0 && b < 0) {
    int temp=a;
    a=b;
    b=temp;
  }
  while (b != 0) {
    if ((b & 01) != 0) {
      result=add(result,a);
    }
    a<<=1;
    b>>=1;
  }
  return result;
}

