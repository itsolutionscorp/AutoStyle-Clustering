public int multiply(int x,int y){
  if ((x == 0) || (y == 0)) {
    return 0;
  }
  if ((y == 2) || (y == 1)) {
    return x << add(y,-1);
  }
  if (isNegative(y)) {
    return multiply(flipSign(x),flipSign(y));
  }
  if ((y & 1) == 0) {
    return multiply((x << 1),(y >>> 1));
  }
  return add(x,multiply(x,add(y,-1)));
}

