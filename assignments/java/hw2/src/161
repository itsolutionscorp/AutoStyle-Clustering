public int multiply(int x,int y){
  if (x == 0 || y == 0) {
    return 0;
  }
  if (y < 0) {
    x=flipSign(x);
    y=flipSign(y);
  }
  if (x == 1) {
    return y;
  }
 else   if (y == 1) {
    return x;
  }
 else   if (isPowerOfTwo(y)) {
    int result=shiftByTwos(x,y);
    return result;
  }
 else {
    int power=findPowerOfTwo(y);
    int result=shiftByTwos(x,power);
    for (int count=power; count < y; count++) {
      result=add(result,x);
    }
    return result;
  }
}

