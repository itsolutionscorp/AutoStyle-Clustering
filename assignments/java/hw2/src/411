public int multiply(int x,int y){
  if (x == 0 || y == 0) {
    return 0;
  }
  if (x < y) {
    int temp=x;
    x=y;
    y=temp;
  }
  int sum=0;
  while (y != 1) {
    if ((y & 1) == 1) {
      sum=add(sum,x);
      y=add(y,-1);
    }
 else {
      x=x << 1;
      y=y >>> 1;
    }
  }
  return add(x,sum);
}

