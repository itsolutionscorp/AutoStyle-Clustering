public int multiply(int x,int y){
  int temp;
  if ((x > 0) && (y < 0)) {
    temp=x;
    x=y;
    y=temp;
  }
 else   if ((x < 0) && (y < 0)) {
    x=~x + 1;
    y=~y + 1;
  }
  int result=0;
  while (y != 0) {
    if ((y & 1) == 1) {
      result=add(result,x);
    }
    y=y >> 1;
    x=x << 1;
  }
  return result;
}

