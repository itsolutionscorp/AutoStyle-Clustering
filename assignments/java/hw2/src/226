public int multiply(int x,int y){
  if (x == 1) {
    return y;
  }
  if (y == 1) {
    return x;
  }
  if ((x == 0) || (y == 0)) {
    return 0;
  }
  int x1=x;
  int y1=y;
  int product=0;
  while (y1 != 0) {
    if ((y1 & 1) != 0) {
      product=add(product,x1);
    }
    x1=x1 << 1;
    y1=y1 >>> 1;
  }
  return product;
}

