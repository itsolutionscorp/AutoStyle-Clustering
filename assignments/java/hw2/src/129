public int multiply(int x,int y){
  if (x < 0) {
    return -multiply(-x,y);
  }
 else   if (y < 0) {
    return -multiply(x,-y);
  }
  int totalSum=0;
  int counter=0;
  while (y != 0) {
    if ((y & 1) != 0) {
      totalSum=add(totalSum,x << counter);
    }
    y=y >> 1;
    counter+=1;
  }
  return totalSum;
}

