public int multiply(int x,int y){
  int total=0;
  boolean flip=false;
  if ((x < 0) ^ (y < 0)) {
    flip=true;
  }
  if (x < 0) {
    x=add(~x,1);
  }
  if (y < 0) {
    y=add(~y,1);
  }
  while (y != 0) {
    if ((y & 1) != 0) {
      total=add(total,x);
    }
    x=x << 1;
    y=y >> 1;
  }
  if (flip) {
    total=add(~total,1);
  }
  return total;
}

