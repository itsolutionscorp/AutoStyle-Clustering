public int multiply(int x,int y){
  int i=0;
  int k=0;
  int z=0;
  if (x < 0) {
    k=add(k,1);
    x=~x;
    x=add(x,1);
  }
  if (y < 0) {
    k=add(k,1);
    y=~y;
    y=add(y,1);
  }
  while (y != 0) {
    if ((y & 1) == 0) {
      y=y >> 1;
      i=add(i,1);
    }
 else {
      z=add(x << i,z);
      y=y >> 1;
      i=add(i,1);
    }
  }
  if ((k % 2) == 0) {
    return z;
  }
 else {
    z=~z;
    z=add(z,1);
    return z;
  }
}

