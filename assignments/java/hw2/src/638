public int multiply(int x,int y){
  int seed=1;
  int result=0;
  int twoMult=0;
  int addIn=0;
  int negAddIn=0;
  if (y < 0) {
    x=add(~x,1);
    y=add(~y,1);
  }
  if (y == 1) {
    return x;
  }
  while (y > 1) {
    twoMult=seed & y;
    seed=seed << 1;
    addIn=multByTwo(x,twoMult);
    result=add(result,addIn);
    negAddIn=add(~addIn,1);
    y=add(y,negAddIn);
  }
  return result;
}

