public int multiply(int x,int y){
  if (x == 1)   return y;
  if (y == 1)   return x;
  if (y == 0 || x == 0)   return 0;
  if (isPowofTwo(y)) {
    while (y > 1) {
      int tempX=x;
      int tempY=y;
      x=tempX << 1;
      y=tempY >> 1;
    }
    return x;
  }
  int sum=0;
  for (int i=0; i < Math.abs(y); i++) {
    sum=add(sum,Math.abs(x));
  }
  if (y < 0 && x > 0 || (y > 0 && x < 0)) {
    return add(~sum,1);
  }
  return sum;
}

