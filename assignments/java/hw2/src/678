public int multiply(int x,int y){
  if (x == 0)   return 0;
  if (y == 0)   return 0;
  if (x == 1)   return y;
  if (y == 1)   return x;
  int xi=x >> 1, xl=x & 1;
  int yi=y >> 1, yl=y & 1;
  int xiyi=multiply(xi,yi) << 2, xlyi=multiply(xl,yi) << 1, xiyl=multiply(xi,yl) << 1, xlyl=xl & yl;
  return add(add(add(xiyi,xlyi),xiyl),xlyl);
}

