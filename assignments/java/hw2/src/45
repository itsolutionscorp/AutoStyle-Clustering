public int multiply(int x,int y){
  int product=0;
  boolean oppositeSign=false;
  if ((x < 0 && y > 0) || (x > 0 && y < 0)) {
    oppositeSign=!oppositeSign;
  }
  if (x < 0) {
    x=~x;
    x=add(x,1);
  }
  if (y < 0) {
    y=~y;
    y=add(y,1);
  }
  for (int i=0; i < y; i++) {
    product=add(product,x);
  }
  if (oppositeSign) {
    product=~product;
    product=add(product,1);
  }
  return product;
}

