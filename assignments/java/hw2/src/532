public int multiply(int x,int y){
  if (x == 0 || y == 0)   return 0;
  if (x > 0) {
    if (y > 0)     return add(x,multiply(x,add(y,add(~1,1))));
    return add(y,multiply(add(x,add(~1,1)),y));
  }
  if (y > 0)   return add(x,multiply(x,add(y,add(~1,1))));
  return multiply(add(~x,1),add(~y,1));
}

