public int multiply(int x,int y){
  boolean negative=x > 0 && y < 0 || x < 0 && y > 0;
  if (x < 0) {
    x=add(~x,1);
  }
  if (y < 0) {
    y=add(~y,1);
  }
  int sum=0;
  while (y > 0) {
    if ((y & 1) != 0) {
      sum=add(x,sum);
    }
    x=x << 1;
    y=y >> 1;
  }
  if (negative) {
    return add(~sum,1);
  }
  return sum;
}

