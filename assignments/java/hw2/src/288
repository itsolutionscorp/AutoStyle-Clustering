public int multiply(int x,int y){
  if ((x == 0) || (y == 0)) {
    return 0;
  }
 else   if (x == 1) {
    return y;
  }
 else   if (y == 1) {
    return x;
  }
 else   if (y % 2 == 0) {
    return multiply(x,y >>> 1) << 1;
  }
 else   if (x % 2 == 0) {
    return multiply(x >>> 1,y) << 1;
  }
 else {
    return add(x,multiply(x,y >>> 1) << 1);
  }
}

