public int multiply(int x,int y){
  int p=x;
  if (x >= 0 && y >= 0) {
    while (y > 1) {
      p=add(p,x);
      y=add(y,-1);
    }
  }
  if (x <= 0 && y <= 0) {
    p=Math.abs(x);
    y=Math.abs(y);
    x=Math.abs(x);
    while (y > 1) {
      p=add(p,x);
      y=add(y,-1);
    }
  }
 else   if (y < 0 && x >= 0) {
    while (y < 1) {
      p=add(p,-x);
      y=add(y,1);
    }
  }
 else {
    while (y > 1) {
      p=add(p,x);
      y=add(y,-1);
    }
  }
  return p;
}

