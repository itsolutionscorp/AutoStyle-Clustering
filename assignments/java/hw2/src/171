public int multiply(int x,int y){
  boolean negative=false;
  if (x < 0) {
    negative=!negative;
    x=add(~x,1);
  }
  if (y < 0) {
    negative=!negative;
    y=add(~y,1);
  }
  if (x > y) {
    int tmp=x;
    x=y;
    y=tmp;
  }
  int result=0;
  while (y != 0) {
    if ((y & 1) == 1) {
      result=add(result,x);
    }
    x=x << 1;
    y=y >> 1;
  }
  if (negative) {
    return add(~result,1);
  }
  return result;
}

