public int multiply(int x,int y){
  int sum=0;
  int a=x;
  int b=y;
  int sign=0;
  if (x < 0 && y > 0) {
    sign=1;
  }
  if (y < 0 && x > 0) {
    sign=1;
  }
  if (x < 0) {
    x=~x;
    x=add(x,1);
  }
  if (y < 0) {
    y=~y;
    y=add(y,1);
  }
  for (int i=0; x != 0; i++) {
    if ((x & 1) != 0) {
      sum=add(sum,y<<=i);
    }
    x>>>=1;
  }
  if (sign == 1) {
    sum=~sum;
    sum=add(sum,1);
  }
  return sum;
}

