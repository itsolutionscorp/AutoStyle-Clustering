public int multiply(int x,int y){
  int mask=1;
  int result=0;
  int term=0;
  int total=0;
  if (x == 0 || y == 0) {
    return 0;
  }
  if (y < 0) {
    x=add(~x,1);
    y=add(~y,1);
  }
  while (total < y) {
    if ((mask & y) == mask) {
      total=add(total,mask);
      term=x;
      for (int temp=mask; temp != 1; temp=temp >>> 1) {
        term=term << 1;
      }
      result=add(result,term);
    }
    mask=mask << 1;
  }
  return result;
}

