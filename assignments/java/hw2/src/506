public int multiply(int x,int y){
  int odd_y=y & 1;
  int odd_x=x & 1;
  if (x == 0 || y == 0) {
    return 0;
  }
 else   if (x == 1) {
    return y;
  }
 else   if (y == 1) {
    return x;
  }
 else   if (odd_y > 0) {
    int new_y=add(y,-1);
    return add(x,multiply(x,new_y));
  }
 else   if (odd_x > 0) {
    int new_x=add(x,-1);
    return add(y,multiply(y,new_x));
  }
  x=x << 1;
  y=y >> 1;
  return multiply(x,y);
}

