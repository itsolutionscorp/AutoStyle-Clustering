public int multiply(int x,int y){
  if ((x < 0) && (y < 0)) {
    return multiply(changeSign(x),changeSign(y));
  }
 else   if (y < 0) {
    return changeSign(multiply(x,changeSign(y)));
  }
 else   if (x < 0) {
    return changeSign(multiply(changeSign(x),y));
  }
  int count=0;
  int total=0;
  while (count < y) {
    total=add(total,x);
    count+=1;
  }
  return total;
}

