public int multiply(int x,int y){
  if (y == 1) {
    return x;
  }
 else   if ((y & 1) == 1) {
    return x + multiply(x << 1,y >> 1);
  }
  int index=0;
  while ((y & 1) != 1) {
    y=y >> 1;
    index+=1;
  }
  x=x << index;
  if (y > 1) {
    return multiply(x,y);
  }
 else {
    return x;
  }
}

