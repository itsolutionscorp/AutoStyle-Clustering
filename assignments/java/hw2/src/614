public int multiply(int x,int y){
  if (x == 0 || y == 0) {
    return 0;
  }
 else   if (x < 0) {
    if (y < 0) {
      return multiply(add(~x,1),add(~y,1));
    }
    return add(~multiply(add(~x,1),y),1);
  }
 else   if ((x & 1) == 1) {
    return add(y,multiply(x >> 1,y << 1));
  }
  return multiply(x >> 1,y << 1);
}

