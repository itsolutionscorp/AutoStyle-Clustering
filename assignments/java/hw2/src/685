public int multiply(int x,int y){
  if (y == 0 || x == 0) {
    return 0;
  }
 else {
    int xTwoExponent=0;
    int total=0;
    while (y != 0) {
      int yholder=y;
      y=y >>> 1;
      if ((y << 1) < yholder) {
        total=add(total,x << xTwoExponent);
      }
      xTwoExponent=add(xTwoExponent,1);
    }
    return total;
  }
}

