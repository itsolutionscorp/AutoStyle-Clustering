public int multiply(int x,int y){
  int sum=0;
  int shift=0;
  if (x < 0 || y < 0) {
    x=~x;
    y=~y;
    x=add(x,1);
    y=add(y,1);
  }
  while (y != 0) {
    if ((y & 1) == 1) {
      sum=add(sum,x << shift);
      shift=add(shift,1);
    }
 else {
      shift=add(shift,1);
    }
    y=y >>> 1;
  }
  return sum;
}

