public int multiply(int x,int y){
  boolean neg1=false;
  boolean neg2=false;
  if (x < 0) {
    neg1=true;
    x=add((~x),1);
  }
  if (y < 0) {
    neg2=true;
    y=add((~y),1);
  }
  boolean neg=neg1 ^ neg2;
  if (x < y) {
    int a=x;
    x=y;
    y=a;
  }
  int hold=0;
  while (y != 0) {
    if ((y & 1) != 0)     hold=add(hold,x);
    x=x << 1;
    y=y >> 1;
  }
  if (neg == true)   return add((~hold),1);
  return hold;
}

