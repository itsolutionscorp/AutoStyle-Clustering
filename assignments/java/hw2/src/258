public int multiply(int x,int y){
  if (x == 0 || y == 0) {
    return 0;
  }
  if (x == 1) {
    return y;
  }
 else   if (y == 1) {
    return x;
  }
  int final_value=0;
  while (y != 0) {
    if ((y & 1) == 1) {
      final_value=add(final_value,x);
    }
    x<<=1;
    y>>=1;
  }
  return final_value;
}

