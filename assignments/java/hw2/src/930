public int multiply(int x,int y){
  if (x < 0 && y > 0) {
    return -multiply(-x,y);
  }
  if (x > 0 && y < 0) {
    return -multiply(x,-y);
  }
  if (x < 0 && y < 0) {
    return multiply(-x,-y);
  }
  int product=0;
  while (y != 0) {
    if ((y & 01) != 0) {
      product=add(product,x);
    }
    x<<=1;
    y>>=1;
  }
  return product;
}

