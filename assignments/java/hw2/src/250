public int multiply(int x,int y){
  int i=0;
  int newx=0;
  int finalans=0;
  int copyy=y;
  while (copyy != 0) {
    if (copyy << 31 != 0) {
      newx=x << i;
    }
 else {
      newx=0;
    }
    copyy=copyy >>> 1;
    i=i + 1;
    Calculator addresult=new Calculator();
    finalans=addresult.add(newx,finalans);
  }
  return finalans;
}

