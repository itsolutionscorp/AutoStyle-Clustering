public int multiply(int x,int y){
  if (x < 0) {
    int temp=x;
    x=y;
    y=temp;
  }
  if (x < 0 && y < 0) {
    x=~x + 1;
    y=~y + 1;
  }
  int sum=0;
  while (x != 0) {
    int last_digit=x & 1;
    if (last_digit == 1) {
      sum=add(sum,y);
    }
    y=y << 1;
    x=x >> 1;
  }
  return sum;
}

