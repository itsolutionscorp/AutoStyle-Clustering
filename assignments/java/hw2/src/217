public int multiply(int x,int y){
  boolean negative=false;
  if (x >= 0 && y < 0) {
    y=-y;
    negative=true;
  }
 else   if (x < 0 && y >= 0) {
    x=-x;
    negative=true;
  }
 else   if (x < 0 && y < 0) {
    x=-x;
    y=-y;
  }
  int result=0;
  while (y != 0) {
    if ((y & 1) == 1) {
      result=add(result,x);
    }
    x<<=1;
    y>>=1;
  }
  return negative ? -result : result;
}

