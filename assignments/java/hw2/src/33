public int multiply(int x,int y){
  int temp=0;
  if (y == 0 || x == 0)   return 0;
  boolean neg=false;
  if (y < 0) {
    neg=!neg;
    y=add(~y,1);
  }
  if (x < 0) {
    neg=!neg;
    x=add(~x,1);
  }
  while (y > 0) {
    if ((y & 1) == 1) {
      temp=add(temp,x);
    }
    x=x << 1;
    y=y >>> 1;
  }
  if (neg) {
    temp=add(~temp,1);
  }
  return temp;
}

