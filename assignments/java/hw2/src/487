public int multiply(int x,int y){
  int counter=y;
  int product=0;
  if (counter > 0) {
    while (counter > 0) {
      product=add(product,x);
      counter-=1;
    }
  }
 else   if (counter < 0) {
    counter=add((~counter),1);
    while (counter > 0) {
      product=add(product,x);
      counter-=1;
    }
    product=add((~product),1);
  }
  return product;
}

