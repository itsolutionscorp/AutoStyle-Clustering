public int multiply(int x,int y){
  int result=0;
  if ((x < 0) && (y < 0)) {
    y=~y;
    y=add(y,1);
    x=~x;
    x=add(x,1);
    while (y != 0) {
      if ((y & 1) != 0) {
        result=add(result,x);
      }
      x=x << 1;
      y=y >> 1;
    }
  }
 else {
    while (y != 0) {
      if ((y & 1) != 0) {
        result=add(result,x);
      }
      x=x << 1;
      y=y >> 1;
    }
  }
  return result;
}

