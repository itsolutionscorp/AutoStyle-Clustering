public int multiply(int x,int y){
  int product=0;
  for (int n=0; n < 32; n+=1) {
    int helper=0;
    int multiplier=x;
    for (int i=0; i < 32; i+=1) {
      int a=getBit(multiplier,0);
      int b=getBit(y,0);
      if (b == 1) {
        if (a == 1) {
          helper=setBit(helper,i);
        }
        multiplier=multiplier >>> 1;
      }
    }
    product=add(product,helper << n);
    y=y >>> 1;
  }
  return product;
}

