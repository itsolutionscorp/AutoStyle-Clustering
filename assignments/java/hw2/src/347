public int multiply(int x,int y){
  int result=0;
  int xSub=x;
  int ySub=y;
  if (x < 0) {
    x=add(~x,1);
  }
  while (y != 0) {
    result=add(x,result);
    if (y < 0) {
      y++;
    }
 else {
      y--;
    }
  }
  if ((xSub < 0 && ySub > 0) || (ySub < 0 && xSub > 0)) {
    result=add(~result,1);
  }
  return result;
}

