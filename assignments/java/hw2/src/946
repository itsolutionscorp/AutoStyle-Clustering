public int multiply(int x,int y){
  if (x == 0 || y == 0) {
    return 0;
  }
  int saver=0;
  int result=x;
  while (y != 1) {
    if ((y & 1) == 0) {
      result=result << 1;
    }
 else {
      saver=add(saver,result);
      result=result << 1;
    }
    y=y >>> 1;
  }
  return result + saver;
}

