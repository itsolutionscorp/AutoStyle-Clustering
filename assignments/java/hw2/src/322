public int multiply(int x,int y){
  int product=0;
  if ((x < 0) && (y < 0)) {
    x=add(~x,1);
    y=add(~y,1);
  }
  if (y < 0) {
    int n=y;
    y=x;
    x=n;
  }
  while (y != 0) {
    if ((y & 01) != 0) {
      product=add(product,x);
      y-=1;
    }
    x<<=1;
    y>>=1;
  }
  return product;
}

