public int multiply(int x,int y){
  if (x > 0 && y > 0) {
    int result=0;
    int i=0;
    while (i < y) {
      result=add(result,x);
      i=i + 1;
    }
    return result;
  }
 else   if (x < 0 && y < 0) {
    x=negate(x);
    y=negate(y);
    return multiply(x,y);
  }
 else   if (x < 0) {
    x=negate(x);
  }
 else {
    y=negate(y);
  }
  return negate(multiply(x,y));
}

