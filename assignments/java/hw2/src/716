public int multiply(int x,int y){
  boolean isNegative=false;
  if (y < 0) {
    y=add(~y,1);
    isNegative=true;
  }
  int total=0;
  while (y != 0) {
    if ((y & 1) != 0) {
      total=add(total,x);
    }
    x=x << 1;
    y=y >> 1;
  }
  if (isNegative) {
    total=add(~total,1);
  }
  return total;
}

