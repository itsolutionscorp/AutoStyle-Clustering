public int multiply(int x,int y){
  int product=0;
  int i=0;
  int temp=0;
  Calculator adder=new Calculator();
  while (y != 0) {
    int currDigitY=y & 1;
    if (currDigitY == 1) {
      temp=x << i;
    }
 else {
      temp=0;
    }
    product=adder.add(product,temp);
    i+=1;
    y=y >>> 1;
  }
  return product;
}

