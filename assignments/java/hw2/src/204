public int multiply(int x,int y){
  int product=0;
  if (x == 0 || y == 0) {
    return 0;
  }
 else   if (x == 1) {
    return y;
  }
 else   if (y == 1) {
    return x;
  }
  while (y != 0) {
    if ((y & 1) != 0) {
      product=product + x;
    }
    x=x << 1;
    y=y >>> 1;
  }
  return product;
}

