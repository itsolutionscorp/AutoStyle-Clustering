public int multiply(int x,int y){
  boolean negative=false;
  if (x == 0 || y == 0)   return 0;
  if (x < 0) {
    x=changeSign(x);
    negative=!negative;
  }
  if (y < 0) {
    y=changeSign(y);
    negative=!negative;
  }
  int answer=x;
  for (int i=1; i < y; i++) {
    answer=add(answer,x);
  }
  return negative ? changeSign(answer) : answer;
}

