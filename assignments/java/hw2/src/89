public int multiply(int x,int y){
  int increment=x;
  int stored=0;
  boolean multiplyneg=false;
  if ((x < 0) && (y < 0)) {
    x=this.add(~x,1);
    y=this.add(~y,1);
  }
 else   if (y < 0) {
    multiplyneg=true;
    y=this.add(~y,1);
  }
  if ((x == 0) || (y == 0)) {
    return 0;
  }
  while (y > 1) {
    if ((y & 1) == 1) {
      stored=this.add(stored,x);
    }
    x=x << 1;
    y=y >>> 1;
  }
  x=this.add(x,stored);
  if (multiplyneg) {
    x=~this.add(x,-1);
  }
  return x;
}

