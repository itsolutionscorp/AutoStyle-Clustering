public int multiply(int x,int y){
  int xmul=x;
  int ymul=y;
  int result=0;
  if (xmul == 0 || ymul == 0) {
    return 0;
  }
  while (ymul != 0) {
    if ((ymul & 1) == 1) {
      result=add(xmul,result);
      xmul=xmul << 1;
      ymul=ymul >>> 1;
    }
 else {
      xmul=xmul << 1;
      ymul=ymul >>> 1;
    }
  }
  return result;
}

