public int multiply(int x,int y){
  int result=0;
  int mask=1;
  boolean xIsNegative=false;
  if (x < 0) {
    x=add(~x,1);
    xIsNegative=true;
  }
  for (int i=0; i < 32; i++) {
    if ((mask & x) >> i == 1) {
      result+=y << i;
    }
    mask=mask << 1;
  }
  if (xIsNegative) {
    result=add(~result,1);
  }
  return result;
}

