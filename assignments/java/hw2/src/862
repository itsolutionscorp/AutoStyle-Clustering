public int multiply(int x,int y){
  int total=0;
  int number=0;
  int counter=0;
  while (counter <= 31) {
    if (getBit(y,counter) == 1) {
      number=x;
      number=number << counter;
    }
 else {
      number=0;
    }
    total=add(number,total);
    counter=counter + 1;
  }
  return total;
}

