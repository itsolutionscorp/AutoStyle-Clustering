public int multiply(int x,int y){
  int flag=1;
  if (x == 0 || y == 0)   return 0;
  if (x < 0 && y < 0) {
    x=-x;
    y=-y;
  }
 else   if (x < 0) {
    flag=-1;
    x=-x;
  }
 else   if (y < 0) {
    flag=-1;
    y=-y;
  }
  int sum=0;
  while (y != 0) {
    if ((1 & y) != 0)     sum=add(sum,x);
    x=x << 1;
    y=y >> 1;
  }
  if (flag == 1)   return sum;
  return -sum;
}

