public int multiply(int x,int y){
  int originalX=x;
  if (y == 0) {
    return 0;
  }
 else   if (y == 1) {
    return x;
  }
 else   if (y > 1) {
    int multiplier=1;
    while (multiplier < y) {
      x=x << 1;
      multiplier=multiplier << 1;
    }
    while (multiplier > y) {
      multiplier=add(multiplier,negate(1));
      x=add(x,negate(originalX));
    }
  }
 else {
    return negate(multiply(x,negate(y)));
  }
  return x;
}

