public int multiply(int x,int y){
  int counter=0;
  boolean negative=false;
  while (y < 0) {
    negative=true;
    y=add(y,1);
    counter=add(counter,1);
  }
  while (counter > 0) {
    y=add(y,1);
    counter=add(counter,-1);
  }
  int val=0;
  while (y > 0) {
    val=add(x,val);
    y=add(y,-1);
  }
  if (negative) {
    val=add(val ^ -1,1);
  }
  return val;
}

