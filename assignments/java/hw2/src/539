public int multiply(int x,int y){
  if (y == 0) {
    return 0;
  }
 else   if (y < 0) {
    return this.multiply(this.add(~x,1),this.add(~y,1));
  }
 else   if (y % 2 == 0) {
    return this.add(this.multiply(x,y >>> 1),this.multiply(x,y >>> 1));
  }
 else {
    return this.add(x,this.multiply(x,this.add(y,-1)));
  }
}

