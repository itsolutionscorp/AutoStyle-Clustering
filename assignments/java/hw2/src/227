public int multiply(int x,int y){
  if ((x == 0) || (y == 0)) {
    return 0;
  }
 else   if (x == 1) {
    return y;
  }
 else   if (y == 1) {
    return x;
  }
 else {
    int product=0;
    int mask=1;
    int count=0;
    while (y != 0) {
      int check=(y & mask);
      if (check != 0) {
        product+=x;
      }
      x=x << 1;
      y=y >> 1;
    }
    return product;
  }
}

