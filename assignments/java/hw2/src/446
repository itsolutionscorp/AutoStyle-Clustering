public int multiply(int x,int y){
  int a=x;
  int b=y;
  if ((a == 0) || (b == 0)) {
    return 0;
  }
 else   if (a == 1) {
    return b;
  }
 else   if (b == 1) {
    return a;
  }
 else   if ((b & 1) == 0) {
    return ((multiply(a,b >>> 1)) << 1);
  }
 else {
    return add(a,(multiply(a,b >>> 1) << 1));
  }
}

