public int multiply(int x,int y){
  int result=0;
  if (x < 0 && y < 0) {
    x=add(~x,1);
    y=add(~y,1);
  }
  if (0 > y) {
    int tmp=y;
    y=x;
    x=tmp;
  }
  while (y != 0) {
    if ((y & 01) != 0) {
      result=add(result,x);
      y-=1;
    }
    x<<=1;
    y>>=1;
  }
  return result;
}

