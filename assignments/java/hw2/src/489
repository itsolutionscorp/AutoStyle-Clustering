public int multiply(int x,int y){
  if (y == 0) {
    return 0;
  }
  if (x == 0) {
    return 0;
  }
  if (y == 1) {
    return x;
  }
  if (x == 1) {
    return y;
  }
  if (x < 0 && y > 0) {
    return multiply(add(1,~x),add(1,~y));
  }
  if (x > 0 && y < 0) {
    return add(add(1,~x),multiply(x,add(y,1)));
  }
  if (x < 0 && y < 0) {
    return multiply(add(1,~x),add(1,~y));
  }
 else {
    return add(x,multiply(x,add(y,-1)));
  }
}

