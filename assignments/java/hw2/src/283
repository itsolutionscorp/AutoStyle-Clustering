public int multiply(int x,int y){
  int product=0;
  boolean negative=false;
  if (x < 0) {
    x=add(~x,1);
    if (y > 0) {
      negative=true;
    }
 else {
      y=add(~y,1);
    }
  }
 else   if (y < 0) {
    negative=true;
    y=add(~y,1);
  }
  while (y > 0) {
    if (LastDigit(y) == 1) {
      product=add(product,x);
    }
    x<<=1;
    y>>>=1;
  }
  if (negative) {
    product=add(~product,1);
  }
  return product;
}

