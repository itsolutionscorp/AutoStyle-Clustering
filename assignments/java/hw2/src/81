public int multiply(int x,int y){
  if (x == 0 || y == 0) {
    return 0;
  }
  int fixedVal=x;
  int index=1;
  int yAbs=(y ^ (y >> 31)) - (y >> 31);
  while (index < yAbs) {
    x=add(x,fixedVal);
    index+=1;
  }
  int xAbs=(x ^ (x >> 31)) - (x >> 31);
  if ((x < 0 && y < 0) || (x > 0 && y > 0)) {
    return xAbs;
  }
  return -xAbs;
}

