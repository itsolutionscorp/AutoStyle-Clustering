public static void main(String[] args){
  Scanner scanner=new Scanner(System.in);
  boolean justStarting=true;
  int total=0;
  int subtotal=0;
  while (true) {
    int k=scanner.nextInt();
    while (k != 0) {
      if (justStarting) {
        justStarting=false;
      }
      subtotal+=k;
      total+=k;
      k=scanner.nextInt();
    }
    if (justStarting) {
      System.out.println("subtotal " + subtotal);
    }
 else {
      if (subtotal == 0) {
        System.out.println("total " + total);
        return;
      }
 else {
        System.out.println("subtotal " + subtotal);
        subtotal=0;
      }
    }
  }
}

