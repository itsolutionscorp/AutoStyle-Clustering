public static void main(String[] args){
  Scanner scanner=new Scanner(System.in);
  boolean justStarting=true;
  int total=0;
  int subtotal=0;
  int prev=1;
  int k;
  while (true) {
    k=scanner.nextInt();
    if (k == 0) {
      if (prev == 0) {
        System.out.println("total" + " " + total);
        return;
      }
 else {
        System.out.println("subtotal" + " " + subtotal);
        subtotal=0;
      }
    }
 else {
      total=total + k;
      subtotal=subtotal + k;
    }
    prev=k;
  }
}

