s(:defn, :combine_anagrams, s(:args, :words), s(:lasgn, :output_ary, s(:call, s(:const, :Array), :new)), s(:lasgn, :sorted_words_ary, s(:call, s(:const, :Array), :new)), s(:iter, s(:call, s(:lvar, :words), :each), s(:args, :w), s(:block, s(:lasgn, :word_ary, s(:call, s(:const, :Array), :new)), s(:lasgn, :sorted_word, s(:call, s(:const, :String), :new)), s(:iter, s(:call, s(:call, s(:lvar, :w), :downcase), :each_char), s(:args, :c), s(:call, s(:lvar, :word_ary), :<<, s(:lvar, :c))), s(:call, s(:lvar, :word_ary), :sort!), s(:iter, s(:call, s(:lvar, :word_ary), :each), s(:args, :s), s(:lasgn, :sorted_word, s(:call, s(:lvar, :sorted_word), :+, s(:lvar, :s)))), s(:call, s(:lvar, :sorted_words_ary), :<<, s(:lvar, :sorted_word)))), s(:lasgn, :anagram_pos_hash, s(:call, s(:const, :Hash), :new)), s(:lasgn, :output_ary_index, s(:lit, 0)), s(:iter, s(:call, s(:lit, 0), :upto, s(:call, s(:call, s(:lvar, :sorted_words_ary), :length), :-, s(:lit, 1))), s(:args, :i), s(:block, s(:lasgn, :cur_anagram, s(:call, s(:lvar, :sorted_words_ary), :[], s(:lvar, :i))), s(:lasgn, :cur_word, s(:call, s(:lvar, :words), :[], s(:lvar, :i))), s(:lasgn, :match_pos, s(:call, s(:lvar, :sorted_words_ary), :index, s(:lvar, :cur_anagram))), s(:if, s(:call, s(:lvar, :match_pos), :==, s(:lvar, :i)), s(:block, s(:call, s(:lvar, :output_ary), :<<, s(:call, nil, :Array, s(:lvar, :cur_word))), s(:attrasgn, s(:lvar, :anagram_pos_hash), :[]=, s(:lvar, :cur_anagram), s(:lvar, :output_ary_index)), s(:lasgn, :output_ary_index, s(:call, s(:lvar, :output_ary_index), :+, s(:lit, 1)))), s(:block, s(:lasgn, :output_ary_pos, s(:call, s(:lvar, :anagram_pos_hash), :[], s(:lvar, :cur_anagram))), s(:call, s(:call, s(:lvar, :output_ary), :[], s(:lvar, :output_ary_pos)), :<<, s(:lvar, :cur_word)))))), s(:lvar, :output_ary))