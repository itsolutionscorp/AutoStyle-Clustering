s(:defn, :combine_anagrams, s(:args, :words), s(:lasgn, :anagrams, s(:array)), s(:iter, s(:call, s(:lvar, :words), :each), s(:args, :w), s(:if, s(:call, s(:call, s(:lvar, :anagrams), :length), :==, s(:lit, 0)), s(:call, s(:lvar, :anagrams), :<<, s(:array, s(:lvar, :w))), s(:block, s(:lasgn, :wCanonical, s(:call, s(:call, s(:call, s(:call, s(:lvar, :w), :downcase), :split, s(:str, "")), :sort), :join)), s(:lasgn, :gotit, s(:false)), s(:iter, s(:call, s(:lvar, :anagrams), :each), s(:args, :a), s(:if, s(:call, s(:call, s(:call, s(:call, s(:call, s(:call, s(:lvar, :a), :[], s(:lit, 0)), :downcase), :split, s(:str, "")), :sort), :join), :==, s(:lvar, :wCanonical)), s(:block, s(:call, s(:lvar, :a), :<<, s(:lvar, :w)), s(:lasgn, :gotit, s(:true)), s(:break)), nil)), s(:if, s(:call, s(:lvar, :gotit), :!), s(:call, s(:lvar, :anagrams), :<<, s(:array, s(:lvar, :w))), nil)))), s(:return, s(:lvar, :anagrams)))