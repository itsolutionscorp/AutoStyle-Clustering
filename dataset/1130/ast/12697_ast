s(:defn, :combine_anagrams, s(:args, :words), s(:lasgn, :check1, s(:call, s(:lvar, :words), :dup)), s(:lasgn, :check2, s(:call, s(:lvar, :words), :dup)), s(:call, s(:lvar, :check2), :shift), s(:lasgn, :temp_array, s(:array)), s(:lasgn, :new_array, s(:array)), s(:iter, s(:call, s(:lvar, :check1), :each), s(:args, :ch1), s(:block, s(:call, s(:lvar, :temp_array), :push, s(:lvar, :ch1)), s(:iter, s(:call, s(:lvar, :check2), :each), s(:args, :ch2), s(:if, s(:call, s(:call, s(:call, s(:call, s(:lvar, :ch1), :chars), :sort), :join), :==, s(:call, s(:call, s(:call, s(:lvar, :ch2), :chars), :sort), :join)), s(:block, s(:call, s(:lvar, :temp_array), :push, s(:lvar, :ch2)), s(:call, s(:lvar, :check1), :delete, s(:lvar, :ch2)), s(:call, s(:lvar, :check2), :delete, s(:lvar, :ch2))), nil)), s(:call, s(:lvar, :check2), :shift), s(:call, s(:lvar, :new_array), :push, s(:lvar, :temp_array)), s(:lasgn, :temp_array, s(:array)))), s(:lvar, :new_array))