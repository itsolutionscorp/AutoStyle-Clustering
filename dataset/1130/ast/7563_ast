s(:defn, :combine_anagrams, s(:args, :words), s(:if, s(:call, s(:lvar, :words), :kind_of?, s(:const, :Array)), s(:block, s(:lasgn, :word, s(:call, s(:const, :Array), :new)), s(:lasgn, :new_words, s(:lvar, :words)), s(:iter, s(:call, s(:lvar, :words), :each), s(:args, :w), s(:iter, s(:call, s(:lvar, :new_words), :each), s(:args, :nw), s(:block, s(:lasgn, :w_arr, s(:call, s(:lvar, :w), :split, s(:lit, //))), s(:lasgn, :nw_arr, s(:call, s(:lvar, :nw), :split, s(:lit, //))), s(:lasgn, :w_arr, s(:call, s(:lvar, :w_arr), :sort)), s(:lasgn, :nw_arr, s(:call, s(:lvar, :nw_arr), :sort)), s(:if, s(:call, s(:lvar, :w_arr), :==, s(:lvar, :nw_arr)), s(:block, s(:lasgn, :nw_word, s(:array, s(:lvar, :w), s(:lvar, :nw))), s(:call, s(:lvar, :word), :<<, s(:call, s(:call, s(:lvar, :nw_word), :sort), :uniq))), nil)))), s(:call, s(:call, s(:lvar, :word), :sort), :uniq), s(:call, nil, :print, s(:lvar, :word))), s(:call, nil, :puts, s(:str, "This is not an Array"))))