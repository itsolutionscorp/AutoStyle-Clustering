s(:block, s(:defn, :combine_anagrams, s(:args, :words), s(:lasgn, :a, s(:array)), s(:iter, s(:call, s(:lvar, :words), :each), s(:args, :i), s(:block, s(:lasgn, :b, s(:array)), s(:iter, s(:call, s(:lvar, :words), :each), s(:args, :j), s(:if, s(:call, s(:call, s(:call, s(:call, s(:lvar, :i), :downcase), :split, s(:str, "")), :sort), :==, s(:call, s(:call, s(:call, s(:lvar, :j), :downcase), :split, s(:str, "")), :sort)), s(:call, s(:lvar, :b), :<<, s(:lvar, :j)), nil)), s(:call, s(:lvar, :a), :<<, s(:lvar, :b)))), s(:return, s(:call, s(:lvar, :a), :uniq))))