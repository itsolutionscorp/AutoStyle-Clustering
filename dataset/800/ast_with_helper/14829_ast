s(:block, s(:defn, :combine_anagrams, s(:args, :words), s(:lasgn, :k, s(:lit, 0)), s(:lasgn, :anagrams, s(:call, s(:const, :Array), :new, s(:lit, 0))), s(:lasgn, :is_in_anagrams, s(:call, s(:const, :Array), :new, s(:call, s(:lvar, :words), :length), s(:false))), s(:for, s(:dot2, s(:lit, 0), s(:call, s(:call, s(:lvar, :words), :length), :-, s(:lit, 1))), s(:lasgn, :i), s(:block, s(:if, s(:call, s(:lvar, :is_in_anagrams), :[], s(:lvar, :i)), s(:next), s(:block, s(:lasgn, :k, s(:call, s(:lvar, :k), :+, s(:lit, 1))), s(:lasgn, :l, s(:lit, 0)), s(:lasgn, :anagram_component, s(:call, s(:const, :Array), :new, s(:lit, 0))), s(:attrasgn, s(:lvar, :anagram_component), :[]=, s(:lvar, :l), s(:call, s(:lvar, :words), :[], s(:lvar, :i))))), s(:for, s(:dot2, s(:call, s(:lvar, :i), :+, s(:lit, 1)), s(:call, s(:call, s(:lvar, :words), :length), :-, s(:lit, 1))), s(:lasgn, :j), s(:if, s(:call, s(:call, s(:call, s(:call, s(:call, s(:call, s(:lvar, :words), :[], s(:lvar, :i)), :downcase), :chars), :sort), :join), :==, s(:call, s(:call, s(:call, s(:call, s(:call, s(:lvar, :words), :[], s(:lvar, :j)), :downcase), :chars), :sort), :join)), s(:block, s(:lasgn, :l, s(:call, s(:lvar, :l), :+, s(:lit, 1))), s(:attrasgn, s(:lvar, :anagram_component), :[]=, s(:lvar, :l), s(:call, s(:lvar, :words), :[], s(:lvar, :j))), s(:attrasgn, s(:lvar, :is_in_anagrams), :[]=, s(:lvar, :j), s(:true))), nil)), s(:call, s(:lvar, :anagrams), :<<, s(:lvar, :anagram_component)))), s(:return, s(:call, s(:lvar, :anagrams), :sort))))