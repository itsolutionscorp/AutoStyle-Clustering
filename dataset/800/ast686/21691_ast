s(:defn, :combine_anagrams, s(:args, :words), s(:lasgn, :words_hash, s(:hash)), s(:for, s(:lvar, :words), s(:lasgn, :word), s(:block, s(:if, s(:call, s(:call, s(:lvar, :words_hash), :[], s(:call, s(:call, s(:call, s(:call, s(:lvar, :word), :downcase), :chars), :sort), :join)), :==, s(:nil)), s(:attrasgn, s(:lvar, :words_hash), :[]=, s(:call, s(:call, s(:call, s(:call, s(:lvar, :word), :downcase), :chars), :sort), :join), s(:array)), nil), s(:call, s(:call, s(:lvar, :words_hash), :[], s(:call, s(:call, s(:call, s(:call, s(:lvar, :word), :downcase), :chars), :sort), :join)), :<<, s(:lvar, :word)))), s(:return, s(:call, s(:lvar, :words_hash), :values)))