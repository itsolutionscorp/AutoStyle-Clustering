s(:defn, :combine_anagrams, s(:args, :words), s(:lasgn, :temp, s(:array)), s(:iter, s(:call, s(:lvar, :words), :each), s(:args, :word), s(:block, s(:lasgn, :w, s(:call, s(:call, s(:call, s(:call, s(:call, s(:lvar, :word), :downcase), :split, s(:lit, //)), :uniq), :sort), :join)), s(:call, s(:lvar, :temp), :<<, s(:lvar, :w)))), s(:lasgn, :finalgrp, s(:array)), s(:lasgn, :tempgrp, s(:array)), s(:lasgn, :blacklist, s(:array)), s(:for, s(:dot2, s(:lit, 0), s(:call, s(:call, s(:lvar, :temp), :length), :-, s(:lit, 1))), s(:lasgn, :i), s(:block, s(:if, s(:call, s(:lvar, :blacklist), :include?, s(:lvar, :i)), s(:next), s(:block, s(:call, s(:lvar, :tempgrp), :<<, s(:call, s(:lvar, :words), :[], s(:lvar, :i))), s(:call, s(:lvar, :blacklist), :<<, s(:lvar, :i)))), s(:for, s(:dot2, s(:call, s(:lvar, :i), :+, s(:lit, 1)), s(:call, s(:lvar, :temp), :length)), s(:lasgn, :j), s(:if, s(:and, s(:call, s(:call, s(:lvar, :temp), :[], s(:lvar, :i)), :==, s(:call, s(:lvar, :temp), :[], s(:lvar, :j))), s(:call, s(:call, s(:lvar, :blacklist), :include?, s(:lvar, :j)), :!)), s(:block, s(:call, s(:lvar, :tempgrp), :<<, s(:call, s(:lvar, :words), :[], s(:lvar, :j))), s(:call, s(:lvar, :blacklist), :<<, s(:lvar, :j))), nil)), s(:call, s(:lvar, :finalgrp), :<<, s(:lvar, :tempgrp)), s(:lasgn, :tempgrp, s(:array)))), s(:return, s(:lvar, :finalgrp)))