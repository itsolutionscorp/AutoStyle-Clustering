s(:defn, :combine_anagrams, s(:args, :words), s(:lasgn, :anagrams, s(:call, s(:const, :Array), :new)), s(:lasgn, :sorted_words_array, s(:call, s(:const, :Array), :new)), s(:iter, s(:call, s(:lvar, :words), :each), s(:args, :word), s(:block, s(:lasgn, :anagrams_word, s(:call, s(:call, s(:call, s(:call, s(:lvar, :word), :downcase), :chars), :sort, s(:block_pass, s(:lit, :casecmp))), :join)), s(:if, s(:call, s(:lvar, :sorted_words_array), :include?, s(:lvar, :anagrams_word)), s(:call, s(:call, s(:lvar, :anagrams), :[], s(:call, s(:lvar, :sorted_words_array), :index, s(:lvar, :anagrams_word))), :insert, s(:lit, -1), s(:lvar, :word)), s(:block, s(:call, s(:lvar, :sorted_words_array), :insert, s(:lit, -1), s(:lvar, :anagrams_word)), s(:call, s(:lvar, :anagrams), :insert, s(:lit, -1), s(:call, s(:const, :Array), :[], s(:lvar, :word))))))), s(:lvar, :anagrams))