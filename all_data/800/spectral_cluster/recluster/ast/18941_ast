s(:defn, :combine_anagrams, s(:args, :words), s(:lasgn, :list, s(:lvar, :words)), s(:lasgn, :anagrams, s(:array)), s(:lasgn, :cntGrp, s(:lit, 0)), s(:for, s(:lvar, :words), s(:lasgn, :x), s(:block, s(:lasgn, :xSrtd, s(:call, s(:iter, s(:call, s(:call, s(:lvar, :x), :chars), :sort), s(:args, :a, :b), s(:call, s(:lvar, :a), :casecmp, s(:lvar, :b))), :join)), s(:call, s(:lvar, :anagrams), :push, s(:array, s(:lvar, :x))), s(:for, s(:lvar, :list), s(:lasgn, :y), s(:block, s(:lasgn, :ySrtd, s(:call, s(:iter, s(:call, s(:call, s(:lvar, :y), :chars), :sort), s(:args, :a, :b), s(:call, s(:lvar, :a), :casecmp, s(:lvar, :b))), :join)), s(:if, s(:call, s(:lvar, :xSrtd), :==, s(:lvar, :ySrtd)), s(:if, s(:call, s(:lvar, :x), :!=, s(:lvar, :y)), s(:block, s(:call, s(:call, s(:lvar, :anagrams), :[], s(:lvar, :cntGrp)), :push, s(:lvar, :y)), s(:call, s(:lvar, :list), :delete_at, s(:call, s(:lvar, :list), :index, s(:lvar, :y)))), nil), nil))), s(:lasgn, :cntGrp, s(:call, s(:lvar, :cntGrp), :+, s(:lit, 1))))), s(:return, s(:lvar, :anagrams)))