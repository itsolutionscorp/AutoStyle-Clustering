s(:block, s(:defn, :combine_anagrams, s(:args, :words), s(:if, s(:call, s(:lvar, :words), :==, s(:array)), s(:return, s(:array)), nil), s(:lasgn, :array_of_anagrams, s(:array, s(:array, s(:call, s(:lvar, :words), :[], s(:lit, 0))))), s(:lasgn, :words, s(:call, s(:lvar, :words), :drop, s(:lit, 1))), s(:iter, s(:call, s(:lvar, :words), :each), s(:args, :word), s(:block, s(:lasgn, :found, s(:lit, 0)), s(:lasgn, :word_clean, s(:call, s(:call, s(:call, s(:call, s(:lvar, :word), :downcase), :chars), :sort), :join)), s(:iter, s(:call, s(:lvar, :array_of_anagrams), :each), s(:args, :line_of_anagrams), s(:if, s(:call, s(:call, s(:call, s(:call, s(:call, s(:call, s(:lvar, :line_of_anagrams), :[], s(:lit, 0)), :downcase), :chars), :sort), :join), :==, s(:lvar, :word_clean)), s(:block, s(:call, s(:lvar, :line_of_anagrams), :push, s(:lvar, :word)), s(:lasgn, :found, s(:lit, 1))), nil)), s(:if, s(:call, s(:lvar, :found), :==, s(:lit, 0)), s(:call, s(:lvar, :array_of_anagrams), :push, s(:array, s(:lvar, :word))), nil))), s(:return, s(:lvar, :array_of_anagrams))))