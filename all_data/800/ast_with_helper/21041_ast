s(:block, s(:defn, :combine_anagrams, s(:args, :words), s(:lasgn, :comb, s(:array)), s(:iter, s(:call, s(:lvar, :words), :each), s(:args, :ana), s(:if, s(:call, s(:call, s(:lvar, :comb), :size), :==, s(:lit, 0)), s(:call, s(:lvar, :comb), :<<, s(:array, s(:lvar, :ana))), s(:block, s(:lasgn, :in_list, s(:false)), s(:iter, s(:call, s(:lvar, :comb), :each), s(:args, :in_ana), s(:if, s(:call, s(:call, s(:call, s(:call, s(:call, s(:call, s(:lvar, :in_ana), :[], s(:lit, 0)), :downcase), :chars), :sort), :join), :==, s(:call, s(:call, s(:call, s(:call, s(:lvar, :ana), :downcase), :chars), :sort), :join)), s(:block, s(:call, s(:call, s(:lvar, :comb), :[], s(:call, s(:lvar, :comb), :index, s(:lvar, :in_ana))), :<<, s(:lvar, :ana)), s(:lasgn, :in_list, s(:true))), nil)), s(:if, s(:call, s(:lvar, :in_list), :!), s(:call, s(:lvar, :comb), :<<, s(:array, s(:lvar, :ana))), nil)))), s(:return, s(:lvar, :comb))))