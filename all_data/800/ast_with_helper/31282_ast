s(:block, s(:defn, :are_anagrams?, s(:args, :w1, :w2), s(:return, s(:if, s(:call, s(:call, s(:call, s(:call, s(:lvar, :w1), :downcase), :split, s(:str, "")), :sort), :==, s(:call, s(:call, s(:call, s(:lvar, :w2), :downcase), :split, s(:str, "")), :sort)), s(:true), s(:false)))), s(:defn, :combine_anagrams, s(:args, :words), s(:lasgn, :groups, s(:array)), s(:lasgn, :added, s(:false)), s(:iter, s(:call, s(:lvar, :words), :each), s(:args, :word), s(:block, s(:lasgn, :added, s(:false)), s(:iter, s(:call, s(:lvar, :groups), :each), s(:args, :group), s(:if, s(:call, nil, :are_anagrams?, s(:call, s(:lvar, :group), :[], s(:lit, 0)), s(:lvar, :word)), s(:block, s(:call, s(:lvar, :group), :push, s(:lvar, :word)), s(:lasgn, :added, s(:true)), s(:break)), nil)), s(:if, s(:call, s(:lvar, :added), :!), s(:call, s(:lvar, :groups), :push, s(:array, s(:lvar, :word))), nil))), s(:return, s(:lvar, :groups))))