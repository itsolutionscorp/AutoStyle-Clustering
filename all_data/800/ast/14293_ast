s(:defn, :combine_anagrams, s(:args, :words), s(:lasgn, :loopit, s(:if, s(:call, s(:lvar, :words), :==, s(:array)), s(:false), s(:true))), s(:lasgn, :sorted_words, s(:array)), s(:while, s(:lvar, :loopit), s(:block, s(:lasgn, :current_loop, s(:array)), s(:lasgn, :current_word_sorted, s(:call, s(:call, s(:call, s(:call, s(:call, s(:lvar, :words), :first), :downcase), :chars), :sort), :join)), s(:iter, s(:call, s(:lvar, :words), :each), s(:args, :word), s(:if, s(:call, s(:lvar, :current_word_sorted), :==, s(:call, s(:call, s(:call, s(:call, s(:lvar, :word), :downcase), :chars), :sort), :join)), s(:call, s(:lvar, :current_loop), :<<, s(:lvar, :word)), nil)), s(:call, s(:lvar, :sorted_words), :<<, s(:lvar, :current_loop)), s(:lasgn, :words, s(:call, s(:lvar, :words), :-, s(:lvar, :current_loop))), s(:if, s(:call, s(:lvar, :words), :==, s(:array)), s(:lasgn, :loopit, s(:false)), nil)), true), s(:return, s(:call, s(:lvar, :sorted_words), :sort)))