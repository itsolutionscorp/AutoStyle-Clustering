s(:defn, :compute, s(:args, :original, :current), s(:lasgn, :o, s(:call, s(:lvar, :original), :split, s(:str, ""))), s(:lasgn, :c, s(:call, s(:lvar, :current), :split, s(:str, ""))), s(:lasgn, :hamming_counter, s(:lit, 0)), s(:iter, s(:call, s(:lvar, :o), :each_with_index), s(:args, :letter, :index), s(:if, s(:call, s(:lvar, :letter), :!=, s(:call, s(:lvar, :c), :[], s(:lvar, :index))), s(:lasgn, :hamming_counter, s(:call, s(:lvar, :hamming_counter), :+, s(:lit, 1))), nil)), s(:lvar, :hamming_counter))