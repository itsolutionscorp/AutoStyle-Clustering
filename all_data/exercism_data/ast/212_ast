s(:defn, :compute, s(:args, :dna1, :dna2), s(:lasgn, :count, s(:lit, 0)), s(:iter, s(:call, s(:call, s(:lvar, :dna1), :chars), :each_with_index), s(:args, :c, :i), s(:if, s(:call, s(:lvar, :i), :<, s(:call, s(:lvar, :dna2), :length)), s(:if, s(:call, s(:lvar, :c), :!=, s(:call, s(:lvar, :dna2), :[], s(:lvar, :i))), s(:lasgn, :count, s(:call, s(:lvar, :count), :+, s(:lit, 1))), nil), nil)), s(:lvar, :count))