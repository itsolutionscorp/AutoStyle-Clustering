s(:defn, :compute, s(:args, :first_dna, :second_dna), s(:lasgn, :first_arr, s(:if, s(:call, s(:call, s(:lvar, :first_dna), :length), :>, s(:call, s(:lvar, :second_dna), :length)), s(:call, s(:lvar, :second_dna), :chars), s(:call, s(:lvar, :first_dna), :chars))), s(:iter, s(:call, s(:call, s(:lvar, :first_arr), :each_with_index), :inject, s(:lit, 0)), s(:args, :acc, s(:masgn, :value, :index)), s(:block, s(:if, s(:call, s(:call, s(:lvar, :first_dna), :[], s(:lvar, :index)), :!=, s(:call, s(:lvar, :second_dna), :[], s(:lvar, :index))), s(:lasgn, :acc, s(:call, s(:lvar, :acc), :+, s(:lit, 1))), nil), s(:lvar, :acc))))