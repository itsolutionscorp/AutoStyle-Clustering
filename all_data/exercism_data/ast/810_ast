s(:defn, :compute, s(:args, :num1, :num2), s(:lasgn, :count, s(:lit, 0)), s(:lasgn, :str1, s(:call, s(:call, s(:lvar, :num1), :to_s), :split, s(:str, ""))), s(:lasgn, :str2, s(:call, s(:call, s(:lvar, :num2), :to_s), :split, s(:str, ""))), s(:iter, s(:call, s(:call, s(:lvar, :str1), :length), :times), s(:args, :i), s(:if, s(:call, s(:call, s(:lvar, :str1), :[], s(:lvar, :i)), :!=, s(:call, s(:lvar, :str2), :[], s(:lvar, :i))), s(:lasgn, :count, s(:call, s(:lvar, :count), :+, s(:lit, 1))), nil)), s(:lvar, :count))