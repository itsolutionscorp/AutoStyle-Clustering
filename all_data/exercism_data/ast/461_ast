s(:defn, :compute, s(:args, :strand_one, :strand_two), s(:lasgn, :hamming_distance, s(:lit, 0)), s(:while, s(:true), s(:block, s(:lasgn, :strand_one_val, s(:call, s(:lvar, :strand_one), :slice!, s(:lit, 0))), s(:lasgn, :strand_two_val, s(:call, s(:lvar, :strand_two), :slice!, s(:lit, 0))), s(:if, s(:or, s(:call, s(:lvar, :strand_one_val), :nil?), s(:call, s(:lvar, :strand_two_val), :nil?)), s(:return, s(:lvar, :hamming_distance)), nil), s(:if, s(:call, s(:lvar, :strand_one_val), :!=, s(:lvar, :strand_two_val)), s(:lasgn, :hamming_distance, s(:call, s(:lvar, :hamming_distance), :+, s(:lit, 1))), nil)), true))