s(:defn, :compute, s(:args, :strand1, :strand2), s(:lasgn, :distance, s(:lit, 0)), s(:if, s(:call, s(:lvar, :strand1), :==, s(:lvar, :strand2)), s(:return, s(:lvar, :distance)), nil), s(:lasgn, :max, s(:if, s(:call, s(:call, s(:lvar, :strand1), :length), :>, s(:call, s(:lvar, :strand2), :length)), s(:call, s(:lvar, :strand2), :length), s(:call, s(:lvar, :strand1), :length))), s(:for, s(:dot2, s(:lit, 0), s(:call, s(:lvar, :max), :-, s(:lit, 1))), s(:lasgn, :i), s(:lasgn, :distance, s(:call, s(:lvar, :distance), :+, s(:if, s(:call, s(:call, s(:lvar, :strand1), :[], s(:lvar, :i)), :==, s(:call, s(:lvar, :strand2), :[], s(:lvar, :i))), s(:lit, 0), s(:lit, 1))))), s(:return, s(:lvar, :distance)))