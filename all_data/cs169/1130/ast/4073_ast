s(:defn, :combine_anagrams, s(:args, :words), s(:if, s(:call, s(:call, s(:lvar, :words), :length), :==, s(:lit, 0)), s(:return, s(:array)), nil), s(:lasgn, :sorted, s(:array)), s(:lasgn, :indexed, s(:array)), s(:iter, s(:call, s(:lvar, :words), :each), s(:args, :w), s(:call, s(:lvar, :sorted), :<<, s(:call, s(:call, s(:call, s(:call, s(:lvar, :w), :downcase), :chars), :sort), :join))), s(:iter, s(:call, s(:lvar, :sorted), :each_index), s(:args, :i), s(:call, s(:lvar, :indexed), :<<, s(:array, s(:call, s(:lvar, :sorted), :[], s(:lvar, :i)), s(:lvar, :i)))), s(:iter, s(:call, s(:lvar, :indexed), :sort!), s(:args, :x, :y), s(:call, s(:call, s(:lvar, :x), :[], s(:lit, 0)), :<=>, s(:call, s(:lvar, :y), :[], s(:lit, 0)))), s(:lasgn, :ret, s(:array)), s(:lasgn, :curset, s(:array)), s(:lasgn, :cur, s(:str, "")), s(:iter, s(:call, s(:lvar, :indexed), :each), s(:args, :x), s(:if, s(:call, s(:call, s(:lvar, :x), :[], s(:lit, 0)), :!=, s(:lvar, :cur)), s(:block, s(:if, s(:call, s(:lvar, :cur), :!=, s(:str, "")), s(:call, s(:lvar, :ret), :<<, s(:lvar, :curset)), nil), s(:lasgn, :curset, s(:array, s(:call, s(:lvar, :words), :[], s(:call, s(:lvar, :x), :[], s(:lit, 1))))), s(:lasgn, :cur, s(:call, s(:lvar, :x), :[], s(:lit, 0)))), s(:call, s(:lvar, :curset), :<<, s(:call, s(:lvar, :words), :[], s(:call, s(:lvar, :x), :[], s(:lit, 1)))))), s(:call, s(:lvar, :ret), :<<, s(:lvar, :curset)), s(:return, s(:lvar, :ret)))