s(:defn, :combine_anagrams, s(:args, :words), s(:lasgn, :anagram_list, s(:array)), s(:iter, s(:call, s(:lvar, :words), :each), s(:args, :word), s(:block, s(:lasgn, :current_list, s(:array)), s(:iter, s(:call, s(:lvar, :words), :each), s(:args, :other_word), s(:if, s(:call, s(:call, s(:call, s(:call, s(:lvar, :word), :downcase), :split, s(:str, "")), :sort), :==, s(:call, s(:call, s(:call, s(:lvar, :other_word), :downcase), :split, s(:str, "")), :sort)), s(:call, s(:lvar, :current_list), :<<, s(:lvar, :other_word)), nil)), s(:call, s(:lvar, :anagram_list), :<<, s(:call, s(:lvar, :current_list), :sort)))), s(:return, s(:call, s(:lvar, :anagram_list), :uniq)))