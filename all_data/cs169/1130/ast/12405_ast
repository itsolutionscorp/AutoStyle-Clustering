s(:defn, :combine_anagrams, s(:args, :words), s(:lasgn, :output, s(:array)), s(:if, s(:call, s(:call, s(:lvar, :words), :length), :==, s(:lit, 0)), s(:return, s(:lvar, :output)), nil), s(:iter, s(:call, s(:lvar, :words), :each), s(:args, :item), s(:block, s(:lasgn, :s_string, s(:call, s(:call, s(:call, s(:call, s(:call, s(:lvar, :item), :downcase), :chars), :sort), :join), :to_s)), s(:lasgn, :temp_arry, s(:array)), s(:iter, s(:call, s(:lvar, :words), :each), s(:args, :scan), s(:if, s(:call, s(:lvar, :s_string), :==, s(:call, s(:call, s(:call, s(:call, s(:call, s(:lvar, :scan), :downcase), :chars), :sort), :join), :to_s)), s(:call, s(:lvar, :temp_arry), :<<, s(:lvar, :scan)), nil)), s(:if, s(:call, s(:lvar, :output), :include?, s(:lvar, :temp_arry)), nil, s(:call, s(:lvar, :output), :<<, s(:lvar, :temp_arry))))), s(:return, s(:lvar, :output)))