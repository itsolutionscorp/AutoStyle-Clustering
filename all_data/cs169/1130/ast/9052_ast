s(:defn, :combine_anagrams, s(:args, :words), s(:lasgn, :out, s(:array)), s(:iter, s(:call, s(:lvar, :words), :each), s(:args, :w), s(:block, s(:lasgn, :r, s(:dregx, "", s(:evstr, s(:call, s(:call, s(:call, s(:lvar, :w), :chars), :sort), :to_s)), 1)), s(:call, nil, :puts, s(:lvar, :r)), s(:if, s(:call, s(:call, s(:call, s(:lvar, :out), :flatten), :include?, s(:lvar, :w)), :!), s(:call, s(:lvar, :out), :<<, s(:iter, s(:call, s(:lvar, :words), :select), s(:args, :word), s(:call, s(:call, s(:call, s(:call, s(:call, s(:lvar, :word), :downcase), :chars), :sort), :to_s), :==, s(:call, s(:call, s(:call, s(:call, s(:lvar, :w), :downcase), :chars), :sort), :to_s)))), nil))), s(:return, s(:lvar, :out)))