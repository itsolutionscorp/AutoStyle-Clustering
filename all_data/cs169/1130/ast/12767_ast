s(:defn, :combine_anagrams, s(:args, :words), s(:lasgn, :anagram_groups, s(:array)), s(:lasgn, :answer_array, s(:array)), s(:iter, s(:call, s(:lvar, :words), :each), s(:args, :word), s(:block, s(:lasgn, :anagram_group, s(:call, s(:call, s(:call, s(:call, s(:lvar, :word), :downcase), :each_char), :sort), :join)), s(:if, s(:call, s(:call, s(:lvar, :anagram_groups), :include?, s(:lvar, :anagram_group)), :!), s(:block, s(:call, s(:lvar, :anagram_groups), :<<, s(:lvar, :anagram_group)), s(:call, s(:lvar, :answer_array), :<<, s(:array, s(:lvar, :word)))), s(:block, s(:lasgn, :anagram_index, s(:call, s(:lvar, :anagram_groups), :index, s(:lvar, :anagram_group))), s(:call, s(:call, s(:lvar, :answer_array), :[], s(:lvar, :anagram_index)), :<<, s(:lvar, :word)))))), s(:return, s(:lvar, :answer_array)))