s(:defn, :combine_anagrams, s(:args, :words), s(:lasgn, :word_hash, s(:hash)), s(:lasgn, :final_array, s(:array)), s(:lasgn, :i, s(:lit, 0)), s(:iter, s(:call, s(:lvar, :words), :each), s(:args, :word), s(:block, s(:lasgn, :ordered_word, s(:call, s(:call, s(:call, s(:call, s(:lvar, :word), :downcase), :chars), :sort), :join)), s(:if, s(:call, s(:call, s(:lvar, :word_hash), :has_key?, s(:lvar, :ordered_word)), :!), s(:block, s(:call, s(:lvar, :word_hash), :store, s(:lvar, :ordered_word), s(:lvar, :i)), s(:call, s(:lvar, :final_array), :push, s(:array, s(:lvar, :word))), s(:lasgn, :i, s(:call, s(:lvar, :i), :+, s(:lit, 1)))), s(:call, s(:call, s(:lvar, :final_array), :[], s(:call, s(:lvar, :word_hash), :fetch, s(:lvar, :ordered_word))), :push, s(:lvar, :word))))), s(:return, s(:lvar, :final_array)))