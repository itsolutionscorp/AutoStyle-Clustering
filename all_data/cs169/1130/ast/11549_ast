s(:defn, :combine_anagrams, s(:args, :words), s(:lasgn, :index, s(:call, s(:const, :Hash), :new)), s(:lasgn, :data, s(:call, s(:const, :Array), :new)), s(:lasgn, :count, s(:lit, 0)), s(:iter, s(:call, s(:lvar, :words), :each), s(:args, :word), s(:block, s(:lasgn, :normalized_word, s(:call, s(:call, s(:call, s(:call, s(:lvar, :word), :downcase), :chars), :sort), :join)), s(:if, s(:call, s(:call, s(:lvar, :index), :[], s(:lvar, :normalized_word)), :==, s(:nil)), s(:block, s(:call, s(:lvar, :index), :store, s(:lvar, :normalized_word), s(:lvar, :count)), s(:call, s(:lvar, :data), :<<, s(:call, s(:const, :Array), :[], s(:lvar, :word))), s(:lasgn, :count, s(:call, s(:lvar, :count), :+, s(:lit, 1)))), s(:call, s(:call, s(:lvar, :data), :[], s(:call, s(:lvar, :index), :[], s(:lvar, :normalized_word))), :<<, s(:lvar, :word))))), s(:lvar, :data))