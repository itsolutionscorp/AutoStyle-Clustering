s(:defn, :combine_anagrams, s(:args, :words), s(:lasgn, :result, s(:array)), s(:lasgn, :x, s(:call, s(:call, s(:lvar, :words), :length), :-, s(:lit, 1))), s(:lasgn, :t, s(:lit, 0)), s(:for, s(:dot2, s(:lit, 0), s(:lvar, :x)), s(:lasgn, :t), s(:block, s(:if, s(:call, s(:call, s(:str, "*"), :==, s(:call, s(:lvar, :words), :[], s(:lvar, :t))), :!), s(:lasgn, :pomocna, s(:array, s(:call, s(:lvar, :words), :[], s(:lvar, :t)))), nil), s(:for, s(:dot2, s(:call, s(:lvar, :t), :+, s(:lit, 1)), s(:lvar, :x)), s(:lasgn, :i), s(:if, s(:call, s(:call, s(:call, s(:call, s(:call, s(:lvar, :words), :[], s(:lvar, :t)), :downcase), :chars), :sort), :==, s(:call, s(:call, s(:call, s(:call, s(:lvar, :words), :[], s(:lvar, :i)), :downcase), :chars), :sort)), s(:block, s(:if, s(:call, s(:call, s(:str, "*"), :==, s(:call, s(:lvar, :words), :[], s(:lvar, :t))), :!), s(:call, s(:lvar, :pomocna), :<<, s(:call, s(:lvar, :words), :[], s(:lvar, :i))), nil), s(:attrasgn, s(:lvar, :words), :[]=, s(:lvar, :i), s(:str, "*"))), nil)), s(:if, s(:call, s(:lvar, :pomocna), :==, s(:array)), nil, s(:call, s(:lvar, :result), :<<, s(:lvar, :pomocna))), s(:lasgn, :pomocna, s(:array)))), s(:return, s(:lvar, :result)))