s(:block, s(:defn, :anagram?, s(:args, :str1, :str2), s(:lasgn, :array1, s(:call, s(:call, s(:lvar, :str1), :downcase), :split, s(:lit, //))), s(:lasgn, :array2, s(:call, s(:call, s(:lvar, :str2), :downcase), :split, s(:lit, //))), s(:call, s(:lvar, :array1), :sort!), s(:call, s(:lvar, :array2), :sort!), s(:if, s(:call, s(:lvar, :array1), :==, s(:lvar, :array2)), s(:true), s(:false))), s(:defn, :combine_anagrams, s(:args, :words), s(:lasgn, :result, s(:array)), s(:while, s(:call, s(:lvar, :words), :!=, s(:array)), s(:block, s(:lasgn, :cmp_word, s(:call, s(:lvar, :words), :shift)), s(:lasgn, :current_anagram_array, s(:array)), s(:call, s(:lvar, :current_anagram_array), :<<, s(:lvar, :cmp_word)), s(:iter, s(:call, s(:lvar, :words), :delete_if), s(:args, :word), s(:if, s(:call, nil, :anagram?, s(:lvar, :word), s(:lvar, :cmp_word)), s(:block, s(:call, s(:lvar, :current_anagram_array), :<<, s(:lvar, :word)), s(:true)), s(:false))), s(:call, s(:lvar, :result), :<<, s(:lvar, :current_anagram_array))), true), s(:lvar, :result)))