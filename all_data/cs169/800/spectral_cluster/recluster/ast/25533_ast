s(:block, s(:defn, :stringer, s(:args, :x), s(:lasgn, :temp_word, s(:str, "")), s(:iter, s(:call, s(:lvar, :x), :each), s(:args, :y), s(:call, s(:lvar, :temp_word), :<<, s(:lvar, :y))), s(:return, s(:lvar, :temp_word))), s(:defn, :combine_anagrams, s(:args, :words), s(:if, s(:call, s(:lvar, :words), :empty?), s(:return, s(:array)), nil), s(:lasgn, :copy, s(:lvar, :words)), s(:lasgn, :copy, s(:call, s(:iter, s(:call, s(:lvar, :copy), :map), s(:args, :x), s(:block, s(:lasgn, :bank, s(:array)), s(:iter, s(:call, s(:call, s(:lvar, :x), :downcase), :each_char), s(:args, :c), s(:call, s(:lvar, :bank), :<<, s(:lvar, :c))), s(:call, s(:lvar, :bank), :sort))), :uniq)), s(:lasgn, :unique_dict, s(:array)), s(:iter, s(:call, s(:lvar, :copy), :each), s(:args, :x), s(:block, s(:lasgn, :temp_word, s(:call, nil, :stringer, s(:lvar, :x))), s(:call, s(:lvar, :unique_dict), :<<, s(:lvar, :temp_word)))), s(:lasgn, :to_return, s(:array)), s(:iter, s(:call, s(:lvar, :unique_dict), :each), s(:args, :unique_word), s(:call, s(:lvar, :to_return), :<<, s(:iter, s(:call, s(:lvar, :words), :select), s(:args, :original_word), s(:block, s(:lasgn, :bank, s(:array)), s(:iter, s(:call, s(:call, s(:lvar, :original_word), :downcase), :each_char), s(:args, :c), s(:call, s(:lvar, :bank), :<<, s(:lvar, :c))), s(:call, s(:lvar, :bank), :sort!), s(:lasgn, :to_compare, s(:call, nil, :stringer, s(:lvar, :bank))), s(:call, s(:lvar, :unique_word), :==, s(:lvar, :to_compare)))))), s(:call, s(:lvar, :to_return), :to_s)))