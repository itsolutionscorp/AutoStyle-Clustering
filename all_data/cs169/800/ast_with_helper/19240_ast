s(:block, s(:defn, :combine_anagrams, s(:args, :words), s(:lasgn, :combined_list, s(:array)), s(:iter, s(:call, s(:lvar, :words), :each), s(:args, :word), s(:block, s(:lasgn, :sorted, s(:call, s(:call, s(:call, s(:lvar, :word), :chars), :sort_by, s(:block_pass, s(:lit, :downcase))), :join)), s(:lasgn, :inserted, s(:false)), s(:iter, s(:call, s(:lvar, :combined_list), :each), s(:args, :anagram), s(:block, s(:lasgn, :compare1, s(:call, s(:lvar, :sorted), :downcase)), s(:lasgn, :compare2, s(:call, s(:call, s(:call, s(:call, s(:call, s(:lvar, :anagram), :[], s(:lit, 0)), :chars), :sort_by, s(:block_pass, s(:lit, :downcase))), :join), :downcase)), s(:call, nil, :puts, s(:call, s(:call, s(:lvar, :compare1), :+, s(:str, " ")), :+, s(:lvar, :compare2))), s(:call, nil, :puts, s(:str, " ")), s(:if, s(:call, s(:lvar, :compare1), :==, s(:lvar, :compare2)), s(:block, s(:call, s(:lvar, :anagram), :<<, s(:lvar, :word)), s(:lasgn, :inserted, s(:true))), nil))), s(:if, s(:call, s(:lvar, :inserted), :==, s(:false)), s(:call, s(:lvar, :combined_list), :<<, s(:array, s(:lvar, :word))), nil))), s(:return, s(:lvar, :combined_list))))