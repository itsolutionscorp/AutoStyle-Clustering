s(:block, s(:defn, :alphabetize, s(:args, s(:lasgn, :words, s(:array))), s(:lasgn, :new_words, s(:array)), s(:iter, s(:call, s(:lvar, :words), :each), s(:args, :word), s(:call, s(:lvar, :new_words), :<<, s(:call, s(:call, s(:call, s(:call, s(:lvar, :word), :downcase), :split, s(:str, "")), :sort), :join))), s(:call, s(:lvar, :new_words), :uniq!), s(:return, s(:lvar, :new_words))), s(:defn, :combine_anagrams, s(:args, s(:lasgn, :words, s(:array))), s(:lasgn, :uniq_alphabetized_words, s(:call, nil, :alphabetize, s(:lvar, :words))), s(:lasgn, :grouped_anagrams, s(:array)), s(:iter, s(:call, s(:lvar, :uniq_alphabetized_words), :each), s(:args, :uaw), s(:block, s(:lasgn, :group, s(:array)), s(:iter, s(:call, s(:lvar, :words), :each), s(:args, :w), s(:block, s(:if, s(:call, s(:call, s(:call, s(:call, s(:call, s(:lvar, :w), :downcase), :split, s(:str, "")), :sort), :join), :==, s(:lvar, :uaw)), s(:call, s(:lvar, :group), :<<, s(:lvar, :w)), nil), s(:call, s(:lvar, :grouped_anagrams), :<<, s(:lvar, :group)))))), s(:return, s(:call, s(:lvar, :grouped_anagrams), :uniq))))