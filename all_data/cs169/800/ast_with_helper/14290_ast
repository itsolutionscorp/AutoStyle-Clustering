s(:block, s(:defn, :isAnagram?, s(:args, :word1, :word2), s(:call, s(:call, s(:call, s(:call, s(:lvar, :word1), :downcase), :split, s(:str, "")), :sort), :==, s(:call, s(:call, s(:call, s(:lvar, :word2), :downcase), :split, s(:str, "")), :sort))), s(:defn, :combine_anagrams, s(:args, :words), s(:lasgn, :array, s(:array)), s(:while, s(:call, s(:call, s(:lvar, :words), :length), :>, s(:lit, 0)), s(:block, s(:lasgn, :i, s(:lit, 0)), s(:lasgn, :temp, s(:array, s(:call, s(:lvar, :words), :[], s(:lvar, :i)))), s(:lasgn, :j, s(:call, s(:lvar, :i), :+, s(:lit, 1))), s(:while, s(:call, s(:lvar, :j), :<, s(:call, s(:lvar, :words), :length)), s(:if, s(:call, nil, :isAnagram?, s(:call, s(:lvar, :words), :[], s(:lvar, :i)), s(:call, s(:lvar, :words), :[], s(:lvar, :j))), s(:block, s(:call, s(:lvar, :temp), :<<, s(:call, s(:lvar, :words), :[], s(:lvar, :j))), s(:call, s(:lvar, :words), :delete_at, s(:lvar, :j))), s(:lasgn, :j, s(:call, s(:lvar, :j), :+, s(:lit, 1)))), true), s(:call, s(:lvar, :array), :<<, s(:lvar, :temp)), s(:call, s(:lvar, :words), :delete_at, s(:lvar, :i))), true), s(:return, s(:lvar, :array))))