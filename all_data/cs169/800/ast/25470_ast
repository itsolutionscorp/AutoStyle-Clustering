s(:defn, :combine_anagrams, s(:args, :words), s(:lasgn, :words2, s(:call, s(:lvar, :words), :dup)), s(:iter, s(:call, s(:lvar, :words2), :map), s(:args, :elt), s(:block, s(:lasgn, :d, s(:call, s(:lvar, :elt), :to_a)), s(:lasgn, :c, s(:call, s(:call, s(:call, s(:call, s(:call, s(:lvar, :elt), :chars), :to_a), :sort), :to_s), :downcase)), s(:iter, s(:call, s(:lvar, :words2), :each), s(:args, :elt2), s(:if, s(:and, s(:call, s(:call, s(:call, s(:call, s(:call, s(:call, s(:lvar, :elt2), :chars), :to_a), :sort), :to_s), :downcase), :==, s(:lvar, :c)), s(:call, s(:lvar, :elt), :!=, s(:lvar, :elt2))), s(:block, s(:call, s(:lvar, :d), :<<, s(:lvar, :elt2)), s(:call, s(:lvar, :words2), :delete, s(:lvar, :elt2))), nil)), s(:lvar, :d))))