s(:defn, :combine_anagrams, s(:args, :words), s(:if, s(:call, s(:lvar, :words), :==, s(:array)), s(:return, s(:array)), nil), s(:lasgn, :final_array, s(:array)), s(:iter, s(:call, s(:lvar, :words), :each), s(:args, :word), s(:block, s(:lasgn, :anagrams, s(:array)), s(:iter, s(:call, s(:lvar, :words), :each), s(:args, :w), s(:if, s(:call, s(:call, s(:call, s(:call, s(:call, s(:lvar, :word), :downcase), :gsub, s(:str, " "), s(:str, "")), :split, s(:str, "")), :sort), :==, s(:call, s(:call, s(:call, s(:call, s(:lvar, :w), :downcase), :gsub, s(:str, " "), s(:str, "")), :split, s(:str, "")), :sort)), s(:call, s(:lvar, :anagrams), :<<, s(:lvar, :w)), nil)), s(:call, s(:lvar, :final_array), :<<, s(:lvar, :anagrams)))), s(:call, s(:lvar, :final_array), :uniq))