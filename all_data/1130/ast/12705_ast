s(:defn, :combine_anagrams, s(:args, :words), s(:lasgn, :poshash, s(:call, s(:const, :Hash), :new)), s(:lasgn, :owords, s(:call, s(:const, :Array), :new)), s(:iter, s(:call, s(:lvar, :words), :each), s(:args, :word), s(:block, s(:lasgn, :tmpword, s(:call, s(:lvar, :word), :downcase)), s(:lasgn, :code, s(:call, s(:call, s(:call, s(:lvar, :tmpword), :chars), :sort), :join)), s(:if, s(:call, s(:call, s(:lvar, :poshash), :has_key?, s(:lvar, :code)), :!), s(:block, s(:attrasgn, s(:lvar, :poshash), :[]=, s(:lvar, :code), s(:call, s(:lvar, :owords), :length)), s(:attrasgn, s(:lvar, :owords), :[]=, s(:call, s(:lvar, :poshash), :[], s(:lvar, :code)), s(:call, s(:const, :Array), :new))), nil), s(:lasgn, :wordpos, s(:call, s(:call, s(:lvar, :owords), :[], s(:call, s(:lvar, :poshash), :[], s(:lvar, :code))), :length)), s(:attrasgn, s(:call, s(:lvar, :owords), :[], s(:call, s(:lvar, :poshash), :[], s(:lvar, :code))), :[]=, s(:lvar, :wordpos), s(:lvar, :word)))), s(:return, s(:lvar, :owords)))