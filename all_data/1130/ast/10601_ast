s(:block, s(:defn, :are_anagrams, s(:args, :s1, :s2), s(:call, s(:call, s(:call, s(:call, s(:call, s(:lvar, :s1), :upcase), :chars), :sort), :join), :==, s(:call, s(:call, s(:call, s(:call, s(:lvar, :s2), :upcase), :chars), :sort), :join))), s(:defn, :combine_anagrams, s(:args, :words_), s(:lasgn, :words, s(:call, s(:lvar, :words_), :dup)), s(:lasgn, :res, s(:array)), s(:for, s(:dot2, s(:lit, 0), s(:call, s(:call, s(:lvar, :words), :length), :-, s(:lit, 1))), s(:lasgn, :i), s(:block, s(:if, s(:call, s(:call, s(:lvar, :words), :[], s(:lvar, :i)), :==, s(:str, "")), s(:next), nil), s(:lasgn, :group, s(:array, s(:call, s(:lvar, :words), :[], s(:lvar, :i)))), s(:for, s(:dot2, s(:call, s(:lvar, :i), :+, s(:lit, 1)), s(:call, s(:call, s(:lvar, :words), :length), :-, s(:lit, 1))), s(:lasgn, :j), s(:if, s(:call, nil, :are_anagrams, s(:call, s(:lvar, :words), :[], s(:lvar, :i)), s(:call, s(:lvar, :words), :[], s(:lvar, :j))), s(:block, s(:call, s(:lvar, :group), :<<, s(:call, s(:lvar, :words), :[], s(:lvar, :j))), s(:attrasgn, s(:lvar, :words), :[]=, s(:lvar, :j), s(:str, ""))), nil)), s(:call, s(:lvar, :res), :<<, s(:lvar, :group)))), s(:lvar, :res)))