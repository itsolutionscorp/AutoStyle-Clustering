s(:defn, :combine_anagrams, s(:args, :words), s(:lasgn, :result, s(:array)), s(:while, s(:call, s(:call, s(:lvar, :words), :length), :!=, s(:lit, 0)), s(:block, s(:lasgn, :word, s(:call, s(:call, s(:call, s(:call, s(:lvar, :words), :[], s(:lit, 0)), :downcase), :chars), :sort)), s(:lasgn, :anagrams, s(:iter, s(:call, s(:lvar, :words), :select), s(:args, :s), s(:call, s(:call, s(:call, s(:call, s(:lvar, :s), :downcase), :chars), :sort), :==, s(:lvar, :word)))), s(:iter, s(:call, s(:lvar, :words), :reject!), s(:args, :s), s(:call, s(:lvar, :anagrams), :include?, s(:lvar, :s))), s(:call, s(:lvar, :result), :<<, s(:lvar, :anagrams))), true), s(:lvar, :result))