s(:defn, :combine_anagrams, s(:args, :words), s(:lasgn, :results, s(:array)), s(:iter, s(:call, s(:lvar, :words), :each), s(:args, :word), s(:block, s(:lasgn, :match, s(:false)), s(:iter, s(:call, s(:lvar, :results), :each), s(:args, :resultset), s(:if, s(:and, s(:call, s(:call, s(:lvar, :resultset), :length), :>, s(:lit, 0)), s(:call, s(:call, s(:call, s(:call, s(:call, s(:call, s(:lvar, :resultset), :[], s(:lit, 0)), :downcase), :chars), :sort), :join, s(:str, "")), :==, s(:call, s(:call, s(:call, s(:call, s(:lvar, :word), :downcase), :chars), :sort), :join, s(:str, "")))), s(:block, s(:lasgn, :match, s(:true)), s(:call, s(:lvar, :resultset), :push, s(:lvar, :word))), nil)), s(:if, s(:call, s(:lvar, :match), :==, s(:false)), s(:block, s(:lasgn, :resultset, s(:call, s(:array), :push, s(:lvar, :word))), s(:call, s(:lvar, :results), :push, s(:lvar, :resultset))), nil))), s(:return, s(:lvar, :results)))