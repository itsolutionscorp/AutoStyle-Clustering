s(:defn, :combine_anagrams, s(:args, :words), s(:if, s(:call, s(:call, s(:lvar, :words), :length), :==, s(:lit, 0)), s(:return, s(:array)), nil), s(:if, s(:call, s(:call, s(:lvar, :words), :length), :==, s(:lit, 1)), s(:return, s(:array, s(:lvar, :words))), nil), s(:lasgn, :x, s(:iter, s(:call, s(:lvar, :words), :partition), s(:args, :w), s(:call, s(:call, s(:call, s(:call, s(:call, s(:call, s(:lvar, :words), :[], s(:lit, 0)), :downcase), :chars), :sort), :join), :==, s(:call, s(:call, s(:call, s(:call, s(:lvar, :w), :downcase), :chars), :sort), :join)))), s(:if, s(:call, s(:call, s(:call, s(:lvar, :x), :[], s(:lit, 1)), :length), :==, s(:lit, 0)), s(:return, s(:array, s(:call, s(:lvar, :x), :[], s(:lit, 0)))), nil), s(:return, s(:call, s(:array, s(:call, s(:lvar, :x), :[], s(:lit, 0))), :+, s(:call, nil, :combine_anagrams, s(:call, s(:lvar, :x), :[], s(:lit, 1))))))