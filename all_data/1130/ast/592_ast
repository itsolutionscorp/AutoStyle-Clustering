s(:defn, :combine_anagrams, s(:args, :words), s(:lasgn, :res, s(:array)), s(:iter, s(:call, s(:lvar, :words), :each_index), s(:args, :ind), s(:if, s(:call, s(:call, s(:call, s(:lvar, :res), :flatten), :include?, s(:call, s(:lvar, :words), :[], s(:lvar, :ind))), :!), s(:block, s(:lasgn, :part, s(:array)), s(:iter, s(:call, s(:call, s(:lvar, :words), :[], s(:dot2, s(:lvar, :ind), s(:call, s(:lvar, :words), :length))), :each), s(:args, :word), s(:if, s(:call, s(:call, s(:call, s(:lvar, :res), :flatten), :include?, s(:lvar, :word)), :!), s(:if, s(:call, s(:call, s(:call, s(:call, s(:call, s(:lvar, :words), :[], s(:lvar, :ind)), :downcase), :split, s(:lit, //)), :sort), :==, s(:call, s(:call, s(:call, s(:lvar, :word), :downcase), :split, s(:lit, //)), :sort)), s(:call, s(:lvar, :part), :<<, s(:lvar, :word)), nil), nil)), s(:attrasgn, s(:lvar, :res), :[]=, s(:call, s(:lvar, :res), :length), s(:lvar, :part))), nil)), s(:return, s(:lvar, :res)))