s(:defn, :combine_anagrams, s(:args, :words), s(:lasgn, :anagram_groups, s(:array)), s(:lasgn, :anagram_group_nums, s(:hash)), s(:lasgn, :i, s(:lit, 0)), s(:iter, s(:call, s(:lvar, :words), :each), s(:args, :x), s(:block, s(:lasgn, :char_array, s(:array)), s(:lasgn, :b, s(:lit, 0)), s(:iter, s(:call, s(:lvar, :x), :each_char), s(:args, :c), s(:block, s(:attrasgn, s(:lvar, :char_array), :[]=, s(:lvar, :b), s(:call, s(:lvar, :c), :downcase)), s(:lasgn, :b, s(:call, s(:lvar, :b), :+, s(:lit, 1))))), s(:lasgn, :x_key, s(:call, s(:lvar, :char_array), :sort)), s(:if, s(:call, s(:lvar, :anagram_group_nums), :has_key?, s(:lvar, :x_key)), s(:call, s(:call, s(:lvar, :anagram_groups), :[], s(:call, s(:lvar, :anagram_group_nums), :[], s(:lvar, :x_key))), :<<, s(:lvar, :x)), s(:block, s(:attrasgn, s(:lvar, :anagram_group_nums), :[]=, s(:lvar, :x_key), s(:lvar, :i)), s(:lasgn, :i, s(:call, s(:lvar, :i), :+, s(:lit, 1))), s(:call, s(:lvar, :anagram_groups), :<<, s(:array, s(:lvar, :x))))))), s(:return, s(:lvar, :anagram_groups)))