s(:defn, :combine_anagrams, s(:args, :words), s(:iasgn, :@anagram, s(:nil)), s(:iter, s(:call, s(:lvar, :words), :each), s(:args, :w), s(:if, s(:call, s(:call, s(:lvar, :w), :length), :>, s(:lit, 0)), s(:block, s(:iasgn, :@sorted_string, s(:call, s(:call, s(:call, s(:call, s(:lvar, :w), :downcase), :chars), :sort), :join)), s(:if, s(:call, s(:ivar, :@anagram), :==, s(:nil)), s(:iasgn, :@anagram, s(:hash, s(:ivar, :@sorted_string), s(:array, s(:lvar, :w)))), s(:if, s(:call, s(:ivar, :@anagram), :has_key?, s(:ivar, :@sorted_string)), s(:call, s(:call, s(:ivar, :@anagram), :[], s(:ivar, :@sorted_string)), :<<, s(:lvar, :w)), s(:attrasgn, s(:ivar, :@anagram), :[]=, s(:ivar, :@sorted_string), s(:array, s(:lvar, :w)))))), nil)), s(:iasgn, :@array, s(:call, s(:const, :Array), :new)), s(:iter, s(:call, s(:ivar, :@anagram), :each_key), s(:args, :key), s(:call, s(:ivar, :@array), :<<, s(:call, s(:ivar, :@anagram), :[], s(:lvar, :key)))), s(:return, s(:ivar, :@array)))