s(:defn, :combine_anagrams, s(:args, :words), s(:lasgn, :res, s(:array)), s(:iter, s(:call, s(:lvar, :words), :each), s(:args, :word), s(:if, s(:call, s(:iter, s(:call, s(:lvar, :res), :any?), s(:args, :anagrams), s(:if, s(:call, s(:call, s(:call, s(:call, s(:lvar, :anagrams), :[], s(:lit, 0)), :split, s(:lit, //)), :sort), :==, s(:call, s(:call, s(:lvar, :word), :split, s(:lit, //)), :sort)), s(:block, s(:call, s(:lvar, :anagrams), :insert, s(:call, s(:lvar, :anagrams), :length), s(:lvar, :word)), s(:true)), s(:false))), :!), s(:call, s(:lvar, :res), :insert, s(:call, s(:lvar, :res), :length), s(:array, s(:lvar, :word))), nil)), s(:lvar, :res))