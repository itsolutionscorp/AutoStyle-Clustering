s(:block, s(:defn, :are_anagrams?, s(:args, :s1, :s2), s(:if, s(:call, s(:call, s(:call, s(:call, s(:call, s(:lvar, :s1), :downcase), :chars), :sort), :join), :==, s(:call, s(:call, s(:call, s(:call, s(:lvar, :s2), :downcase), :chars), :sort), :join)), s(:return, s(:true)), s(:return, s(:false)))), s(:defn, :combine_anagrams, s(:args, :words), s(:lasgn, :sorted, s(:array)), s(:iter, s(:call, s(:lvar, :words), :each), s(:args, :w1), s(:block, s(:lasgn, :group, s(:array, s(:lvar, :w1))), s(:attrasgn, s(:lvar, :words), :[]=, s(:call, s(:lvar, :words), :index, s(:lvar, :w1)), s(:str, "")), s(:if, s(:call, s(:call, s(:lvar, :words), :size), :>, s(:lit, 1)), s(:iter, s(:call, s(:lvar, :words), :each), s(:args, :w2), s(:if, s(:call, nil, :are_anagrams?, s(:lvar, :w1), s(:lvar, :w2)), s(:block, s(:call, s(:lvar, :group), :push, s(:lvar, :w2)), s(:attrasgn, s(:lvar, :words), :[]=, s(:call, s(:lvar, :words), :index, s(:lvar, :w2)), s(:str, ""))), nil)), nil), s(:if, s(:match3, s(:lit, /\w+/), s(:call, s(:lvar, :group), :[], s(:lit, 0))), s(:call, s(:lvar, :sorted), :push, s(:lvar, :group)), nil))), s(:return, s(:lvar, :sorted))))