s(:defn, :combine_anagrams, s(:args, :words), s(:lasgn, :groups, s(:call, s(:const, :Hash), :new)), s(:lasgn, :output, s(:call, s(:const, :Array), :new)), s(:iter, s(:call, s(:lvar, :words), :each), s(:args, :word), s(:block, s(:lasgn, :hash, s(:call, s(:call, s(:call, s(:call, s(:lvar, :word), :downcase), :chars), :sort), :join)), s(:if, s(:call, s(:call, s(:lvar, :groups), :[], s(:lvar, :hash)), :==, s(:nil)), s(:attrasgn, s(:lvar, :groups), :[]=, s(:lvar, :hash), s(:call, s(:const, :Array), :new)), nil), s(:call, s(:call, s(:lvar, :groups), :[], s(:lvar, :hash)), :<<, s(:lvar, :word)))), s(:iter, s(:call, s(:lvar, :groups), :each), s(:args, :key, :value), s(:call, s(:lvar, :output), :<<, s(:lvar, :value))), s(:call, nil, :print, s(:str, "The grouping is: ")), s(:call, nil, :print, s(:lvar, :output)), s(:call, nil, :print, s(:str, "\n")), s(:return, s(:lvar, :output)))