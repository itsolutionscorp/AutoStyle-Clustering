s(:defn, :combine_anagrams, s(:args, :words), s(:lasgn, :hsh, s(:hash)), s(:iter, s(:call, s(:lvar, :words), :each), s(:args, :word), s(:block, s(:lasgn, :key, s(:call, s(:call, s(:call, s(:lvar, :word), :downcase), :scan, s(:lit, /\w/)), :sort)), s(:if, s(:call, s(:lvar, :hsh), :has_key?, s(:lvar, :key)), s(:attrasgn, s(:lvar, :hsh), :[]=, s(:lvar, :key), s(:call, s(:call, s(:lvar, :hsh), :[], s(:lvar, :key)), :<<, s(:lvar, :word))), s(:call, s(:lvar, :hsh), :merge!, s(:hash, s(:lvar, :key), s(:array, s(:lvar, :word))))))), s(:lasgn, :anagrams, s(:array)), s(:iter, s(:call, s(:lvar, :hsh), :each_key), s(:args, :key), s(:call, s(:lvar, :anagrams), :<<, s(:call, s(:lvar, :hsh), :[], s(:lvar, :key)))), s(:return, s(:lvar, :anagrams)))